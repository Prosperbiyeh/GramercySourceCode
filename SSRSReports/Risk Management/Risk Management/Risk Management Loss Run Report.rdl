    <DataSet Name="InsuredList">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportMinBoundDate">
            <Value>=Parameters!ReportMinBoundDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportAsOfDate">
            <Value>=Parameters!ReportAsOfDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select e.InsuredGUID,
       e.PolicyName
from [GramercyRisk].[dbo].tblProducerLocations a,
     [GramercyRisk].[dbo].tblProducerContacts b,
	 [GramercyRisk].[dbo].tblQuotes c,
     [GramercyRisk].[dbo].ivQuotes d,
	 [GramercyRisk].[dbo].tblInsureds e
where c.PolicyNumber is not NULL
  and c.DateBound &gt; @ReportMinBoundDate
  and c.DateBound &lt;= @ReportAsOfDate
  and c.TransactionTypeID is NULL
  and c.QuoteGUID = d.QuoteGUID
  and d.InsuredGUID = e.InsuredGUID
group by e.InsuredGUID,
       e.PolicyName
order by 2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
