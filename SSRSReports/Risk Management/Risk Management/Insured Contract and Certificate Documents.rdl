  <DataSources>
    <DataSource Name="SharedDS">
      <DataSourceReference>/SharedDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e945a3de-1718-4dc0-8366-cf3c8146021e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Folder">
            <Value>=Parameters!Folder.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InsuredName">
            <Value>=Parameters!InsuredName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PolicyEffectiveDate">
            <Value>=Parameters!PolicyEffectiveDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select *
from (
select d.Name InsuredName,
       a.DocumentStoreGUID,
	   e.FolderName,
	   a.Description,
	   a.DateAdded,
	   a.FileName,
	   a.CreatedDate,
	  (select min(convert(date, tq.EffectiveDate))
	   from GramercyRisk.dbo.tblQuotes tq
	   where c.SubmissionGroupGUID = tq.SubmissionGroupGUID) PolicyEffectiveDate
from GramercyRisk.dbo.tblDocumentStore a,
     GramercyRisk.dbo.tblDocumentAssociations b,
	 GramercyRisk.dbo.tblSubmissionGroup c,
	 GramercyRisk.ReportReference.v_InsuredName d,
	 GramercyRisk.dbo.tblDocumentFolders e,	 
	 GramercyRisk.dbo.tblDocumentFolders e1
 where a.DocumentStoreGUID = b.DocumentStoreGUID  
   and b.SubmissionGroupGuid = c.SubmissionGroupGUID  
   and c.InsuredGuid = d.InsuredGUID
   and a.FolderID = e.FolderID
   and e.ParentFolderID = e1.FolderID  /*
   and e1.FolderName = 'Risk Transfer'  
   and e.FolderName in ('Certificates', 'Contracts') */
   and e.FolderID in (@Folder)
) aaa
where aaa.InsuredName in (@InsuredName)
  and aaa.PolicyEffectiveDate in (@PolicyEffectiveDate)
order by aaa.DateAdded desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="InsuredName">
          <DataField>InsuredName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DocumentStoreGUID">
          <DataField>DocumentStoreGUID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="FolderName">
          <DataField>FolderName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateAdded">
          <DataField>DateAdded</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FileName">
          <DataField>FileName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CreatedDate">
          <DataField>CreatedDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PolicyEffectiveDate">
          <DataField>PolicyEffectiveDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Folder">
            <Value>=Parameters!Folder.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select d.Name InsuredName
from GramercyRisk.dbo.tblDocumentStore a,
	 GramercyRisk.dbo.tblDocumentAssociations b,
	 GramercyRisk.dbo.tblSubmissionGroup c,
	 GramercyRisk.ReportReference.v_InsuredName d,
	 GramercyRisk.dbo.tblDocumentFolders e,
	 GramercyRisk.dbo.tblDocumentFolders e1
where a.DocumentStoreGUID = b.DocumentStoreGUID
  and b.SubmissionGroupGuid = c.SubmissionGroupGUID
  and c.InsuredGuid = d.InsuredGUID
  and a.FolderID = e.FolderID
  and e.ParentFolderID = e1.FolderID  /*
   and e1.FolderName = 'Risk Transfer'  
   and e.FolderName in ('Certificates', 'Contracts') */
   and e.FolderID in (@Folder)
group by d.Name
order by 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="InsuredName">
          <DataField>InsuredName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InsuredName">
            <Value>=Parameters!InsuredName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Folder">
            <Value>=Parameters!Folder.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select convert(date, tq.EffectiveDate) PolicyEffectiveDate
from GramercyRisk.dbo.tblDocumentStore a,
	 GramercyRisk.dbo.tblDocumentAssociations b,
	 GramercyRisk.dbo.tblSubmissionGroup c,
	 GramercyRisk.ReportReference.v_InsuredName d,
	 GramercyRisk.dbo.tblDocumentFolders e,
	 GramercyRisk.dbo.tblDocumentFolders e1,
	 GramercyRisk.dbo.tblQuotes tq
where a.DocumentStoreGUID = b.DocumentStoreGUID
  and b.SubmissionGroupGuid = c.SubmissionGroupGUID
  and c.InsuredGuid = d.InsuredGUID
  and d.Name in (@InsuredName)
  and a.FolderID = e.FolderID
  and e.ParentFolderID = e1.FolderID  /*
   and e1.FolderName = 'Risk Transfer'  
   and e.FolderName in ('Certificates', 'Contracts') */
   and e.FolderID in (@Folder)
  and c.SubmissionGroupGUID = tq.SubmissionGroupGuid
group by convert(date, tq.EffectiveDate)
order by 1 desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PolicyEffectiveDate">
          <DataField>PolicyEffectiveDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet4">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select FolderID, FolderName
from [dbo].[tblDocumentFolders] 
where ParentFolderID = (select FolderID
						from [dbo].[tblDocumentFolders]
						where FolderName = 'Risk Transfer')
union all
select FolderID, FolderName
from [dbo].[tblDocumentFolders] 
where FolderID = 170
order by 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FolderID">
          <DataField>FolderID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FolderName">
          <DataField>FolderName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
