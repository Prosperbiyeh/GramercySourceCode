	   sum(aaa.[Indemnity Erosion]) [Indemnity Erosion],
	   sum(aaa.[Indemnity Takedown]) [Indemnity Takedown],
	   sum(aaa.[Indemnity Total Change]) [Indemnity Total Change],
	   sum(aaa.[Expense New]) [Expense New],
	   sum(aaa.[Expense Increase]) [Expense Increase],
	   sum(aaa.[Expense Erosion]) [Expense Erosion],
	   sum(aaa.[Expense Takedown]) [Expense Takedown],
	   sum(aaa.[Expense Total Change]) [Expense Total Change],
	   sum(aaa.Recoveries) Recoveries,
	   sum(aaa.[Ending Balance]) [Ending Balance],
	   IsNull(sum(case when aaa.MonthNum = 1 then aaa.[Beginning Inventory] end), 0) "LOB Beginning Inventory", 
	   IsNull(sum(case when aaa.MonthNum = @MaxMonthNum then aaa.[Closing Inventory] end), 0) "LOB Closing Inventory",
	   sum(case when aaa.MonthNum = 1 then aaa.[Beginning Balance] end) "LOB Beginning Balance",
	   sum(case when aaa.MonthNum = @MaxMonthNum then aaa.[Ending Balance] end) [LOB Ending Balance]
from (
select a.RptYear, 
       a.MonthNum, 
	   a.Month, 
	   a.LOB,
	  (select b.lob_name
	   from [dbo].[fh_lob] b
	   where a.LOB = b.lob_id) LOBName,
	   sum(case when a.CategoryType = 'Beginning Inventory' then a.Count end) [Beginning Inventory], 
	   sum(case when a.CategoryType = 'Opened'				then a.Count end) Opened, 
	   sum(case when a.CategoryType = 'Reopened'			then a.Count end) Reopened, 
	   sum(case when a.CategoryType = 'Closed'				then a.Count end) Closed,
	   sum(case when a.CategoryType = 'Closing Inventory'	then a.Count end) [Closing Inventory],
	   0 [Beginning Balance],
	   0 [Indemity Paid],
	   0 [ALAE Paid],
	   0 [Indemnity New],
	   0 [Indemnity Increase],
	   0 [Indemnity Erosion],
	   0 [Indemnity Takedown],
	   0 [Indemnity Total Change],
	   0 [Expense New],
	   0 [Expense Increase],
	   0 [Expense Erosion],
	   0 [Expense Takedown],
	   0 [Expense Total Change],
	   0 Recoveries,
	   0 [Ending Balance]
from #ClaimArisingInfo a
group by a.RptYear, 
       a.MonthNum, 
	   a.Month,
	   a.LOB

union all

select a.RptYear, 
       a.MonthNum, 
	   a.Month, 
	   a.LOB,
	  (select b.lob_name
	   from [dbo].[fh_lob] b
	   where a.LOB = b.lob_id) LOBName,
	   0 [Beginning Inventory], 
	   0 Opened, 
	   0 Reopened, 
	   0 Closed,
	   0 [Closing Inventory],
	   sum(case when a.CategoryType = 'Beginning Balance'	then a.IndeminityOutstandingReserve + a.ExpenseOutstandingReserve end) [Beginning Balance],
	   0 [Indemnity Paid],
	   0 [ALAE Paid],
	   sum(case when a.CategoryType = 'New Reserve'			then a.IndeminityOutstandingReserve end) [Indemnity New],
	   sum(case when a.CategoryType = 'Reserve Increase'	then a.IndeminityOutstandingReserve end) [Indemnity Increase],
	   sum(case when a.CategoryType = 'Reserve Erosion'		then a.IndeminityOutstandingReserve end) [Indemnity Erosion],
	   sum(case when a.CategoryType = 'Reserve Takedown'	then a.IndeminityOutstandingReserve end) [Indemnity Takedown],
	   sum(case when a.CategoryType not in ('Beginning Balance', 'Ending Balance')
															then a.IndeminityOutstandingReserve end) [Indemnity Total Change],
	   sum(case when a.CategoryType = 'New Reserve'			then a.ExpenseOutstandingReserve end) [Expense New],
	   sum(case when a.CategoryType = 'Reserve Increase'	then a.ExpenseOutstandingReserve end) [Expense Increase],
	   sum(case when a.CategoryType = 'Reserve Erosion'		then a.ExpenseOutstandingReserve end) [Expense Erosion],
	   sum(case when a.CategoryType = 'Reserve Takedown'	then a.ExpenseOutstandingReserve end) [Expense Takedown],
	   sum(case when a.CategoryType not in ('Beginning Balance', 'Ending Balance')
															then a.ExpenseOutstandingReserve end) "Expense Total Change",
       0 Recoveries,
	   sum(case when a.CategoryType = 'Ending Balance'	then a.IndeminityOutstandingReserve + a.ExpenseOutstandingReserve end) [Ending Balance]
from #ClaimReserveData a
group by a.RptYear, 
       a.MonthNum, 
	   a.Month, 
	   a.LOB

union all

select a.RptYear, 
       a.MonthNum, 
	   a.Month, 
	   a.LOB, 
	  (select b.lob_name
	   from [dbo].[fh_lob] b
	   where a.LOB = b.lob_id) LOBName,
	   0 [Beginning Inventory], 
	   0 Opened, 
	   0 Reopened, 
	   0 Closed,
	   0 [Closing Inventory],
	   0 [Beginning Balance],
	   sum(case when a.CategoryType = 'Indemnity Payments'	then a.Amount end) [Indemnity Paid],
	   sum(case when a.CategoryType = 'Expense Payments'	then a.Amount end) [Alae Paid],
	   0 [Indemnity New],
	   0 [Indemnity Increase],
	   0 [Indemnity Erosion],
	   0 [Indemnity Takedown],
	   0 [Indemnity Total Change],
	   0 [Expense New],
	   0 [Expense Increase],
	   0 [Expense Erosion],
	   0 [Expense Takedown],
	   0 [Expense Total Change],
	   sum(case when a.CategoryType = 'Recoveries'			then a.Amount end)  Recoveries,
	   0 [Ending Balance]
from #ClaimPaymentData a
group by a.RptYear, 
       a.MonthNum, 
	   a.Month, 
	   a.LOB
) aaa
group by aaa.RptYear, 
       aaa.MonthNum, 
	   aaa.Month, 
	   aaa.LOB, 
	   aaa.LOBName

order by 1, 2

drop table #ClaimArisingInfo
drop table #ClaimReserveData
drop table #ClaimPaymentData

drop table #Treaties
drop table #TreatyLineShare

end</CommandText>
