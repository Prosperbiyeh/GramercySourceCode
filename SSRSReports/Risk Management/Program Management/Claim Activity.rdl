<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SharedLocalDS">
      <DataSourceReference>/SharedLocalDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>4da0c916-216b-44e7-8ed6-c0f012b40c2f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RptYear">
            <Value>=Parameters!RptYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RptEndMonth">
            <Value>=Parameters!RptEndMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AccidentYear">
            <Value>=Parameters!AccidentYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CauseOfLossID">
            <Value>=Parameters!CauseOfLossID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GICOnly">
            <Value>=Parameters!GICOnly.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Carrier">
            <Value>=Parameters!Carrier.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LOB">
            <Value>=Parameters!LOB.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @RptYear			char(4);
declare @RptEndMonth		int;
declare	@LOB				varchar(2000);
declare @AccidentYear		char(4);
declare @CauseOfLossID		varchar(max);
declare @GICOnly			int;
declare @Carrier			varchar(100);

*/

-- Hard-coded params
declare @IndemnityExpense	char(1);
declare @RptBeginDate		date;
declare @RptEndDate			date;
declare @AsOfDate			date;
declare @MaxMonthNum		int

begin

/*

set @RptYear			= '2020'
set @RptEndMonth		= 6
set @LOB				= 'AP'
set @AccidentYear		= '2019'
set @CauseOfLossID		= '0'
set @GICOnly			= 0
set @Carrier			= 'Tri-State Consumer Insurance Company'

*/

-- Hard-coded params
set @IndemnityExpense	= 'B'
set @RptBeginDate		= convert(date, '01/01/' + convert(char(4), (@RptYear/* - 1*/)))
set @RptEndDate			= dateadd(dd, -1, dateadd(mm, 1, convert(date, '01/01/' + convert(char(4), (@RptYear/* - 1*/)))))
-- @AsOfDate set below 


create table #ClaimArisingInfo (RptYear char(4), 
								MonthNum int, 
								Month varchar(15), 
								CategoryOrder int, 
								CategoryType varchar(50), 
								LOB	varchar(10),
								Count int)
create nonclustered index idx_CAI on #ClaimArisingInfo (RptYear, MonthNum)

create table #ClaimReserveData (RptYear char(4), 
								MonthNum int, 
								Month varchar(15), 
								CategoryOrder int, 
								CategoryType varchar(50),  
								LOB	varchar(10),
								IndeminityOutstandingReserve Money, 
								ExpenseOutstandingReserve Money)
create nonclustered index idx_CRD on #ClaimReserveData (RptYear, MonthNum)

create table #ClaimPaymentData (RptYear char(4), 
								MonthNum int, 
								Month varchar(15), 
								CategoryOrder int, 
								CategoryType varchar(50),  
								LOB	varchar(10),
								Amount Money)
create nonclustered index idx_CPD on #ClaimPaymentData (RptYear, MonthNum)


create table #Treaties		   (CompanyReinsTreatyID int, 
								CompanyID int, 
								TreatyID int, 
								TreatyName varchar(100), 
								TreatyBeginDate date, 
								TreatyEndDate date, 
								CommissionBasis varchar(100),
								CommissionPct decimal(6, 5))


create table #TreatyLineShare  (CompanyReinsTreatyID int, 
								LineID int, 
								TreatySharePct decimal(6, 5), 
								AssumedCommissionPct decimal(6, 5))


insert into #Treaties
select  CompanyReinsTreatyID, 
		CompanyID, 
		TreatyID, 
		TreatyName, 
		TreatyBeginDate, 
		TreatyEndDate, 
		CommissionBasis,
		CommissionPct
from [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreaties]

insert into #TreatyLineShare
select  CompanyReinsTreatyID, 
		LineID, 
		TreatySharePct, 
		AssumedCommissionPct
from [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreatyLineShare]


while @RptEndDate &lt; dateadd(m, 1, convert(date, convert(varchar, @RptEndMonth) + '/01/' + convert(char(4), @RptYear)))

	begin



--  Claim Arisings


		insert into #ClaimArisingInfo
		select aaaaaa.RptYear,
			   aaaaaa.MonthNum,
			   aaaaaa.Month,
			   aaaaaa.CategoryOrder,
			   aaaaaa.CategoryType,
			   aaaaaa.lob_id LOB,
			   count(case when datepart(yyyy, aaaaaa.loss_date) in (@AccidentYear)
			              then aaaaaa.claim_id
						  end) Count
		from (


		-- Month 1

		select 1 CategoryOrder, datepart(yyyy, @RptBeginDate) RptYear, 
			   'Beginning Inventory' CategoryType,
			   datepart(mm, @RptBeginDate) MonthNum,
			   datename(Month, @RptBeginDate) Month,
			   aaaaa.*
		from ( 
		select c.claim_id,
			   e.party_id,
			   b.policy_num,
			   g.company_name,
			   ltrim(case when e.first_name is NULL 
					then e.last_name
					else e.first_name + ' ' + e.last_name end) ClaimantName,
			   c.fh_claim_num,
			   c.loss_date,
			   c.reported_date,
			   f.history_action_type,
			   f.history_datetime,
			   c.handler_id,
			   c.handler_id2,
			   d.first_name,
			   d.last_name, a.lob_id,	   
			   case when f.open_close_status = 'R' then IsNull(c.claim_status_id, 'ST0CNP') else c.claim_status_id end claim_status_id,
			   case when f.open_close_status = 'O' then 'Open'
					when f.open_close_status = 'C' then 'Closed'
					when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
			   c.closure_method_id,
			   f.closed_date
		from [dbo].[fh_claim] a,
			 [dbo].[fh_policy] b,
			 [dbo].[fh_claim_history] c,
			 [dbo].[fh_user] d,
			 [dbo].[fh_party] e,
			 [dbo].[fh_party_history] f,
			 [dbo].[fh_company] g
		where a.policy_id = b.policy_id
		  and b.tier1_company_id = 'C13339'
		  and a.claim_id = c.claim_id
		  and IsNull(c.handler_id, a.handler_id) = d.user_id
		  and a.claim_id = e.claim_id
		  and e.party_id = f.party_id
		  and c.history_datetime = (select max(cc.history_datetime)	
									from [dbo].[fh_claim_history] cc
									where c.claim_id = cc.claim_id
									  and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
									  and cc.history_action_type = case when cc.open_close_status = 'O'
																		then 'I'
																		else cc.history_action_type end
									  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
		  and f.history_action_type = case when f.open_close_status = 'O'
										   then 'I'
										   else f.history_action_type end
		  and b.tier_company_id = g.company_id
		  and (a.loss_causation_id in (@CauseOfLossID)
		   or  IsNull(a.loss_causation_id, '0') = case when @CauseOfLossID = '0' then IsNull(a.loss_causation_id, '0') end)
		  and (b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISTILL' 
										   else b.primary_carrier_id end
           or  b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISWEXS' 
										   else b.primary_carrier_id end)
          and b.primary_carrier_id in (select primary_carrier_id		
									   from [dbo].[ClaimCarrierNames] cn
									   where cn.carrier_name in (@Carrier))
		--  and a.claim_id = '001020000224447'
		--  and a.fh_claim_num = '209114'

		) aaaaa
		where aaaaa.history_datetime &lt; @RptBeginDate
		  and aaaaa.open_close_status_name in ('Open', 'Reopened')
		  and not exists (select 1
						  from [dbo].[fh_party_history] ccccc
						  where aaaaa.claim_id = ccccc.claim_id
							and aaaaa.party_id = ccccc.party_id
							and ccccc.open_close_status = 'C'
							and ccccc.history_datetime &gt; aaaaa.history_datetime
							and ccccc.history_datetime &lt; @RptBeginDate
							and ccccc.closed_date &lt; @RptBeginDate)
		  and aaaaa.history_datetime = (select max(ccccc.history_datetime)
										from [dbo].[fh_party_history] ccccc
										where aaaaa.claim_id = ccccc.claim_id
										  and aaaaa.party_id = ccccc.party_id
										  and ccccc.history_action_type = case when ccccc.open_close_status = 'O'
																			   then 'I'
																			   else ccccc.history_action_type end
										  and ccccc.history_datetime &lt; @RptBeginDate)

		union all

		select 2 CategoryOrder, datepart(yyyy, @RptBeginDate) RptYear, 
			   'Opened' CategoryType,
			   datepart(mm, @RptBeginDate) MonthNum,
			   datename(Month, @RptBeginDate) Month,
			   aaaaa.*
		from (
		select c.claim_id,
			   e.party_id,
			   b.policy_num,
			   g.company_name,
			   ltrim(case when e.first_name is NULL 
					then e.last_name
					else e.first_name + ' ' + e.last_name end) ClaimantName,
			   c.fh_claim_num,
			   c.loss_date,
			   c.reported_date,
			   f.history_action_type,
			   f.history_datetime,
			   c.handler_id,
			   c.handler_id2,
			   d.first_name,
			   d.last_name, a.lob_id,	 
			   c.claim_status_id,
			   case when f.open_close_status = 'O' then 'Open'
					when f.open_close_status = 'C' then 'Closed'
					when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,
       
			   c.closure_method_id,
			   f.closed_date
		from [dbo].[fh_claim] a,
			 [dbo].[fh_policy] b,
			 [dbo].[fh_claim_history] c,
			 [dbo].[fh_user] d,
			 [dbo].[fh_party] e,
			 [dbo].[fh_party_history] f,
			 [dbo].[fh_company] g
		where a.policy_id = b.policy_id
		  and b.tier1_company_id = 'C13339'
		  and a.claim_id = c.claim_id
		  and IsNull(c.handler_id, a.handler_id) = d.user_id
		  and a.claim_id = e.claim_id
		  and e.party_id = f.party_id
		  and f.history_action_type = 'I'
		  and c.history_datetime = (select max(cc.history_datetime)	
									from [dbo].[fh_claim_history] cc
									where c.claim_id = cc.claim_id
									  and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
									  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
		  and b.tier_company_id = g.company_id
		--  and c.claim_status_id is not NULL
		  and IsNull(c.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
		  and (a.loss_causation_id in (@CauseOfLossID)
		   or  IsNull(a.loss_causation_id, '0') = case when @CauseOfLossID = '0' then IsNull(a.loss_causation_id, '0') end)
		  and (b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISTILL' 
										   else b.primary_carrier_id end
           or  b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISWEXS' 
										   else b.primary_carrier_id end)
          and b.primary_carrier_id in (select primary_carrier_id		
									   from [dbo].[ClaimCarrierNames] cn
									   where cn.carrier_name in (@Carrier))
		--  and a.claim_id = '000250000220591' 
		--  and a.fh_claim_num = '209114'

		) aaaaa
		where aaaaa.history_datetime &gt;= @RptBeginDate
		  and aaaaa.history_datetime &lt; dateadd(d, 1, @RptEndDate)
		  and aaaaa.open_close_status_name in ('Open')

		union all

		select 3 CategoryOrder, datepart(yyyy, @RptBeginDate) RptYear, 
			   'Reopened' CategoryType,
			   datepart(mm, @RptBeginDate) MonthNum,
			   datename(Month, @RptBeginDate) Month,
			   aaaaa.*
		from (
		select c.claim_id,
			   e.party_id,
			   b.policy_num,
			   g.company_name,
			   ltrim(case when e.first_name is NULL 
					then e.last_name
					else e.first_name + ' ' + e.last_name end) ClaimantName,
			   c.fh_claim_num,
			   c.loss_date,
			   c.reported_date,
			   f.history_action_type,
			   f.history_datetime,
			   c.handler_id,
			   c.handler_id2,
			   d.first_name,
			   d.last_name, a.lob_id,	 
			   IsNull(c.claim_status_id, 'ST0CNP') claim_status_id,
			   case when f.open_close_status = 'O' then 'Open'
					when f.open_close_status = 'C' then 'Closed'
					when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
			   c.closure_method_id,
			   f.closed_date
		from [dbo].[fh_claim] a,
			 [dbo].[fh_policy] b,
			 [dbo].[fh_claim_history] c,
			 [dbo].[fh_user] d,
			 [dbo].[fh_party] e,
			 [dbo].[fh_party_history] f,
			 [dbo].[fh_company] g
		where a.policy_id = b.policy_id
		  and b.tier1_company_id = 'C13339'
		  and a.claim_id = c.claim_id
		  and IsNull(c.handler_id, a.handler_id) = d.user_id
		  and a.claim_id = e.claim_id
		  and e.party_id = f.party_id
		  and f.history_action_type = case when f.open_close_status = 'O'
										   then 'I'
										   else f.history_action_type end
		  and f.history_datetime = (select min(ff.history_datetime)
											from [dbo].[fh_party_history] ff
											where ff.claim_id = f.claim_id
											  and ff.party_id = f.party_id
											  and ff.open_close_status = f.open_close_status
											  and ff.closed_date = f.closed_date)
		  and c.history_datetime = (select max(cc.history_datetime)	
									from [dbo].[fh_claim_history] cc
									where c.claim_id = cc.claim_id
									  and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
									  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
		  and b.tier_company_id = g.company_id
		--  and c.claim_status_id is not NULL
		  and IsNull(c.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
		  and (a.loss_causation_id in (@CauseOfLossID)
		   or  IsNull(a.loss_causation_id, '0') = case when @CauseOfLossID = '0' then IsNull(a.loss_causation_id, '0') end)
		  and (b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISTILL' 
										   else b.primary_carrier_id end
           or  b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISWEXS' 
										   else b.primary_carrier_id end)
          and b.primary_carrier_id in (select primary_carrier_id		
									   from [dbo].[ClaimCarrierNames] cn
									   where cn.carrier_name in (@Carrier))
--  MH  1/20/2022  - Don't count reopens right after another reopen
		  and not exists (select 1
					  from [dbo].[fh_party_history] ff
					  where ff.claim_id = f.claim_id
					    and ff.party_id = f.party_id
					    and ff.history_datetime = (select max(fff.history_datetime)
													from [dbo].[fh_party_history] fff
													where fff.claim_id = ff.claim_id
													and fff.party_id = ff.party_id
													and fff.history_datetime &lt; f.history_datetime)
					    and ff.open_close_status = 'R')
		--  and a.claim_id = '001020000199427' 
		--  and a.fh_claim_num = '209114'

		) aaaaa
		where aaaaa.history_datetime &gt;= @RptBeginDate 
		  and aaaaa.history_datetime &lt; dateadd(d, 1, @RptEndDate)
		  and aaaaa.open_close_status_name in ('Reopened')

		union all

		select 4 CategoryOrder, datepart(yyyy, @RptBeginDate) RptYear, 
			   'Closed' CategoryType,
			   datepart(mm, @RptBeginDate) MonthNum,
			   datename(Month, @RptBeginDate) Month,
			   aaaaa.*
		from (
		select c.claim_id,
			   e.party_id,
			   b.policy_num,
			   g.company_name,
			   ltrim(case when e.first_name is NULL 
					then e.last_name
					else e.first_name + ' ' + e.last_name end) ClaimantName,
			   c.fh_claim_num,
			   c.loss_date,
			   c.reported_date,
			   f.history_action_type,
			   f.history_datetime,
			   c.handler_id,
			   c.handler_id2,
			   d.first_name,
			   d.last_name, a.lob_id,	 
			   IsNull(c.claim_status_id, 'ST0CNP') claim_status_id,
			   case when f.open_close_status = 'O' then 'Open'
					when f.open_close_status = 'C' then 'Closed'
					when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
			   c.closure_method_id,
			   f.closed_date
		from [dbo].[fh_claim] a,
			 [dbo].[fh_policy] b,
			 [dbo].[fh_claim_history] c,
			 [dbo].[fh_user] d,
			 [dbo].[fh_party] e,
			 [dbo].[fh_party_history] f,
			 [dbo].[fh_company] g
		where a.policy_id = b.policy_id
		  and b.tier1_company_id = 'C13339'
		  and a.claim_id = c.claim_id
		  and IsNull(c.handler_id, a.handler_id) = d.user_id
		  and a.claim_id = e.claim_id
		  and e.party_id = f.party_id
		  and f.history_action_type = case when f.open_close_status = 'O'
										   then 'I'
										   else f.history_action_type end
		 and c.history_datetime = (select max(cc.history_datetime)	
									from [dbo].[fh_claim_history] cc
									where c.claim_id = cc.claim_id
									  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
		  and b.tier_company_id = g.company_id
		--  and IsNull(c.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
		  and f.history_datetime = IsNull((select max(ff.history_datetime)
											from [dbo].[fh_party_history] ff
											where ff.claim_id = f.claim_id
											  and ff.party_id = f.party_id
											  and ff.open_close_status = 'C'
											  and ff.open_close_status = f.open_close_status
											  and ff.closed_date = f.closed_date
											  and ff.history_datetime &gt;= @RptBeginDate 
											  and ff.history_datetime &lt; dateadd(d, 1, @RptEndDate)), f.history_datetime)
		  and (a.loss_causation_id in (@CauseOfLossID)
		   or  IsNull(a.loss_causation_id, '0') = case when @CauseOfLossID = '0' then IsNull(a.loss_causation_id, '0') end)
		  and (b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISTILL' 
										   else b.primary_carrier_id end
           or  b.primary_carrier_id = case when @GICOnly = 1
		    							   then 'ISWEXS' 
										   else b.primary_carrier_id end)
          and b.primary_carrier_id in (select primary_carrier_id		
									   from [dbo].[ClaimCarrierNames] cn
									   where cn.carrier_name in (@Carrier))
--  MH  1/20/2022  - Don't count closes right after another close
		  and not exists (select 1
					  from [dbo].[fh_party_history] ff
					  where ff.claim_id = f.claim_id
					    and ff.party_id = f.party_id
					    and ff.history_datetime = (select max(fff.history_datetime)
													from [dbo].[fh_party_history] fff
													where fff.claim_id = ff.claim_id
													and fff.party_id = ff.party_id
													and fff.history_datetime &lt; f.history_datetime)
					    and ff.open_close_status = 'C')
		--  and e.party_id = '001020000186411' 
		--  and a.fh_claim_num = '209613'

		) aaaaa
		where aaaaa.history_datetime &gt;= @RptBeginDate 
		  and aaaaa.history_datetime &lt; dateadd(d, 1, @RptEndDate)
		  and aaaaa.closed_date &gt;= @RptBeginDate 
		  and aaaaa.closed_date &lt; dateadd(d, 1, @RptEndDate)
		  and aaaaa.open_close_status_name in ('Closed')

		union all

		select 5 CategoryOrder, datepart(yyyy, @RptBeginDate) RptYear, 
			   'Closing Inventory' CategoryType,
			   datepart(mm, @RptBeginDate) MonthNum,
			   datename(Month, @RptBeginDate) Month,
			   aaaaa.*
		from (
		select c.claim_id,
			   e.party_id,
			   b.policy_num,
			   g.company_name,
			   ltrim(case when e.first_name is NULL 
					then e.last_name
					else e.first_name + ' ' + e.last_name end) ClaimantName,
			   c.fh_claim_num,
			   c.loss_date,
			   c.reported_date,
			   f.history_action_type,
			   f.history_datetime,
			   c.handler_id,
			   c.handler_id2,
			   d.first_name,
			   d.last_name, a.lob_id,   
			   case when f.open_close_status = 'R' then IsNull(c.claim_status_id, 'ST0CNP') else c.claim_status_id end claim_status_id,
			   case when f.open_close_status = 'O' then 'Open'
					when f.open_close_status = 'C' then 'Closed'
					when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
			   c.closure_method_id,
			   f.closed_date
		from [dbo].[fh_claim] a,
			 [dbo].[fh_policy] b,
			 [dbo].[fh_claim_history] c,
			 [dbo].[fh_user] d,
			 [dbo].[fh_party] e,
			 [dbo].[fh_party_history] f,
			 [dbo].[fh_company] g
		where a.policy_id = b.policy_id
		  and b.tier1_company_id = 'C13339'
		  and a.claim_id = c.claim_id
		  and IsNull(c.handler_id, a.handler_id) = d.user_id
		  and c.history_action_type = case when c.open_close_status = 'O'
										   then 'I'
										   else c.history_action_type end
		  and a.claim_id = e.claim_id
		  and e.party_id = f.party_id
		  and f.history_action_type = case when f.open_close_status = 'O'
										   then 'I'
										   else f.history_action_type end
		  and c.history_datetime = (select max(cc.history_datetime)	
