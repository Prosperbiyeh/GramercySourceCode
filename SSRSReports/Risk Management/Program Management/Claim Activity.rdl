    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <CommandText>select datepart(yyyy, c.loss_date) RptYear
from fh_policy b,
	 fh_claim c
where b.tier1_company_id = 'C13339'
  and b.policy_id = c.policy_id
group by datepart(yyyy, c.loss_date)
union select datepart(yyyy, GetDate())
order by 1 desc</CommandText>
