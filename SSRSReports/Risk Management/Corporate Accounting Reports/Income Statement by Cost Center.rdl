  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateFrom">
            <Value>=Parameters!DateFrom.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GLCompanyId">
            <Value>=Parameters!GLCompanyId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @GLCompanyId		varchar(10);
--declare @DateFrom			date;
--declare @DateTo				date;

begin 


--set @DateFrom		= '1/1/2020'
--set @DateTo			= '8/31/2020'
--set @GLCompanyId	= '17'

select bbb.GLAccountGroup,
       aaa.*
from (
SELECT DISTINCT
     GL.GLAcctId,
     GL.FullName,
     GL.AcctNum,
     CASE WHEN LGT.AcctClassName = 'Income'
          THEN -ROUND(SUM(CCA.Amount), 2) 
          ELSE  ROUND(SUM(CCA.Amount), 2) 
     END AS [Amount],
     LGT.AcctTypeID, 
     LGT.AcctClassName, 
     LGT.AcctTypeDescription,
     case when EG.GroupId = 2 then 'Overhead' else EG.GroupName end AS [CostCenter],
     EG.GroupId AS [CostCenterId],
	 GL.GlCompanyId,
	 CO.Location
FROM
     tblFin_GLAccounts GL
INNER JOIN 
     tblFin_JournalPostings jp
ON
     JP.GLAcctID = GL.GLAcctID
INNER JOIN 
     tblFin_Journal J
ON
     J.TransactNum = JP.TransactNum
INNER JOIN
     tblFin_GlAccountTypes GLT
ON
     GLT.GlAcctId = GL.GLAcctID
INNER JOIN
     tblFin_GlFinancialAcctTypes GLFT 
ON
     GLFT.GlAcctId = GL.GlAcctId
INNER JOIN
     lstGLAcctTypes LGT 
ON
     LGT.AcctTypeId = GLFT.AcctTypeId
INNER JOIN
     tblFin_CostCenterAllocation CCA
ON
     CCA.PostingNum = JP.PostingNum
INNER JOIN
     tblEntityGroups EG
ON
     EG.GroupId = CCA.CostCenterId AND EG.Inactive = 0
INNER JOIN
	 tblClientOffices CO
ON CO.OfficeID = GL.GlCompanyId
WHERE
     (DATEDIFF(d, @DateFrom, J.PostDate) &gt;= 0 AND DATEDIFF(d, @DateTo, J.PostDate) &lt;= 0)
AND
     GLT.AccountType IN ('CX', 'RCE', 'I', 'E', 'RCI', 'CI', 'MCI', 'FCI')
AND
     GL.GLCompanyId in (@GLCompanyId)
AND
     J.TransDescID &lt;&gt; 'Y'
AND 
     J.TransactNum NOT IN (SELECT VoidedBy FROM tblFin_Journal WHERE TransDescId = 'Y' AND VoidedBy IS NOT NULL)
     
GROUP BY
     GL.FullName,
     GL.AcctNum,
     LGT.AcctTypeID, 
     LGT.AcctClassName, 
     LGT.AcctTypeDescription,
     EG.GroupName,
     GLT.AccountType,
     EG.GroupId,
     GL.GLAcctId,
	 GL.GlCompanyId,
	 CO.Location
) aaa
  left join ReportReference.GLAccountGroupMap bbb
    on aaa.AcctNum = bbb.GLAccountNumber

end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
