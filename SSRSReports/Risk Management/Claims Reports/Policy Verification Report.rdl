    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DateofLoss">
            <Value>=Parameters!DateofLoss.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InsuredID">
            <Value>=Parameters!InsuredID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @InsuredID		int
declare @DateofLoss		datetime



set @InsuredID		= 3921
set @DateofLoss = '9/24/2022'
--set @DateofLoss = '9/23/2021'

*/

select a.InsuredID,
       c.PolicyNumber,
	   a.Name InsuredName,       
	   convert(date, c.EffectiveDate) EffectiveDate,
	   case when c.TransactionTypeID = 'C'
	        then convert(date, dateadd(yyyy, 1, c.EffectiveDate))
			else convert(date, c.ExpirationDate) end ExpirationDate,
	   case when c.TransactionTypeID = 'C'
			then convert(date, c.ExpirationDate) end CancellationDate,
	   h.LineName,
	   convert(date, IsNull(c.EndorsementEffective, c.EffectiveDate)) EndorsementEffective,
	   c.DateIssued,
	   i.LocationName Carrier,
	   c.ProducerLocationName,
	   c.QuoteID,
	   c.NetRate_QuoteID,
	   case when h.LineName = 'Commercial General Liability' then	       
	   (select convert(int, replace(qgl.EachOccurrence, ',', ''))
		from [dbo].[NetRate_Quote_Insur_Quote_Liabi] qgl
		where c.NetRate_QuoteID = qgl.QuoteID
		  and qgl.NodeName = 'LiabilityLimits') 
		    when h.LineName = 'Commercial Auto' then	       
	   (select convert(int, replace(qb.Liability, ',', ''))
		from [dbo].[NetRate_Quote_Insur_Quote_Busin] qb
		where c.NetRate_QuoteID = qb.QuoteID
		  and qb.NodeName = 'BusinessAutoPolicyLimits') 
		    when h.LineName = 'Commercial Property' then
	   (select IsNull(sum(convert(int, replace(locp.BuildingLimit, ',', ''))), 0) +
	           IsNull(sum(convert(int, replace(locp.PropertyExposure, ',', ''))), 0) +
	           IsNull(sum(convert(int, replace(locp.BusinessIncomeExposure, ',', ''))), 0)
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Premi] locp
		where c.NetRate_QuoteID = loc.QuoteID
		  and loc.LocationID = locp.LocationID)
		    when h.LineName = 'Commercial Inland Marine' then
	   (select sum(IsNull(convert(int, replace(replace(locig.MiscItemsLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.InstallBasicLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.OwnedLeasedEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.RentalReimLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.RentedEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.ToolsLimit, '$', ''), ',', '')), 0) )
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan] loci,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan_Grame] locig
	    where c.NetRate_QuoteID = loc.QuoteID
		  and loc.LocationID = loci.LocationID
		  and loci.InlandMarineID = locig.InlandMarineID)
		    when h.LineName = 'Commercial Crime' then
	   (select sum(Limit)
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Crime] locc,
			 [dbo].[gramercyCrimeLimit_vw] gcl
	    where c.NetRate_QuoteID = loc.QuoteID
		  and loc.LocationID = locc.LocationID
		  and locc.CrimeID = gcl.CrimeID)
		    when h.LineName = 'Umbrella' then
	   (select sum(convert(int, replace(iql.ExcessLimit, ',', '')))
		from [dbo].[NetRate_Quote_Insur_Quote_Liabi] iql
		where c.NetRate_QuoteID = iql.QuoteID)
		  end Limit,
        case when convert(date, IsNull(c.EndorsementEffective, c.EffectiveDate)) = (select max(convert(date, IsNull(cc.EndorsementEffective, cc.EffectiveDate)))
																		from tblQuotes cc,
																			 tblFin_Invoices ee,
																			 tblFin_InvoiceDetails ff
																		where c.PolicyNumber = cc.PolicyNumber
																		  and ee.QuoteID = cc.QuoteID
																		  and ee.Failed = 0
																		  and ee.InvoiceNum = ff.InvoiceNum
																		  and f.CompanyLineGuid = ff.CompanyLineGuid
																		  and convert(date, IsNull(cc.EndorsementEffective, cc.EffectiveDate)) &lt;= @DateofLoss
																		  and convert(date, cc.ExpirationDate) &gt;= @DateofLoss)

			   and c.DateIssued = (select max(cc.DateIssued)
									from tblQuotes cc,
										 tblFin_Invoices ee,
										 tblFin_InvoiceDetails ff
									where c.PolicyNumber = cc.PolicyNumber
										and ee.QuoteID = cc.QuoteID
										and ee.Failed = 0
										and ee.InvoiceNum = ff.InvoiceNum
										and f.CompanyLineGuid = ff.CompanyLineGuid
										and convert(date, IsNull(cc.EndorsementEffective, cc.EffectiveDate)) &lt;= @DateofLoss
										and convert(date, cc.ExpirationDate) &gt;= @DateofLoss)	
              then 'X' end Selected
from tblInsureds a,
     tblSubmissionGroup b,
	 tblQuotes c,
	 tblFin_Invoices e,
	 tblFin_InvoiceDetails f,
	 tblCompanyLines g,
	 lstLines h,
	 tblCompanyLocations i
where a.InsuredID = @InsuredID
  and a.InsuredGUID = b.InsuredGuid
  and b.SubmissionGroupGUID = c.SubmissionGroupGuid
--  and c.PolicyNumber is not NULL
  and c.DateIssued is not NULL

  																	     

  and e.QuoteID = c.QuoteID
  and e.Failed = 0
  and e.InvoiceNum = f.InvoiceNum
  and f.CompanyLineGuid = g.CompanyLineGuid
  and g.LineGUID = h.LineGUID
  and g.CompanyLocationGUID = i.CompanyLocationGUID
--  and c.PolicyNumber = 'MPGR2684-02'
group by a.InsuredID,
       c.PolicyNumber,
	   a.Name,       
	   convert(date, c.EffectiveDate),
	   case when c.TransactionTypeID = 'C'
	        then convert(date, dateadd(yyyy, 1, c.EffectiveDate))
			else convert(date, c.ExpirationDate) end,
	   case when c.TransactionTypeID = 'C'
			then convert(date, c.ExpirationDate) end,
	   h.LineName,
	   IsNull(c.EndorsementEffective, c.EffectiveDate),
	   c.DateIssued,
	   i.LocationName,
	   c.ProducerLocationName,
	   c.QuoteID,
	   c.NetRate_QuoteID,
	   f.CompanyLineGuid
--order by Selected desc, 3, 2, 7, 8</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
