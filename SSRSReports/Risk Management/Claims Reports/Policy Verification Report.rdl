      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select a.InsuredID,
       a.Name
from tblInsureds a,
	 tblSubmissionGroup b,
     tblQuotes c
where a.InsuredGUID = b.InsuredGuid
  and b.SubmissionGroupGUID = c.SubmissionGroupGuid
  and c.DateIssued is not NULL
group by a.InsuredID,
       a.Name
order by a.Name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
