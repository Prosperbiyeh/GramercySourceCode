    <DataSource Name="SharedDS">
      <DataSourceReference>/SharedDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>41e29005-9bae-4941-92d1-54e050219e56</rd:DataSourceID>
    </DataSource>
    <DataSource Name="SharedLocalDS">
      <DataSourceReference>/SharedLocalDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>9c7e4d43-6dd3-44b0-927d-2b4982a9e80f</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportAsOfDate">
            <Value>=Parameters!ReportAsOfDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportEndDate">
            <Value>=Parameters!ReportEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyReinTreatyID">
            <Value>=Parameters!CompanyReinTreatyID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportBeginDate">
            <Value>=Parameters!ReportBeginDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IncludeUmbrellaClaims">
            <Value>=Parameters!IncludeUmbrellaClaims.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
declare @ReportAsOfDate			date;
declare @ReportEndDate			date;
declare @ReportBeginDate		date;
declare @IncludeUmbrellaClaims	char(1);
declare @CompanyReinTreatyID	int;
*/


begin

/*

set @ReportBeginDate = '7/1/2017'
set @ReportAsOfDate = dateadd(d, 10, GetDate())
set @ReportEndDate = dateadd(d, 10, GetDate())
set @IncludeUmbrellaClaims = 1
set @CompanyReinTreatyID = 1

*/

create table #tblQuotes	(QuoteID				int, 
						 PolicyNumber			varchar(20), 
						 EndorsementEffective	date, 
						 EffectiveDate			date, 
						 CompanyLineGuid		uniqueidentifier,
						 LineGUID				uniqueidentifier,
						 RiskDescription		varchar(1000),
						 InsuredPolicyName		varchar(500))

create nonclustered index idx_tblQuotes1 on #tblQuotes (QuoteID, PolicyNumber)

insert into #tblQuotes
select QuoteID,
       PolicyNumber,
	   EndorsementEffective,
	   EffectiveDate,
	   CompanyLineGuid,
	   LineGUID,
	   RiskDescription,
	   InsuredPolicyName
from [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblQuotes]



/*  Outer Query to check for balance  all claims should balance to 0
select aaaaa.ClaimNumber,
       IndeminityOutstandingReserve + IndemnityPaid - IndemnityRecovery - IndemnityIncurredLoss
from (

*/



select aaa.InsuredName,
	  (select max(RiskDescription)
	   from #tblQuotes tq
	   where aaa.InsuredName = tq.InsuredPolicyName
	     and tq.QuoteID = (select max(tq2.QuoteID)
						   from #tblQuotes tq2
						   where tq2.InsuredPolicyName = tq.InsuredPolicyName)) ContractorType,
--       max(aaa.RiskDescription) ContractorType,
	   aaa.accident_state,
	   aaa.ClaimNumber,
	   aaa.ClaimantName,
	   aaa.DateOfLoss,
	   aaa.DateReceived,
	   aaa.DateEntered,
	   aaa.ClaimStatus,
	   aaa.DateClosed,
	   aaa.in_ligitation,
	   ltrim(rtrim(case when aaa.CauseOfLoss is NULL
	        then aaa.accident_description
			else aaa.CauseOfLoss end)) CauseOfLoss,
	   aaa.LOB,
	   aaa.Coverage,
	   aaa.Adjuster,
	   aaa.CompanyName,
	   aaa.PolicyNumber,
	   aaa.PolicyEffectiveDate,
	   datepart(yyyy, aaa.PolicyEffectiveDate) PolicyYear,
	   datepart(yyyy, aaa.DateOfLoss) AccidentYear,
--	   aaa.accident_city,
--	   aaa.accident_zip,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'I' 
					   then aaa.Amount * -1 
					   when aaa.MainTransType in ('Reserve', 'Recovery') and aaa.IndExp = 'I'
					   then aaa.Amount
					   end), 0) IndeminityOutstandingReserve,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'E'
					   then aaa.Amount * -1 
					   when aaa.MainTransType in ('Reserve', 'Recovery') and aaa.IndExp = 'E'
					   then aaa.Amount
					   end), 0) ExpenseOutstandingReserve,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'I' and aaa.transaction_indicator not in ('R', 'I') 
/*  MH  6/14/2022 - This is a Paid Reimbursement that should be included as a Paid Loss
	                                                        and aaa.reserve_paid_id &lt;&gt; '000250000263171' */
					   then aaa.Amount end), 0) IndemnityPaid,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'E' and aaa.transaction_indicator not in ('R', 'I')
					   then aaa.Amount end), 0) ExpensePaid,
	   IsNull(sum(case when aaa.MainTransType = 'Recovery' and aaa.IndExp = 'I'
					   then aaa.Amount * -1 end), 0) IndemnityRecovery,
	   IsNull(sum(case when aaa.MainTransType = 'Recovery' and aaa.IndExp = 'E'
					   then aaa.Amount * -1 end), 0) ExpenseRecovery,
	   IsNull(sum(case when aaa.IndExp = 'I' and aaa.MainTransType in ('Reserve', 'Recovery')
	                   then aaa.Amount end), 0) IndemnityIncurredLoss,
	   IsNull(sum(case when aaa.IndExp = 'E' and aaa.MainTransType in ('Reserve', 'Recovery')
	                   then aaa.Amount end), 0) ExpenseIncurredLoss
from (
select e.InsuredPolicyName InsuredName,
--       a.file_claim_id,
       e.RiskDescription,
	   g.item_name CauseOfLoss,
	   b.accident_description,
	   co.CompanyName,
	   e.PolicyNumber,
	   convert(date, e.EffectiveDate) PolicyEffectiveDate,
	   f.LineCode PolicyType,
	   lob.lob_name LOB,
	   j.item_name Coverage,
	   h.reserve_type_name, 
	   a.fh_claim_num ClaimNumber,
	   b.party_id,
	   loc.accident_city,
	   loc.accident_state,
	   loc.accident_zip,
      (select u.first_name + ' ' + u.last_name
		from [dbo].[fh_claim_history] k,
			 [dbo].[fh_user] u
		where k.claim_id = a.claim_id
	      and k.history_datetime = (select max(kk.history_datetime)
									from [dbo].[fh_claim_history] kk
									where k.claim_id = kk.claim_id
										and kk.history_datetime &lt; dateadd(d, 1, @ReportAsOfDate))
		  and k.handler_id = u.user_id) Adjuster,
	   ltrim(case when b.first_name is NULL 
			then b.last_name
			else b.first_name + ' ' + b.last_name end) ClaimantName,
       a.loss_date DateOfLoss,
	   a.received_date DateReceived,
	   a.date_entered DateEntered,
	   (select cs.open_close_date
	    from [dbo].[fv_party_status] cs
		where b.party_id = cs.party_id
		  and cs.open_close_date = (select max(cs2.open_close_date)
									from [dbo].[fv_party_status] cs2
									where cs.party_id = cs2.party_id
									  and cs2.open_close_status_name = 'Closed'
									  and cs2.open_close_date &lt; dateadd(d, 1, @ReportAsOfDate))) DateClosed,
	   (select cs.open_close_status_name
	    from [dbo].[fv_party_status] cs
		where b.party_id = cs.party_id
		  and cs.open_close_date = (select max(cs2.open_close_date)
									from [dbo].[fv_party_status] cs2
									where cs.party_id = cs2.party_id
									  and cs2.open_close_date &lt; dateadd(d, 1, @ReportAsOfDate)))
	   ClaimStatus,
	   c.reserve_date,
/*  MH  6/14/2022 - To back out Paid Reimbursements from Paid Losses instead of being recoveries */
	   case when c.recovery_type = 4
	        then 'P'
/*  MH  9/26/2022 - To include this particular transaction with transaction_indicator = 'I' as a Paid Loss */
			when c.reserve_paid_id = '001020000264612'
			then 'P'
			else c.transaction_indicator end transaction_indicator,
--	   c.transaction_indicator,
	   case when c.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when c.recovery_type is not NULL or c.reserve_description like '%Recovery%'
			then 'Recovery'
			when c.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when c.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when c.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where c.recovery_type = rt.recovery_type_code)
			when c.reserve_or_paid_code = 'R'
	        then c.reserve_description
			end TransType,
	   case when i.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,  	   
	  (select ph.in_litigation_yn
	   from [dbo].[fh_party_history] ph
	   where b.party_id = ph.party_id
	     and ph.history_datetime = (select max(ph2.history_datetime)
									 from [dbo].[fh_party_history] ph2
									 where ph.party_id = ph2.party_id
									   and ph2.history_datetime &lt; dateadd(d, 1, @ReportAsOfDate))) in_ligitation,
--	   c.recovery_type,
       c.loss_amt Amount,
	   c.reserve_paid_id 
from [dbo].[fh_claim] a
     left join [dbo].[fh_code_client] g
	   on a.loss_causation_id = g.item_id,
	 [dbo].[fh_claim_accident] loc,
     [dbo].[fh_party] b
	 left join [dbo].[fh_reserve_paid] c
	   on b.party_id = c.party_id
      and c.reserve_date &lt;= dateadd(d, 1, @ReportEndDate)
	 left join [dbo].[fh_reserve_type] rt
	   on rt.reserve_type_id = c.reserve_type_id
	 left join [dbo].[fh_reserve_type] h
       on c.reserve_type_id = h.reserve_type_id
	 left join [dbo].[fh_code_general] i
       on i.item_id = h.reserve_group_id /* 
	 left join [dbo].[fh_claim_history] k
       on k.claim_id = c.claim_id
	  and k.history_datetime = (select max(kk.history_datetime)
								from [dbo].[fh_claim_history] kk
								where k.claim_id = kk.claim_id
									and kk.history_datetime &lt; dateadd(d, 1, @ReportAsOfDate)) */
	 left join [dbo].[fh_policy_coverage] pc
	   on b.policy_coverage_id = pc.policy_coverage_id
	 left join [dbo].[fh_code_general] j
       on j.item_id = pc.coverage_id
	 left join [dbo].[fh_lob] lob
       on pc.lob_id = lob.lob_id,
	 [dbo].[fh_policy] d,
     #tblQuotes e,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[lstLines] f,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblCompanyLines] cl,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblCompanyLocations] cloc,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblCompanies] co,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreaties] ct,
	 [dbo].[fh_party_history] ph
where /*d.tier1_company_id = 'C13339'
  and */a.claim_id = b.claim_id
  and b.claim_id = loc.claim_id
  and a.policy_id = d.policy_id
  and replace(d.policy_num, '-', '') = replace(e.PolicyNumber, '-', '')
--  and convert(date, d.policy_begin_date) = convert(date, e.EffectiveDate)
  and e.QuoteID =  IsNull((select max(ee.QuoteID)
					from #tblQuotes ee
					where ee.PolicyNumber = e.PolicyNumber
					  and IsNull(ee.EndorsementEffective, ee.EffectiveDate) = (select max(IsNull(eee.EndorsementEffective, eee.EffectiveDate))
																				 from #tblQuotes eee
																				 where eee.PolicyNumber = ee.PolicyNumber
																				   and IsNull(eee.EndorsementEffective, eee.EffectiveDate) &lt; dateadd(d, 1, a.loss_date))),
                           (select max(ee.QuoteID)
							from #tblQuotes ee
							where ee.PolicyNumber = e.PolicyNumber))
  and e.LineGUID = f.LineGUID
  and e.CompanyLineGuid = cl.CompanyLineGUID
  and cl.CompanyLocationGUID = cloc.CompanyLocationGUID
  and cloc.CompanyGUID = co.CompanyGUID
  and co.CompanyID = ct.CompanyID
  and convert(date, e.EffectiveDate) between ct.TreatyBeginDate and ct.TreatyEndDate 
  and ph.claim_id = b.claim_id
  and ph.party_id = b.party_id
  and ph.history_datetime = (select max(ph2.history_datetime)
							from [dbo].[fh_party_history] ph2
							where ph.claim_id = ph2.claim_id
							  and ph.party_id = ph2.party_id
							  and ph2.history_datetime &lt; dateadd(d, 1, @ReportAsOfDate))
  and CompanyReinsTreatyID in (@CompanyReinTreatyID)
--  and a.fh_claim_num in (210586)
--  and abs(reserve_paid_amt) = 4750.00


) aaa 
where (aaa.DateClosed between @ReportBeginDate and dateadd(d, 1, @ReportEndDate)
   or aaa.ClaimStatus in ('Open', 'Reopened'))
/*  MH  5/10/2022 - Added DateEntered to report and changed report criteria to use that date
                  - instead of DateReceived, which may nor coincide with the date a claim is opened
  and aaa.DateReceived &lt; dateadd(d, 1, @ReportAsOfDate)
  and aaa.DateReceived &lt; dateadd(d, 1, @ReportEndDate) */
  and aaa.DateEntered &lt; dateadd(d, 1, @ReportAsOfDate)
  and aaa.DateEntered &lt; dateadd(d, 1, @ReportEndDate)
  and aaa.LOB &lt;&gt; case when @IncludeUmbrellaClaims = 1
					  then '##'
					  else 'Excess Liability' end
					  
group by aaa.InsuredName,
	   ltrim(rtrim(case when aaa.CauseOfLoss is NULL
	        then aaa.accident_description
			else aaa.CauseOfLoss end)),
	   aaa.PolicyNumber,
	   aaa.PolicyEffectiveDate,
	   aaa.LOB,
	   aaa.Coverage,
	   aaa.adjuster,
	   aaa.CompanyName,
	   aaa.ClaimNumber,
	   aaa.ClaimantName,
	   aaa.DateOfLoss,
	   aaa.DateReceived,
	   aaa.DateEntered,
	   aaa.DateClosed,
	   aaa.ClaimStatus,
	   aaa.accident_city,
	   aaa.accident_state,
	   aaa.accident_zip,
	   aaa.in_ligitation

--) aaaaa


drop table #tblQuotes


end

/*   OLD

/*
declare @ReportAsOfDate			date;
declare @ReportEndDate			date;
declare @ReportBeginDate		date;
declare @IncludeUmbrellaClaims	char(1);
declare @CompanyReinTreatyID	int;

*/

begin


/*
set @ReportBeginDate = '7/1/2017'
set @ReportAsOfDate = '4/1/2021'
set @ReportEndDate = '4/1/2021'
set @IncludeUmbrellaClaims = 1
set @CompanyReinTreatyID = 1

*/

create table #tblQuotes	(QuoteID				int, 
						 PolicyNumber			varchar(20), 
						 EndorsementEffective	date, 
						 EffectiveDate			date, 
						 CompanyLineGuid		uniqueidentifier,
						 LineGUID				uniqueidentifier,
						 RiskDescription		varchar(1000),
						 InsuredPolicyName		varchar(500))

create nonclustered index idx_tblQuotes1 on #tblQuotes (QuoteID, PolicyNumber)

insert into #tblQuotes
select QuoteID,
       PolicyNumber,
	   EndorsementEffective,
	   EffectiveDate,
	   CompanyLineGuid,
	   LineGUID,
	   RiskDescription,
	   InsuredPolicyName
from [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblQuotes]



/*  Outer Query to check for balance  all claims should balance to 0
select aaaaa.ClaimNumber,
       IndeminityOutstandingReserve + IndemnityPaid - IndemnityRecovery - IndemnityIncurredLoss
from (

*/

select aaa.InsuredName,
       max(aaa.RiskDescription) ContractorType,
	   aaa.accident_state,
	   aaa.ClaimNumber,
	   aaa.ClaimantName,
	   aaa.DateOfLoss,
	   aaa.DateReceived,
	   aaa.ClaimStatus,
	   aaa.DateClosed,
	   aaa.in_ligitation,
	   ltrim(rtrim(case when aaa.CauseOfLoss is NULL
	        then aaa.accident_description
			else aaa.CauseOfLoss end)) CauseOfLoss,
	   aaa.LOB,
	   aaa.Coverage,
	   aaa.Adjuster,
	   aaa.CompanyName,
	   aaa.PolicyNumber,
	   aaa.PolicyEffectiveDate,
	   datepart(yyyy, aaa.PolicyEffectiveDate) PolicyYear,
	   datepart(yyyy, aaa.DateOfLoss) AccidentYear,
--	   aaa.accident_city,
--	   aaa.accident_zip,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'I' 
					   then aaa.Amount * -1 
					   when aaa.MainTransType in ('Reserve', 'Recovery') and aaa.IndExp = 'I'
					   then aaa.Amount
					   end), 0) IndeminityOutstandingReserve,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'E'
					   then aaa.Amount * -1 
					   when aaa.MainTransType in ('Reserve', 'Recovery') and aaa.IndExp = 'E'
					   then aaa.Amount
					   end), 0) ExpenseOutstandingReserve,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'I' and aaa.transaction_indicator not in ('R') 
	                                                        and aaa.reserve_paid_id &lt;&gt; '000250000263171'
					   then aaa.Amount end), 0) IndemnityPaid,
	   IsNull(sum(case when aaa.MainTransType = 'Paid Loss' and aaa.IndExp = 'E' and aaa.transaction_indicator not in ('R')
					   then aaa.Amount end), 0) ExpensePaid,
	   IsNull(sum(case when aaa.MainTransType = 'Recovery' and aaa.IndExp = 'I'
					   then aaa.Amount * -1 end), 0) IndemnityRecovery,
	   IsNull(sum(case when aaa.MainTransType = 'Recovery' and aaa.IndExp = 'E'
					   then aaa.Amount * -1 end), 0) ExpenseRecovery,
	   IsNull(sum(case when aaa.IndExp = 'I' and aaa.MainTransType in ('Reserve', 'Recovery')
	                   then aaa.Amount end), 0) IndemnityIncurredLoss,
	   IsNull(sum(case when aaa.IndExp = 'E' and aaa.MainTransType in ('Reserve', 'Recovery')
	                   then aaa.Amount end), 0) ExpenseIncurredLoss
from ( 
select e.InsuredPolicyName InsuredName,
--       a.file_claim_id,
       e.RiskDescription,
	   g.item_name CauseOfLoss,
	   b.accident_description,
	   co.CompanyName,
	   e.PolicyNumber,
	   convert(date, e.EffectiveDate) PolicyEffectiveDate,
	   f.LineCode PolicyType,
	   lob.lob_name LOB,
	   j.item_name Coverage,
	   h.reserve_type_name, 
	   a.fh_claim_num ClaimNumber,
	   b.party_id,
	   loc.accident_city,
	   loc.accident_state,
	   loc.accident_zip,
      (select u.first_name + ' ' + u.last_name
		from [dbo].[fh_user] u
		where k.handler_id = u.user_id) Adjuster,
	   ltrim(case when b.first_name is NULL 
			then b.last_name
			else b.first_name + ' ' + b.last_name end) ClaimantName,
       a.loss_date DateOfLoss,
	   a.received_date DateReceived,	   
	   (select cs.open_close_date
	    from [dbo].[fv_party_status] cs
		where b.party_id = cs.party_id
		  and cs.open_close_date = (select max(cs2.open_close_date)
									from [dbo].[fv_party_status] cs2
									where cs.party_id = cs2.party_id
									  and cs2.open_close_status_name = 'Closed'
									  and cs2.open_close_date &lt; dateadd(d, 1, @ReportAsOfDate))) DateClosed,
	   (select cs.open_close_status_name
	    from [dbo].[fv_party_status] cs
		where b.party_id = cs.party_id
		  and cs.open_close_date = (select max(cs2.open_close_date)
									from [dbo].[fv_party_status] cs2
									where cs.party_id = cs2.party_id
									  and cs2.open_close_date &lt; dateadd(d, 1, @ReportAsOfDate)))
	   ClaimStatus,
	   c.reserve_date,
	   c.transaction_indicator,
	   case when c.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when c.recovery_type is not NULL or c.reserve_description like '%Recovery%'
			then 'Recovery'
			when c.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when c.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when c.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where c.recovery_type = rt.recovery_type_code)
			when c.reserve_or_paid_code = 'R'
	        then c.reserve_description
			end TransType,
	   case when i.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,  	   
	  (select ph.in_litigation_yn
	   from [dbo].[fh_party_history] ph
	   where b.party_id = ph.party_id
	     and ph.history_datetime = (select max(ph2.history_datetime)
									 from [dbo].[fh_party_history] ph2
									 where ph.party_id = ph2.party_id
									   and ph2.history_datetime &lt; dateadd(d, 1, @ReportAsOfDate))) in_ligitation,
--	   c.recovery_type,
       c.loss_amt Amount,
	   c.reserve_paid_id
from [dbo].[fh_claim] a
     left join [dbo].[fh_code_client] g
	   on a.loss_causation_id = g.item_id,
	 [dbo].[fh_claim_accident] loc,
     [dbo].[fh_party] b
	 left join [dbo].[fh_reserve_paid] c
	   on b.party_id = c.party_id
	 left join [dbo].[fh_policy_coverage] pc
	   on b.policy_coverage_id = pc.policy_coverage_id
	 left join [dbo].[fh_code_general] j
       on j.item_id = pc.coverage_id
	 left join [dbo].[fh_lob] lob
       on pc.lob_id = lob.lob_id,
	 [dbo].[fh_policy] d,
     #tblQuotes e,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[lstLines] f,
	 [dbo].[fh_reserve_type] h,
	 [dbo].[fh_code_general] i,	 
	 [dbo].[fh_claim_history] k,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblCompanyLines] cl,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblCompanyLocations] cloc,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[dbo].[tblCompanies] co,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreaties] ct,
	 [dbo].[fh_reserve_type] rt
where d.tier1_company_id = 'C13339'
  and a.claim_id = b.claim_id
  and b.claim_id = loc.claim_id
  and c.reserve_date &lt;= dateadd(d, 1, @ReportEndDate)
  and a.policy_id = d.policy_id
  and replace(d.policy_num, '-', '') = replace(e.PolicyNumber, '-', '')
--  and convert(date, d.policy_begin_date) = convert(date, e.EffectiveDate)
  and e.QuoteID =  IsNull((select max(ee.QuoteID)
					from #tblQuotes ee
					where ee.PolicyNumber = e.PolicyNumber
					  and IsNull(ee.EndorsementEffective, ee.EffectiveDate) = (select max(IsNull(eee.EndorsementEffective, eee.EffectiveDate))
																				 from #tblQuotes eee
																				 where eee.PolicyNumber = ee.PolicyNumber
																				   and IsNull(eee.EndorsementEffective, eee.EffectiveDate) &lt; dateadd(d, 1, a.loss_date))),
                           (select max(ee.QuoteID)
							from #tblQuotes ee
							where ee.PolicyNumber = e.PolicyNumber))
  and e.LineGUID = f.LineGUID
  and c.reserve_type_id = h.reserve_type_id
  and i.item_id = h.reserve_group_id 
  and k.claim_id = a.claim_id
  and k.history_datetime = (select max(kk.history_datetime)
							from [dbo].[fh_claim_history] kk
							where k.claim_id = kk.claim_id
								and kk.history_datetime &lt; dateadd(d, 1, @ReportAsOfDate))
  and e.CompanyLineGuid = cl.CompanyLineGUID
  and cl.CompanyLocationGUID = cloc.CompanyLocationGUID
  and cloc.CompanyGUID = co.CompanyGUID
  and co.CompanyID = ct.CompanyID
  and convert(date, e.EffectiveDate) between ct.TreatyBeginDate and ct.TreatyEndDate 
  and rt.reserve_type_id = c.reserve_type_id
  and CompanyReinsTreatyID in (@CompanyReinTreatyID)
--  and a.fh_claim_num in ('209383')

) aaa 
where (aaa.DateClosed between @ReportBeginDate and dateadd(d, 1, @ReportEndDate)
   or aaa.ClaimStatus in ('Open', 'Reopened'))
  and aaa.LOB &lt;&gt; case when @IncludeUmbrellaClaims = 1
					  then '##'
					  else 'Excess Liability' end
					  
group by aaa.InsuredName,
	   ltrim(rtrim(case when aaa.CauseOfLoss is NULL
	        then aaa.accident_description
			else aaa.CauseOfLoss end)),
	   aaa.PolicyNumber,
	   aaa.PolicyEffectiveDate,
	   aaa.LOB,
	   aaa.Coverage,
	   aaa.adjuster,
	   aaa.CompanyName,
	   aaa.ClaimNumber,
	   aaa.ClaimantName,
	   aaa.DateOfLoss,
	   aaa.DateReceived,
	   aaa.DateClosed,
	   aaa.ClaimStatus,
	   aaa.accident_city,
	   aaa.accident_state,
	   aaa.accident_zip,
	   aaa.in_ligitation

--) aaaaa


drop table #tblQuotes


end

*/</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
