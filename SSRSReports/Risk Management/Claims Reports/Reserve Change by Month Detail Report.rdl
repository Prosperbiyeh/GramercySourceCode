  and ((aaaaa.TransType in ('Close Reserve', 'Other Recovery'))
   or (aaaaa.TransType = 'Change Reserve'
  and aaaaa.Amount &lt; 0))
  and aaaaa.IndExp = case when @IndemnityExpense = 'B'
