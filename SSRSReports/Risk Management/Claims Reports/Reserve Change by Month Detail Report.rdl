  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RptMonth">
            <Value>=Parameters!RptMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RptYear">
            <Value>=Parameters!RptYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AsOfDate">
            <Value>=Parameters!AsOfDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IndemnityExpense">
            <Value>=Parameters!IndemnityExpense.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CategoryType">
            <Value>=Parameters!CategoryType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LOB">
            <Value>=Parameters!LOB.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--declare @RptYear			char(4);
--declare @RptMonth			int;
--declare	@LOB				varchar(2000);
--declare @IndemnityExpense	char(1);
--declare @CategoryType		varchar(50);
--declare @AsOfDate			date;

declare @RptBeginDate		date;
declare @RptEndDate			date;

begin


--set @RptYear			= '2020'
--set @RptMonth			= 1
--set @CategoryType		= 'Reserve Takedown'
--set @LOB				= 'AP'
--set @IndemnityExpense	= 'B'
--set @AsOfDate			= '11/23/2020'

set @RptBeginDate		= convert(date, convert(varchar(2), @RptMonth) + '/01/' + @RptYear)
set @RptEndDate			= dateadd(d, -1, dateadd(m, 1,  convert(date, convert(varchar(2), @RptMonth) + '/01/' + @RptYear)))



select *
from (
select 1 CategoryOrder, @RptYear RptYear, 
       'Beginning Balance' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
	   datename(Month, @RptBeginDate) Month, aaaaa.lob_id,
	   IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) Amount
from (
select a.claim_id,
       a.fh_claim_num claim_num,
	   a.lob_id,
	   a.loss_date,
	   a.reported_date,
	   e.reserve_date,
	   f.reserve_type_name,
	   e.transaction_indicator,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when e.recovery_type is not NULL or e.reserve_description like '%Recovery%'
			then 'Recovery'
			when e.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when e.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where e.recovery_type = rt.recovery_type_code)
			when e.reserve_or_paid_code = 'R'
	        then e.reserve_description
			end TransType,
	   case when cg.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,
	   e.loss_amt Amount
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
	 [dbo].[fh_reserve_paid] e,
	 [dbo].[fh_reserve_type] f, 
	 [dbo].[fh_policy_coverage] pc,
	 [dbo].[fh_reserve_type] rt,
	 [dbo].[fh_code_general] cg,
	 [dbo].[fh_code_general] cg2
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = e.claim_id
  and e.reserve_type_id = f.reserve_type_id
  and a.policy_id = pc.policy_id
  and a.lob_id = pc.lob_id and pc.coverage_id = (select max(pc1.coverage_id) from [dbo].[fh_policy_coverage] pc1 where pc.policy_id = pc1.policy_id and pc.lob_id = pc1.lob_id)
  and e.reserve_type_id = rt.reserve_type_id
  and cg.item_id = rt.reserve_group_id
  and cg2.item_id = pc.coverage_id
) aaaaa
where aaaaa.reserve_date &lt; @AsOfDate and aaaaa.reserve_date &lt; @RptBeginDate
  and aaaaa.IndExp = case when @IndemnityExpense = 'B'
						  then aaaaa.IndExp
						  else @IndemnityExpense end
group by aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.lob_id,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator
having IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) &lt;&gt; 0.00

union all

select 2 CategoryOrder, @RptYear RptYear, 
       'New Reserve' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
	   datename(Month, @RptBeginDate) Month, aaaaa.lob_id,
	   IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) Amount
from (
select a.claim_id,
       a.fh_claim_num claim_num,
	   a.lob_id,
	   a.loss_date,
	   a.reported_date,
	   f.reserve_type_name,
	   e.reserve_date,
	   e.transaction_indicator,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when e.recovery_type is not NULL or e.reserve_description like '%Recovery%'
			then 'Recovery'
			when e.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when e.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where e.recovery_type = rt.recovery_type_code)
			when e.reserve_or_paid_code = 'R'
	        then e.reserve_description
			end TransType,
	   case when cg.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,
	   e.loss_amt Amount
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
	 [dbo].[fh_reserve_paid] e,
	 [dbo].[fh_reserve_type] f, 
	 [dbo].[fh_policy_coverage] pc,
	 [dbo].[fh_reserve_type] rt,
	 [dbo].[fh_code_general] cg,
	 [dbo].[fh_code_general] cg2
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = e.claim_id
  and e.reserve_type_id = f.reserve_type_id
  and a.policy_id = pc.policy_id
  and a.lob_id = pc.lob_id and pc.coverage_id = (select max(pc1.coverage_id) from [dbo].[fh_policy_coverage] pc1 where pc.policy_id = pc1.policy_id and pc.lob_id = pc1.lob_id)
  and e.reserve_type_id = rt.reserve_type_id
  and cg.item_id = rt.reserve_group_id
  and cg2.item_id = pc.coverage_id
) aaaaa
where aaaaa.reserve_date &gt;= @RptBeginDate
  and aaaaa.reserve_date &lt; dateadd(d, 1, @AsOfDate) and aaaaa.reserve_date &lt; dateadd(d, 1, @RptEndDate)
  and aaaaa.TransType = 'New Reserve'
  and aaaaa.IndExp = case when @IndemnityExpense = 'B'
						  then aaaaa.IndExp
						  else @IndemnityExpense end
group by aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.lob_id,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator
having IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) &lt;&gt; 0.00

union all

select 3 CategoryOrder, @RptYear RptYear, 
       'Reserve Increase' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
	   datename(Month, @RptBeginDate) Month, aaaaa.lob_id,
	   IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) Amount
from (
select a.claim_id,
       a.fh_claim_num claim_num,
	   a.lob_id,
	   a.loss_date,
	   a.reported_date,
	   e.reserve_date,
	   f.reserve_type_name,
	   e.transaction_indicator,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when e.recovery_type is not NULL or e.reserve_description like '%Recovery%'
			then 'Recovery'
			when e.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when e.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where e.recovery_type = rt.recovery_type_code)
			when e.reserve_or_paid_code = 'R'
	        then e.reserve_description
			end TransType,
	   case when cg.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,
	   e.loss_amt Amount
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
	 [dbo].[fh_reserve_paid] e,
	 [dbo].[fh_reserve_type] f, 
	 [dbo].[fh_policy_coverage] pc,
	 [dbo].[fh_reserve_type] rt,
	 [dbo].[fh_code_general] cg,
	 [dbo].[fh_code_general] cg2
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = e.claim_id
  and e.reserve_type_id = f.reserve_type_id
  and a.policy_id = pc.policy_id
  and a.lob_id = pc.lob_id and pc.coverage_id = (select max(pc1.coverage_id) from [dbo].[fh_policy_coverage] pc1 where pc.policy_id = pc1.policy_id and pc.lob_id = pc1.lob_id)
  and e.reserve_type_id = rt.reserve_type_id
  and cg.item_id = rt.reserve_group_id
  and cg2.item_id = pc.coverage_id
) aaaaa
where aaaaa.reserve_date &gt;= @RptBeginDate
  and aaaaa.reserve_date &lt; dateadd(d, 1, @AsOfDate) and aaaaa.reserve_date &lt; dateadd(d, 1, @RptEndDate)
  and ((aaaaa.TransType = 'Change Reserve'
  and aaaaa.Amount &gt; 0)
   or aaaaa.TransType in ('Reopen Reserve', 'Reallocation'))
  and aaaaa.IndExp = case when @IndemnityExpense = 'B'
						  then aaaaa.IndExp
						  else @IndemnityExpense end
group by aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.lob_id,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator
having IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) &lt;&gt; 0.00



union all

select 4 CategoryOrder, @RptYear RptYear, 
       'Reserve Erosion' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
	   datename(Month, @RptBeginDate) Month, aaaaa.lob_id,
	   IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) Amount
from (
select a.claim_id,
       a.fh_claim_num claim_num,
	   a.lob_id,
	   a.loss_date,
	   a.reported_date,
	   e.reserve_date,
	   f.reserve_type_name,
	   e.transaction_indicator,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when e.recovery_type is not NULL or e.reserve_description like '%Recovery%'
			then 'Recovery'
			when e.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when e.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where e.recovery_type = rt.recovery_type_code)
			when e.reserve_or_paid_code = 'R'
	        then e.reserve_description
			end TransType,
	   case when cg.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,
	   e.loss_amt Amount
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
	 [dbo].[fh_reserve_paid] e,
	 [dbo].[fh_reserve_type] f, 
	 [dbo].[fh_policy_coverage] pc,
	 [dbo].[fh_reserve_type] rt,
	 [dbo].[fh_code_general] cg,
	 [dbo].[fh_code_general] cg2
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = e.claim_id
  and e.reserve_type_id = f.reserve_type_id
  and a.policy_id = pc.policy_id
  and a.lob_id = pc.lob_id and pc.coverage_id = (select max(pc1.coverage_id) from [dbo].[fh_policy_coverage] pc1 where pc.policy_id = pc1.policy_id and pc.lob_id = pc1.lob_id)
  and e.reserve_type_id = rt.reserve_type_id
  and cg.item_id = rt.reserve_group_id
  and cg2.item_id = pc.coverage_id
) aaaaa
where aaaaa.reserve_date &gt;= @RptBeginDate
  and aaaaa.reserve_date &lt; dateadd(d, 1, @AsOfDate) and aaaaa.reserve_date &lt; dateadd(d, 1, @RptEndDate)
  and aaaaa.TransType in ('Paid Loss', 'Paid After Closing')
  and aaaaa.IndExp = case when @IndemnityExpense = 'B'
						  then aaaaa.IndExp
						  else @IndemnityExpense end
group by aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.lob_id,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator
having IsNull(sum(case when aaaaa.MainTransType = 'Paid Loss' 
					   then aaaaa.Amount * -1 
					   when aaaaa.MainTransType in ('Reserve', 'Recovery')
					   then aaaaa.Amount
					   end), 0) &lt;&gt; 0.00



union all

select 5 CategoryOrder, @RptYear RptYear, 
       'Reserve Takedown' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.IndExp,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
