<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SharedDS">
      <DataSourceReference>/SharedDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>cfed413d-1c57-4177-a5c6-7a5f63972a4a</rd:DataSourceID>
    </DataSource>
    <DataSource Name="SharedLocalDS">
      <DataSourceReference>/SharedLocalDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>496f33a3-2df2-476b-95a5-b26abe0c55f5</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RptMonth">
            <Value>=Parameters!RptMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RptYear">
            <Value>=Parameters!RptYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GICOnly">
            <Value>=Parameters!GICOnly.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AccidentYear">
            <Value>=Parameters!AccidentYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CauseOfLossID">
            <Value>=Parameters!CauseOfLossID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CategoryType">
            <Value>=Parameters!CategoryType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LOB">
            <Value>=Parameters!LOB.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @RptYear			char(4);
declare @RptMonth			int;
declare	@LOB				varchar(2000);
declare @CategoryType		varchar(50);
declare @CauseOfLossID		varchar(max);
declare @AccidentYear		varchar(max);
declare @GICOnly			char(1)
declare @IndemnityExpense	char(1);

*/

-- hardcoded params
declare @RptBeginDate		date;
declare @RptEndDate			date;
declare @AsOfDate		date;

begin

/*

set @RptYear			= '2022'
set @RptMonth			= 6
set @CategoryType		= 'Indemnity Payments'
set @LOB				= 'GL'
set @CauseOfLossID		= '0'
set @GICOnly			= 0
set @AccidentYear		= '2020'
set @IndemnityExpense	= 'I'			

*/

-- hardcoded params

/*  MH  2/1/2021 - OLD before new Claim Activity report in Program Management folder

set @RptBeginDate		= convert(date, convert(varchar(2), @RptMonth) + '/01/' + @RptYear)
set @RptEndDate			= dateadd(d, -1, dateadd(m, 1,  convert(date, convert(varchar(2), @RptMonth) + '/01/' + @RptYear)))
set @AsOfDate	

*/

set @RptBeginDate		= convert(date, convert(varchar(2), case when @RptMonth &gt; 12
                                                                 then 1
																 else @RptMonth end) + '/01/' + @RptYear)
set @RptEndDate			= dateadd(d, -1, dateadd(m, 1,  convert(date, convert(varchar(2), case when @RptMonth &gt; 12
																							   then @RptMonth - 12
																							   else @RptMonth end) + '/01/' + @RptYear)))
set @AsOfDate			= @RptEndDate


select *
from (
select 1 CategoryOrder, @RptYear RptYear, 
       'Indemnity Payments' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month, 
	   aaaaa.company_name InsuredName,
	   aaaaa.policy_num,
	   aaaaa.ClaimantName,
	   aaaaa.Adjuster,
	   aaaaa.lob_id,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
	   IsNull(case when aaaaa.MainTransType = 'Paid Loss' and aaaaa.IndExp = 'I'
					   then aaaaa.Amount
					   end, 0) Amount
from (
select a.claim_id,
	   g.company_name,
	   b.policy_num,
	   ltrim(case when p.first_name is NULL 
		          then p.last_name
		          else p.first_name + ' ' + p.last_name end) ClaimantName,
        d.first_name + ' ' + d.last_name Adjuster,
       a.fh_claim_num claim_num,
	   a.lob_id,
	   a.loss_date,
	   a.reported_date,
	   e.reserve_date,
	   f.reserve_type_name,
	   e.transaction_indicator,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when e.recovery_type is not NULL or e.reserve_description like '%Recovery%'
			then 'Recovery'
			when e.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when e.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where e.recovery_type = rt.recovery_type_code)
			when e.reserve_or_paid_code = 'R'
	        then e.reserve_description
			end TransType,
	   case when i.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,
	   e.loss_amt * case when @GICOnly = 1
								then IsNull(crtls.TreatySharePct, 0)
								else 1 end Amount
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
	 [dbo].[fh_reserve_paid] e,
	 [dbo].[fh_reserve_type] f, 
	 [dbo].[fh_policy_coverage] pc,
	 [dbo].[fh_reserve_type] h,
	 [dbo].[fh_code_general] i,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreaties] crt,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreatyLineShare] crtls,
	 [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] p,
	 [dbo].[fh_party_history] ph,
	 [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = e.claim_id
  and e.reserve_or_paid_code = 'P'
/*  MH  6/14/2022 - Fix for Paid Reimbursements
  and e.transaction_indicator &lt;&gt; 'R' */
  and (e.transaction_indicator &lt;&gt; 'R' or IsNull(e.recovery_type, '') = 4)
  and e.reserve_type_id = f.reserve_type_id
  and a.policy_id = pc.policy_id			 
  and a.lob_id = pc.lob_id and pc.coverage_id = (select max(pc1.coverage_id) from [dbo].[fh_policy_coverage] pc1 where pc.policy_id = pc1.policy_id and pc.lob_id = pc1.lob_id)
  and e.reserve_type_id = h.reserve_type_id
  and i.item_id = h.reserve_group_id
  and a.claim_id = c.claim_id
  and c.history_datetime = (select max(cc.history_datetime)	
	    					from [dbo].[fh_claim_history] cc
		    				where c.claim_id = cc.claim_id
			    				and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
				    			and cc.history_action_type = case when cc.open_close_status = 'O'
					    										  then 'I'
						        								  else cc.history_action_type end
							    and cc.history_datetime &lt;= dateadd(ss, 3, ph.history_datetime))
  and p.party_id = e.party_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and a.claim_id = p.claim_id
  and p.party_id = ph.party_id
  and ph.history_action_type = case when ph.open_close_status = 'O'
	    							then 'I'
								    else ph.history_action_type end
  and ph.history_datetime = (select max(ph2.history_datetime)	
						     from [dbo].[fh_party_history] ph2
						     where ph.party_id = ph2.party_id
							   and ph2.history_action_type = case when ph2.open_close_status = 'O'
															      then 'I'
															      else ph2.history_action_type end)
  and b.tier_company_id = g.company_id
  and datepart(yyyy, a.loss_date) in (@AccidentYear)
  and (a.loss_causation_id in (@CauseOfLossID)
   or  IsNull(a.loss_causation_id, '0') = case when @CauseOfLossID = '0' then IsNull(a.loss_causation_id, '0') end)
  and b.policy_begin_date between crt.TreatyBeginDate and crt.TreatyEndDate
  and crt.CompanyID = case when b.primary_carrier_id in ('ITSC01', 'ITSEXS') then 6
		                   when b.primary_carrier_id in ('ISTILL', 'ISWEXS') then 7 end
  and crt.CompanyReinsTreatyID = crtls.CompanyReinsTreatyID
  and crtls.LineID = case when a.lob_id in ('AL', 'AP') then 7
						  when a.lob_id = 'CP'			then 6
						  when a.lob_id = 'CR'			then 14
						  when a.lob_id = 'UE'			then 24
						  when a.lob_id = 'GL'			then 5
						  when a.lob_id = 'IM'			then 22 end
) aaaaa
where aaaaa.reserve_date &lt; dateadd(d, 1, @AsOfDate) and aaaaa.reserve_date &gt;= @RptBeginDate
  and aaaaa.reserve_date &lt; dateadd(d, 1, @RptEndDate)
  and IsNull(case when aaaaa.MainTransType = 'Paid Loss' and aaaaa.IndExp = 'I'
					   then aaaaa.Amount
					   end, 0) &lt;&gt; 0.00

union all

select 2 CategoryOrder, @RptYear RptYear, 
       'Expense Payments' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month, 
	   aaaaa.company_name InsuredName,
	   aaaaa.policy_num,
	   aaaaa.ClaimantName,
	   aaaaa.Adjuster,
	   aaaaa.lob_id,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
	   IsNull(case when aaaaa.MainTransType = 'Paid Loss' and aaaaa.IndExp = 'E'
					   then aaaaa.Amount
					   end, 0) Amount
from (
select a.claim_id,
	   g.company_name,
	   b.policy_num,
	   ltrim(case when p.first_name is NULL 
		          then p.last_name
		          else p.first_name + ' ' + p.last_name end) ClaimantName,
        d.first_name + ' ' + d.last_name Adjuster,
       a.fh_claim_num claim_num,
	   a.lob_id,
	   a.loss_date,
	   a.reported_date,
	   e.reserve_date,
	   f.reserve_type_name,
	   e.transaction_indicator,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when e.recovery_type is not NULL or e.reserve_description like '%Recovery%'
			then 'Recovery'
			when e.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when e.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where e.recovery_type = rt.recovery_type_code)
			when e.reserve_or_paid_code = 'R'
	        then e.reserve_description
			end TransType,
	   case when i.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,
	   e.loss_amt * case when @GICOnly = 1
								then IsNull(crtls.TreatySharePct, 0)
								else 1 end Amount
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
	 [dbo].[fh_reserve_paid] e,
	 [dbo].[fh_reserve_type] f, 
	 [dbo].[fh_policy_coverage] pc,
	 [dbo].[fh_reserve_type] h,
	 [dbo].[fh_code_general] i,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreaties] crt,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreatyLineShare] crtls,
	 [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] p,
	 [dbo].[fh_party_history] ph,
	 [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = e.claim_id
  and e.reserve_or_paid_code = 'P'
/*  MH  6/14/2022 - Fix for Paid Reimbursements
  and e.transaction_indicator &lt;&gt; 'R' */
  and (e.transaction_indicator &lt;&gt; 'R' or IsNull(e.recovery_type, '') = 4)
  and e.reserve_type_id = f.reserve_type_id
  and a.policy_id = pc.policy_id			 
  and a.lob_id = pc.lob_id and pc.coverage_id = (select max(pc1.coverage_id) from [dbo].[fh_policy_coverage] pc1 where pc.policy_id = pc1.policy_id and pc.lob_id = pc1.lob_id)
  and e.reserve_type_id = h.reserve_type_id
  and i.item_id = h.reserve_group_id
  and a.claim_id = c.claim_id
  and c.history_datetime = (select max(cc.history_datetime)	
	    					from [dbo].[fh_claim_history] cc
		    				where c.claim_id = cc.claim_id
			    				and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
				    			and cc.history_action_type = case when cc.open_close_status = 'O'
					    										  then 'I'
						        								  else cc.history_action_type end
							    and cc.history_datetime &lt;= dateadd(ss, 3, ph.history_datetime))
  and p.party_id = e.party_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and a.claim_id = p.claim_id
  and p.party_id = ph.party_id
  and ph.history_action_type = case when ph.open_close_status = 'O'
	    							then 'I'
								    else ph.history_action_type end
  and ph.history_datetime = (select max(ph2.history_datetime)	
						     from [dbo].[fh_party_history] ph2
						     where ph.party_id = ph2.party_id
							   and ph2.history_action_type = case when ph2.open_close_status = 'O'
															      then 'I'
															      else ph2.history_action_type end)
  and b.tier_company_id = g.company_id
  and datepart(yyyy, a.loss_date) in (@AccidentYear)
  and (a.loss_causation_id in (@CauseOfLossID)
   or  IsNull(a.loss_causation_id, '0') = case when @CauseOfLossID = '0' then IsNull(a.loss_causation_id, '0') end)
  and b.policy_begin_date between crt.TreatyBeginDate and crt.TreatyEndDate
  and crt.CompanyID = case when b.primary_carrier_id in ('ITSC01', 'ITSEXS') then 6
		                   when b.primary_carrier_id in ('ISTILL', 'ISWEXS') then 7 end
  and crt.CompanyReinsTreatyID = crtls.CompanyReinsTreatyID
  and crtls.LineID = case when a.lob_id in ('AL', 'AP') then 7
						  when a.lob_id = 'CP'			then 6
						  when a.lob_id = 'CR'			then 14
						  when a.lob_id = 'UE'			then 24
						  when a.lob_id = 'GL'			then 5
						  when a.lob_id = 'IM'			then 22 end
) aaaaa
where aaaaa.reserve_date &lt; dateadd(d, 1, @AsOfDate) and aaaaa.reserve_date &gt;= @RptBeginDate
  and aaaaa.reserve_date &lt; dateadd(d, 1, @RptEndDate)
  and IsNull(case when aaaaa.MainTransType = 'Paid Loss' and aaaaa.IndExp = 'E'
					   then aaaaa.Amount
					   end, 0) &lt;&gt; 0.00

union all
select 3 CategoryOrder, @RptYear RptYear, 
       'Recoveries' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month, 
	   aaaaa.company_name InsuredName,
	   aaaaa.policy_num,
	   aaaaa.ClaimantName,
	   aaaaa.Adjuster,
	   aaaaa.lob_id,
	   aaaaa.claim_num,
	   aaaaa.loss_date,
	   aaaaa.reported_date,
	   aaaaa.reserve_date,
	   aaaaa.MainTransType,
	   aaaaa.TransType,
	   aaaaa.reserve_type_name,
	   aaaaa.transaction_indicator,
	   IsNull(case when aaaaa.MainTransType = 'Recovery'
					   then aaaaa.Amount * -1
					   end, 0) Amount
from (
select a.claim_id,
	   g.company_name,
	   b.policy_num,
	   ltrim(case when p.first_name is NULL 
		          then p.last_name
		          else p.first_name + ' ' + p.last_name end) ClaimantName,
        d.first_name + ' ' + d.last_name Adjuster,
       a.fh_claim_num claim_num,
	   a.lob_id,
	   a.loss_date,
	   a.reported_date,
	   e.reserve_date,
	   f.reserve_type_name,
	   e.transaction_indicator,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
	        when e.recovery_type is not NULL or e.reserve_description like '%Recovery%'
			then 'Recovery'
			when e.reserve_or_paid_code = 'R'
	        then 'Reserve'
			end MainTransType,
	   case when e.reserve_or_paid_code = 'P'
			then 'Paid Loss'
			when e.recovery_type is not NULL
			then (select rt.recovery_type_desc
			      from [dbo].[fv_lookup_pay_recov_type] rt
				  where e.recovery_type = rt.recovery_type_code)
			when e.reserve_or_paid_code = 'R'
	        then e.reserve_description
			end TransType,
	   case when i.item_name = 'Indemnity' then 'I'
	        else 'E' end IndExp,
	   e.loss_amt * case when @GICOnly = 1
								then IsNull(crtls.TreatySharePct, 0)
								else 1 end Amount
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
	 [dbo].[fh_reserve_paid] e,
	 [dbo].[fh_reserve_type] f, 
	 [dbo].[fh_policy_coverage] pc,
	 [dbo].[fh_reserve_type] h,
	 [dbo].[fh_code_general] i,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreaties] crt,
	 [MGADS0005.NY.MGASYSTEMS.COM].[GramercyRisk].[ReportReference].[CompanyReinsTreatyLineShare] crtls,
	 [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] p,
	 [dbo].[fh_party_history] ph,
	 [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = e.claim_id
  and e.reserve_type_id = f.reserve_type_id
  and a.policy_id = pc.policy_id		 
  and a.lob_id = pc.lob_id and pc.coverage_id = (select max(pc1.coverage_id) from [dbo].[fh_policy_coverage] pc1 where pc.policy_id = pc1.policy_id and pc.lob_id = pc1.lob_id)
  and e.reserve_type_id = h.reserve_type_id
  and i.item_id = h.reserve_group_id
  and a.claim_id = c.claim_id
  and c.history_datetime = (select max(cc.history_datetime)	
	    					from [dbo].[fh_claim_history] cc
		    				where c.claim_id = cc.claim_id
			    				and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
				    			and cc.history_action_type = case when cc.open_close_status = 'O'
					    										  then 'I'
						        								  else cc.history_action_type end
							    and cc.history_datetime &lt;= dateadd(ss, 3, ph.history_datetime))
  and p.party_id = e.party_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and a.claim_id = p.claim_id
  and p.party_id = ph.party_id
  and ph.history_action_type = case when ph.open_close_status = 'O'
	    							then 'I'
								    else ph.history_action_type end
  and ph.history_datetime = (select max(ph2.history_datetime)	
						     from [dbo].[fh_party_history] ph2
						     where ph.party_id = ph2.party_id
							   and ph2.history_action_type = case when ph2.open_close_status = 'O'
															      then 'I'
															      else ph2.history_action_type end)
  and b.tier_company_id = g.company_id
  and datepart(yyyy, a.loss_date) in (@AccidentYear)
  and (a.loss_causation_id in (@CauseOfLossID)
   or  IsNull(a.loss_causation_id, '0') = case when @CauseOfLossID = '0' then IsNull(a.loss_causation_id, '0') end)
  and b.policy_begin_date between crt.TreatyBeginDate and crt.TreatyEndDate
  and crt.CompanyID = case when b.primary_carrier_id in ('ITSC01', 'ITSEXS') then 6
		                   when b.primary_carrier_id in ('ISTILL', 'ISWEXS') then 7 end
  and crt.CompanyReinsTreatyID = crtls.CompanyReinsTreatyID
  and crtls.LineID = case when a.lob_id in ('AL', 'AP') then 7
						  when a.lob_id = 'CP'			then 6
						  when a.lob_id = 'CR'			then 14
						  when a.lob_id = 'UE'			then 24
						  when a.lob_id = 'GL'			then 5
						  when a.lob_id = 'IM'			then 22 end
) aaaaa
where aaaaa.reserve_date &lt; dateadd(d, 1, @AsOfDate) and aaaaa.reserve_date &gt;= @RptBeginDate
  and aaaaa.reserve_date &lt; dateadd(d, 1, @RptEndDate)
  and IsNull(case when aaaaa.MainTransType = 'Recovery'
					   then aaaaa.Amount * -1
					   end, 0) &lt;&gt; 0.00


) aaaaaa
where aaaaaa.CategoryType like '%' + (@CategoryType)
  and aaaaaa.lob_id in (@LOB)

end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CategoryOrder">
          <DataField>CategoryOrder</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RptYear">
          <DataField>RptYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategoryType">
          <DataField>CategoryType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthNum">
          <DataField>MonthNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="claim_num">
          <DataField>claim_num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InsuredName">
          <DataField>InsuredName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="policy_num">
          <DataField>policy_num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
