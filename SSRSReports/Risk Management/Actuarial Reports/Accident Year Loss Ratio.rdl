                              <TextRuns>
                                <TextRun>
                                  <Value>=IIf(
IIf(Parameters!ReportPeriod.Value = "Y",
dateadd("yyyy", RunningValue(CountDistinct("accident_year"),Count,"accident_year") -1, CDate(Cstr(max(Fields!accident_year.Value, "accident_year")) &amp; "-01-01")),
IIf(Parameters!ReportPeriod.Value = "Q",
dateadd("q", RunningValue(CountDistinct("accident_year"),Count,"accident_year") -1, CDate(Cstr(max(Fields!accident_year.Value, "accident_year")) &amp; "-01-01")),
dateadd("m", RunningValue(CountDistinct("accident_year"),Count,"accident_year") -1, CDate(Cstr(max(Fields!accident_year.Value, "accident_year")) &amp; "-01-01"))))

&gt;= dateadd("m", 1, CDate(Parameters!ReportYear.Value &amp; "-" &amp; Parameters!ReportMonth.Value &amp; "-01"))

, 0, 
IIf(Parameters!CellAggregration.Value = "PO",
Sum(IIf(Parameters!ProgramGIC.Value = "P", Fields!IndemnityIncurredLoss.Value, Fields!IndemnityIncurredLoss.Value * Fields!TreatySharePct.Value)) + Sum(IIf(Parameters!ProgramGIC.Value = "P", Fields!ExpenseIncurredLoss.Value, Fields!ExpenseIncurredLoss.Value * Fields!TreatySharePct.Value)),
RunningValue(IIf(Parameters!ProgramGIC.Value = "P", Fields!IndemnityIncurredLoss.Value, Fields!IndemnityIncurredLoss.Value * Fields!TreatySharePct.Value), Sum, "accident_year") + 
 RunningValue(IIf(Parameters!ProgramGIC.Value = "P", Fields!ExpenseIncurredLoss.Value, Fields!ExpenseIncurredLoss.Value * Fields!TreatySharePct.Value), Sum, "accident_year")))</Value>
                                  <Style>
                                    <FontWeight>Normal</FontWeight>
                                    <Format>'$'#,0.00;('$'#,0.00);''</Format>
                                    <Color>#333333</Color>
                                  </Style>
                                </TextRun>
                              </TextRuns>
                              <Style />
