    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AsOfDate">
            <Value>=Parameters!AsOfDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LOB">
            <Value>=Parameters!LOB.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Carrier">
            <Value>=Parameters!Carrier.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @AsOfDate			datetime
declare @LOB				varchar(2000)
declare @Carrier			varchar(100)

*/

begin

/*

set @AsOfDate = '1/6/2022'
set @LOB = 'Auto Liability'
set @Carrier = 'Tri-State Consumer Insurance Company'

*/

select a.*,
	   0 TotWP,
	   0 AssumedWP,
	   0 TotEP,
	   0 AssumedEP
from [dbo].[LossesByYYYYMM] a
where a.added_date &lt; dateadd(d, 1, @AsOfDate)
  and a.LOB in (@LOB)
  and a.Carrier in (@Carrier)
union all
select '',
       '',
       '',
       '',
       '',
       '',
       '',
       '',
       '',
       '',
	   left(b.YYYYMM, 4) YYYY,
	   convert(date, NULL),
	   '',
	   datediff(yyyy, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(q, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(m, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(yyyy, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(q, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(m, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   convert(date, NULL),
	   0,
	   0,
	   b.ClaimLineName LOB,
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   b.CompanyName Carrier,
	   0,
	   0,
	   0,
	   0,
	   0,
	   0,
	   0,
	   0,
	   convert(date, NULL),
	   convert(date, NULL),
	   0,
	   NULL,
	   0,
	   0,
	   sum(PeriodEarnedPremium) TotEP,
	   sum(PeriodAssumedEarnedPremium) AssumedEP
from [Sandbox].[dbo].[v_PremiumByYYYYMM] b
where b.ClaimLineName in (@LOB)
  and b.CompanyName in (@Carrier)
  and b.YYYYMM &lt;= convert(char(6), @AsOfDate, 112)
group by left(b.YYYYMM, 4),
     b.ClaimLineName,
	 b.CompanyName

union all
select '',
       '',
       '',
       '',
       '',
       '',
       '',
       '',
       '',
       '',
	   left(b.YYYYMM, 4) YYYY,
	   convert(date, NULL),
	   '',	   
	   datediff(yyyy, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(q, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(m, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(yyyy, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(q, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   datediff(m, convert(date, left(b.YYYYMM, 4) + '0101'), @AsOfDate),
	   convert(date, NULL),
	   0,
	   0,
	   b.ClaimLineName LOB,
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   '',
	   b.CompanyName Carrier,
	   0,
	   0,
	   0,
	   0,
	   0,
	   0,
	   0,
	   0,
	   convert(date, NULL),
	   convert(date, NULL),
	   0,
	   NULL,
	   sum(GWP) TotWP,
	   sum(AssumedPremium) AssumedWP,
	   0 TotEP,
	   0 AssumedEP
from [Sandbox].[dbo].[v_PremiumByYYYYMM] b
where b.ClaimLineName in (@LOB)
  and b.CompanyName in (@Carrier)
  and b.YYYYMM &lt;= convert(char(6), @AsOfDate, 112)
  and b.YYYYMM = (select min(bb.YYYYMM)
				  from [Sandbox].[dbo].[v_PremiumByYYYYMM] bb
				  where bb.QuoteID = b.QuoteID)
group by left(b.YYYYMM, 4),
     b.ClaimLineName,
	 b.CompanyName
       

end</CommandText>
