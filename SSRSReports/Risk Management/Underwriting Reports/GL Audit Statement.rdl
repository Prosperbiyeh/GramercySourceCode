        <CommandText>/*

declare @ReportBeginDate  date
declare @InsuredName	  varchar(500)

set @ReportBeginDate	= '12/31/2021'
set @InsuredName		= 'Misa Plumbing, LLC'

*/

begin

/*

create table #Auto_Subline_Premium_by_QuoteID (QuoteID int, PolicyNumber varchar(20), ALPrem money, APDPrem money, AutoBTM money)
create nonclustered index idx_QuoteID on #Auto_Subline_Premium_by_QuoteID (QuoteID)

insert into #Auto_Subline_Premium_by_QuoteID
select a.*
from [ReportReference].[v_Auto_Subline_Premium_by_QuoteID] a,
     [dbo].[tblQuotes] b,
	 [dbo].[lstLines] c
where a.QuoteID = b.QuoteID
  and b.LineGUID = c.LineGUID
  and c.LineName = 'Commercial Auto'
  and convert(date, b.EffectiveDate) between dateadd(yyyy, -1, convert(date, @ReportBeginDate)) and convert(date, @ReportEndDate)


*/



select *
from (
select --aaa.QuoteID,
       aaa.InsuredName,
	   max(aaa.Address1) InsuredAddress1,
	   max(aaa.Address2) InsuredAddress2,
	   max(aaa.City) InsuredCity,
	   max(aaa.State) InsuredState,
	   max(aaa.ZipCode) InsuredZipCode,
       max(aaa.ContractorType) ContractorType,
	   max(aaa.Broker) Broker,
	   max(aaa.ProducerAddress1) ProducerAddress1,
	   max(aaa.ProducerAddress2) ProducerAddress2,
	   max(aaa.ProducerCity) ProducerCity,
	   max(aaa.ProducerState) ProducerState,
	   max(aaa.ProducerZipCode) ProducerZipCode,
	   max(aaa.PolicyNumber) PolicyNumber,
	   max(aaa.CarrierName) CarrierName,
	   max(aaa.Underwriter) Underwriter,
	   aaa.LineOfBusiness,
	   aaa.LOBCategory,
	   aaa.EffectiveDate PriorYearEffDate,
	   max(aaa.EffectiveDate) CurrYearEffDate,
	   min(aaa.ExpirationDate) PriorYearExpDate,
	   max(aaa.ExpirationDate) CurrYearExpDate,
---	   aaa.UnitNumber,
	   aaa.Description Clarifications,
	   aaa.ClassCode GLClassCode,
	   aaa.ExposureBasis ExposureType,	   
	   sum(aaa.CurrPremium) CurrPremium,
	   replace(convert(varchar, max(case when aaa.CurrPremium = 0 then '0.00' else aaa.CurrExposure end)), '.00', '') CurrExposure,
	   max(aaa.CurrScheduleMod) CurrScheduleMod,
	   max(aaa.CurrExperienceMod) CurrExperienceMod,
	   max(aaa.CurrFinalMod) CurrFinalMod,
	   sum(aaa.RenPremium) RenPremium,
	   sum(aaa.PremFinalRate) PremFinalRate,
	   sum(aaa.PremTotalPremium) PremTotalPremium,
	   sum(aaa.PremEndorse) PremTotalEndorse,
	   sum(aaa.ProdFinalRate) ProdFinalRate,
--	   sum(aaa.ProdTotalPremium) ProdTotalPremium,
	   sum(aaa.ProdEndorse) ProdTotalEndorse,
	   replace(convert(varchar, max(case when aaa.RenPremium = 0 then '0.00' else aaa.RenExposure end)), '.00', '') RenExposure,
	   max(aaa.RenScheduleMod) RenScheduleMod,
	   max(aaa.RenExperienceMod) RenExperienceMod,
	   max(aaa.RenFinalMod) RenFinalMod,
	   max(aaa.ExpiringQuoteID) ExpiringQuoteID,
	   max(aaa.AuditQuoteID) AuditQuoteID,
/*  MH  11/17/2021 - Removing min for Return Premium transactions, adding to group
	   max(aaa.TerrEndorse) TerrEndorse
*/
	   IsNull(max(case when aaa.TerrEndorse &lt;&gt; 0
					   then aaa.TerrEndorse end), 0) TerrEndorse
from ( 



/*  GL  */



select --f.LiabilityExposuresID,
--       a.QuoteID,
	   a.QuoteID AuditQuoteID,
	   Null ExpiringQuoteID,
--       a.InsuredPolicyName InsuredName,
	   ti.PolicyName InsuredName,
	   tiloc.Address1,
	   tiloc.Address2,
	   tiloc.City,
	   tiloc.State,
	   tiloc.ZipCode,
	   a.RiskDescription ContractorType,
	  (select co.CompanyName
	   from [GramercyRisk].[dbo].[tblCompanyLines] cl,
	        [GramercyRisk].[dbo].[tblCompanyLocations] cloc,
			[GramercyRisk].[dbo].[tblCompanies] co
	   where a.CompanyLineGUID = cl.CompanyLineGUID
	     and cl.CompanyLocationGUID = cloc.CompanyLocationGUID
		 and cloc.CompanyGUID = co.CompanyGUID) CarrierName,
	   a.ProducerName Broker,
	   a.ProducerAddress1,
	   a.ProducerAddress2,
	   a.ProducerCity,
	   a.ProducerState,
	   a.ProducerZipCode,
	   users.FirstName + ' ' +  users.LastName Underwriter,
	   a.PolicyNumber,
	   'Commercial General Liability' LineOfBusiness,
	   'Prem Ops/Prod Comp Ops' LOBCategory,
	   convert(date, a.EffectiveDate) EffectiveDate,
	   convert(date, a.ExpirationDate) ExpirationDate,
	   f.UnitNumber,
	   f.Description,
	   f.ClassCode,       
       case when f.ClassCode in    (91581,
									91582,
									91583,
									91584,
									91585,
									91587,
									91591)
	        then 'SUB COSTS'
			else upper(f.ExposureBasis) end ExposureBasis,
	   f.Exposure RenExposure,
	   g.LiabilityPremium RenPremium,
	   g.PremFinalRate,
	   g.ProdFinalRate,
	   IsNull(g.PremTotalPremium, 0) PremTotalPremium,
	   IsNull(g.ProdTotalPremium, 0) ProdTotalPremium,
	   g.PremEndorse,
	   g.ProdEndorse,
	   e.LiabIRPMMod RenScheduleMod,
	   e.LiabExperMod RenExperienceMod,
	   e.LiabRMF RenFinalMod,
	   '' CurrExposure,
	   NULL CurrPremium,
	   NULL CurrScheduleMod,
	   NULL CurrExperienceMod,
	   '' CurrFinalMod,
	   IsNull(
	   (select qp.TerrorismEndorse
	    from [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] qp
		where a.NetRate_QuoteID = qp.QuoteID), 0) TerrEndorse
from [GramercyRisk].[dbo].[tblQuotes] a,
	 [GramercyRisk].[dbo].[lstPolicyTypes] b,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat] e,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat_Liabi] f,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat_Liabi_Liabi] g,
	 [GramercyRisk].[dbo].[tblUsers] users,
	 [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
	 [GramercyRisk].[dbo].[tblInsureds] ti,
	 [GramercyRisk].[dbo].[tblInsuredLocations] tiloc
where a.PolicyNumber is not NULL
  and a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.InsuredPolicyName = a.InsuredPolicyName
					and aa.CompanyLineGuid = a.CompanyLineGuid
					and convert(date, aa.ExpirationDate) = convert(date, @ReportBeginDate)
					and aa.TransactionTypeID = 'A') 
  and a.PolicyTypeID = b.PolicyTypeID
  and a.NetRate_QuoteID = d.QuoteID
  and d.QuoteID = e.QuoteID
  and e.LocationID = f.LocationID
  and f.LiabilityExposuresID = g.LiabilityExposuresID
  and IsNull(f.[Transaction], '1') &lt;&gt; 'Deleted'
  and a.UnderwriterUserGuid = users.UserGUID
  and a.SubmissionGroupGuid = tsg.SubmissionGroupGUID
  and tsg.InsuredGuid = ti.InsuredGUID
  and ti.PolicyName = @InsuredName
  and ti.InsuredGUID = tiloc.InsuredGUID
  and tiloc.LocationTypeID = (select min(tiloc2.LocationTypeID)
							  from [GramercyRisk].[dbo].[tblInsuredLocations] tiloc2
							  where tiloc.InsuredGUID = tiloc2.InsuredGUID)
--  and a.InsuredPolicyName like 'Zena Electric Inc.'
union all
select --f.LiabilityExposuresID,
--       a.QuoteID,
	   Null AuditQuoteID,
	   a.QuoteID ExpiringQuoteID,
--       a.InsuredPolicyName InsuredName,
	   ti.PolicyName InsuredName,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   a.RiskDescription ContractorType,
	  (select co.CompanyName
	   from [GramercyRisk].[dbo].[tblCompanyLines] cl,
	        [GramercyRisk].[dbo].[tblCompanyLocations] cloc,
			[GramercyRisk].[dbo].[tblCompanies] co
	   where a.CompanyLineGUID = cl.CompanyLineGUID
	     and cl.CompanyLocationGUID = cloc.CompanyLocationGUID
		 and cloc.CompanyGUID = co.CompanyGUID) CarrierName,
	   NULL Broker,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   NULL Underwriter,
	   a.PolicyNumber,
	   'Commercial General Liability' LineOfBusiness,
	   'Prem Ops/Prod Comp Ops' LOBCategory,
	   convert(date, a.EffectiveDate) EffectiveDate,
	   convert(date, a.ExpirationDate) ExpirationDate,
	   f.UnitNumber,
	   f.Description,
	   f.ClassCode,
       case when f.ClassCode in    (91581,
									91582,
									91583,
									91584,
									91585,
									91587,
									91591)
	        then 'SUB COSTS'
			else upper(f.ExposureBasis) end ExposureBasis,
	   '' RenExposure,
	   0 RenPremium,
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   NULL RenScheduleMod,
	   NULL RenExperienceMod,
	   '' RenFinalMod,
	   f.Exposure CurrExposure,
	   g.LiabilityPremium CurrPremium,
	   e.LiabIRPMMod CurrScheduleMod,
	   e.LiabExperMod CurrExperienceMod,
	   e.LiabRMF CurrFinalMod,
	   0 TerrEndorse
from [GramercyRisk].[dbo].[tblQuotes] a,
	 [GramercyRisk].[dbo].[lstPolicyTypes] b,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat] e,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat_Liabi] f,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat_Liabi_Liabi] g,
	 [GramercyRisk].[dbo].[tblUsers] users,
	 [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
	 [GramercyRisk].[dbo].[tblInsureds] ti
where a.PolicyNumber is not NULL
/*  and exists (select 1
			  from [GramercyRisk].[dbo].[tblQuotes] aa
			  where aa.PolicyNumber = a.PolicyNumber
			    and convert(date, aa.ExpirationDate) between convert(date, @ReportBeginDate) and convert(date, @ReportEndDate)
			    and aa.TransactionTypeID = 'A') */
  and a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.InsuredPolicyName = a.InsuredPolicyName
					and aa.CompanyLineGuid = a.CompanyLineGuid
					and convert(date, aa.ExpirationDate) = convert(date, @ReportBeginDate)
					and IsNull(aa.TransactionTypeID, 'E') &lt;&gt; 'A'
					)
  and a.PolicyTypeID = b.PolicyTypeID
  and a.NetRate_QuoteID = d.QuoteID
  and d.QuoteID = e.QuoteID
  and e.LocationID = f.LocationID
  and f.LiabilityExposuresID = g.LiabilityExposuresID
  and IsNull(f.[Transaction], '1') &lt;&gt; 'Deleted'
  and a.UnderwriterUserGuid = users.UserGUID
  and a.SubmissionGroupGuid = tsg.SubmissionGroupGUID
  and tsg.InsuredGuid = ti.InsuredGUID
  and ti.PolicyName = @InsuredName
--  and a.InsuredPolicyName like 'Zena Electric Inc.'

union all

/*  GL  Optional Coverages*/



select --f.LiabilityExposuresID,
--       a.QuoteID,
	   a.QuoteID AuditQuoteID,
	   Null ExpiringQuoteID,
--       a.InsuredPolicyName InsuredName,
	   ti.PolicyName InsuredName,
	   tiloc.Address1,
	   tiloc.Address2,
	   tiloc.City,
	   tiloc.State,
	   tiloc.ZipCode,
	   a.RiskDescription ContractorType,
	  (select co.CompanyName
	   from [GramercyRisk].[dbo].[tblCompanyLines] cl,
	        [GramercyRisk].[dbo].[tblCompanyLocations] cloc,
			[GramercyRisk].[dbo].[tblCompanies] co
	   where a.CompanyLineGUID = cl.CompanyLineGUID
	     and cl.CompanyLocationGUID = cloc.CompanyLocationGUID
		 and cloc.CompanyGUID = co.CompanyGUID) CarrierName,
	   a.ProducerName Broker,
	   a.ProducerAddress1,
	   a.ProducerAddress2,
	   a.ProducerCity,
	   a.ProducerState,
	   a.ProducerZipCode,
	   users.FirstName + ' ' +  users.LastName Underwriter,
	   a.PolicyNumber,
	   'Commercial General Liability' LineOfBusiness,
	   'Prem Ops/Prod Comp Ops' LOBCategory,
	   convert(date, a.EffectiveDate) EffectiveDate,
	   convert(date, a.ExpirationDate) ExpirationDate,
	   f.UnitNumber,
	   f.Description,
	   NULL ClassCode,       
       NULL ExposureBasis,
	   f.Amount RenExposure,
	   f.Premium RenPremium,
	   case when IsNull(f.Amount, 0) &lt;&gt; 0
		    then f.Premium / f.Amount
	        else f.Premium end PremFinalRate,
	   convert(money, 0) ProdFinalRate,
	   convert(money, 0) PremTotalPremium,
	   convert(money, 0) ProdTotalPremium,
	   f.Endorse PremEndorse,
	   convert(money, 0) ProdEndorse,
	   e.LiabIRPMMod RenScheduleMod,
	   e.LiabExperMod RenExperienceMod,
	   e.LiabRMF RenFinalMod,
	   '' CurrExposure,
	   NULL CurrPremium,
	   NULL CurrScheduleMod,
	   NULL CurrExperienceMod,
	   '' CurrFinalMod,
	   IsNull(
	   (select qp.TerrorismEndorse
	    from [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] qp
		where a.NetRate_QuoteID = qp.QuoteID), 0) TerrEndorse
from [GramercyRisk].[dbo].[tblQuotes] a,
	 [GramercyRisk].[dbo].[lstPolicyTypes] b,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat] e,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat_Compa] f,
	 [GramercyRisk].[dbo].[tblUsers] users,
	 [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
	 [GramercyRisk].[dbo].[tblInsureds] ti,
	 [GramercyRisk].[dbo].[tblInsuredLocations] tiloc
where a.PolicyNumber is not NULL
  and a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.InsuredPolicyName = a.InsuredPolicyName
					and aa.CompanyLineGuid = a.CompanyLineGuid
					and convert(date, aa.ExpirationDate) = convert(date, @ReportBeginDate)
					and aa.TransactionTypeID = 'A') 
  and a.PolicyTypeID = b.PolicyTypeID
  and a.NetRate_QuoteID = d.QuoteID
  and d.QuoteID = e.QuoteID
  and e.LocationID = f.LocationID
  and IsNull(f.[Transaction], '1') &lt;&gt; 'Deleted'
  and f.LOB = 'Liability'
  and a.UnderwriterUserGuid = users.UserGUID
  and a.SubmissionGroupGuid = tsg.SubmissionGroupGUID
  and tsg.InsuredGuid = ti.InsuredGUID
  and ti.PolicyName = @InsuredName
  and ti.InsuredGUID = tiloc.InsuredGUID
  and tiloc.LocationTypeID = (select min(tiloc2.LocationTypeID)
							  from [GramercyRisk].[dbo].[tblInsuredLocations] tiloc2
							  where tiloc.InsuredGUID = tiloc2.InsuredGUID)
--  and a.InsuredPolicyName like 'Zena Electric Inc.'


union all
select --f.LiabilityExposuresID,
--       a.QuoteID,
	   Null AuditQuoteID,
	   a.QuoteID ExpiringQuoteID,
--       a.InsuredPolicyName InsuredName,
	   ti.PolicyName InsuredName,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   a.RiskDescription ContractorType,
	  (select co.CompanyName
	   from [GramercyRisk].[dbo].[tblCompanyLines] cl,
	        [GramercyRisk].[dbo].[tblCompanyLocations] cloc,
			[GramercyRisk].[dbo].[tblCompanies] co
	   where a.CompanyLineGUID = cl.CompanyLineGUID
	     and cl.CompanyLocationGUID = cloc.CompanyLocationGUID
		 and cloc.CompanyGUID = co.CompanyGUID) CarrierName,
	   NULL Broker,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   NULL,
	   NULL Underwriter,
	   a.PolicyNumber,
	   'Commercial General Liability' LineOfBusiness,
	   'Prem Ops/Prod Comp Ops' LOBCategory,
	   convert(date, a.EffectiveDate) EffectiveDate,
	   convert(date, a.ExpirationDate) ExpirationDate,
	   f.UnitNumber,
	   f.Description,
	   NULL ClassCode,
       NULL ExposureBasis,
	   '' RenExposure,
	   0 RenPremium,
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   convert(money, 0),
	   NULL RenScheduleMod,
	   NULL RenExperienceMod,
	   '' RenFinalMod,
	   f.Amount CurrExposure,
	   f.Premium CurrPremium,
	   e.LiabIRPMMod CurrScheduleMod,
	   e.LiabExperMod CurrExperienceMod,
	   e.LiabRMF CurrFinalMod,
	   0 TerrEndorse
from [GramercyRisk].[dbo].[tblQuotes] a,
	 [GramercyRisk].[dbo].[lstPolicyTypes] b,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat] e,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Locat_Compa] f,
	 [GramercyRisk].[dbo].[tblUsers] users,
	 [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
	 [GramercyRisk].[dbo].[tblInsureds] ti
where a.PolicyNumber is not NULL
/*  and exists (select 1
			  from [GramercyRisk].[dbo].[tblQuotes] aa
			  where aa.PolicyNumber = a.PolicyNumber
			    and convert(date, aa.ExpirationDate) between convert(date, @ReportBeginDate) and convert(date, @ReportEndDate)
			    and aa.TransactionTypeID = 'A') */
  and a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.InsuredPolicyName = a.InsuredPolicyName
					and aa.CompanyLineGuid = a.CompanyLineGuid
					and convert(date, aa.ExpirationDate) = convert(date, @ReportBeginDate)
					and IsNull(aa.TransactionTypeID, 'E') &lt;&gt; 'A'
					)
  and a.PolicyTypeID = b.PolicyTypeID
  and a.NetRate_QuoteID = d.QuoteID
  and d.QuoteID = e.QuoteID
  and e.LocationID = f.LocationID
  and IsNull(f.[Transaction], '1') &lt;&gt; 'Deleted'
  and f.LOB = 'Liability'
  and a.UnderwriterUserGuid = users.UserGUID
  and a.SubmissionGroupGuid = tsg.SubmissionGroupGUID
  and tsg.InsuredGuid = ti.InsuredGUID
  and ti.PolicyName = @InsuredName
--  and a.InsuredPolicyName like 'Zena Electric Inc.'



) aaa
group by 
 --      aaa.QuoteID,
       aaa.InsuredName,
	   aaa.EffectiveDate,
--       aaa.ContractorType,
	   aaa.LineOfBusiness,
	   aaa.LOBCategory,
	   aaa.UnitNumber,
	   aaa.Description,
	   aaa.ClassCode,
	   aaa.ExposureBasis/*,
/*  MH  11/17/2021 - adding terrorism prem to group */
	   aaa.TerrEndorse
--having '0' &lt; max(aaa.AuditQuoteID) */
) aaaaa
order by IsNull(aaaaa.GLClassCode, 'ZZZZZ'), aaaaa.Clarifications

						   

--drop table #Auto_Subline_Premium_by_QuoteID

end

