  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PolEffBeginDate">
            <Value>=Parameters!PolEffBeginDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportAsOfDate">
            <Value>=Parameters!ReportAsOfDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PolEffEndDate">
            <Value>=Parameters!PolEffEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InForceOnly">
            <Value>=Parameters!InForceOnly.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyGUID">
            <Value>=Parameters!CompanyGUID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @PolEffBeginDate		date;
declare @PolEffEndDate			date;
declare @ReportAsOfDate			date;
declare @InForceOnly			int;
declare @CompanyGUID			uniqueidentifier;

set @PolEffBeginDate	= '7/1/2017'
set @PolEffEndDate		= '7/31/2022'
set @ReportAsOfDate		= '7/31/2022'
set @InForceOnly		= 1
set @CompanyGUID		= 'CF3A1D5B-200D-4089-ADDD-BEF011BA7031'

*/

begin


/* Umbrella General Liability */

select aaaa.InsuredName,
       aaaa.PolicyNumber,
	   aaaa.EffectiveDate,
	   aaaa.TotalUmbrellaPremium,
	   IsNull(aaaa.UnderlyingGLPolicyNumber, case when aaaa.GLPremium &lt;&gt; 0
												  then 'Not Written by Gramercy'
												  else 'No Coverage' end) UnderlyingGLPolicyNumber,
	   aaaa.GLManualPremium,
       aaaa.GLPremium,
	   aaaa.[GL 1M LayerPremium],
	   aaaa.[GL 2M LayerPremium],
	   aaaa.[GL 3M LayerPremium],
	   aaaa.[GL 4M LayerPremium],
	   aaaa.[GL 5M LayerPremium],
	   IsNull(aaaa.UnderlyingAutoPolicyNumber, case when aaaa.AutoPremium &lt;&gt; 0
												  then 'Not Written by Gramercy'
												  else 'No Coverage' end) UnderlyingAutoPolicyNumber,
	   aaaa.ALManualPremium,
       aaaa.AutoPremium ALPremium,
	   aaaa.[AL 1M LayerPremium],
	   aaaa.[AL 2M LayerPremium],
	   aaaa.[AL 3M LayerPremium],
	   aaaa.[AL 4M LayerPremium],
	   aaaa.[AL 5M LayerPremium],
	   IsNull(aaaa.GLJudgementFactor, 0) GLJudgementFactor,
	   IsNull(aaaa.ALJudgementFactor, 0) ALJudgementFactor
from (
select --aaa.NetRate_QuoteID,
       aaa.InsuredName,
       aaa.PolicyNumber,
	   aaa.EffectiveDate,
	   (select sum(iqlltu.TotalPremium)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat] iqll,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat_TaxBa] iqllt,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat_TaxBa_Umbre] iqlltu
		where aaa.NetRate_QuoteID = iql.QuoteID
		  and iql.LocationID = iqll.LocationID
		  and iqll.LocationTotalsID = iqllt.LocationTotalsID
		  and iqllt.TaxBasesID = iqlltu.TaxBasesID) TotalUmbrellaPremium,
	   max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.UnderlyingPolicyNumber end) UnderlyingGLPolicyNumber,
	   convert(money, IsNull(max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.Exposure end), 0)) GLManualPremium,
	   max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.UnderlyingPolicyNumber end) UnderlyingAutoPolicyNumber,
	   convert(money, IsNull(max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.Exposure end), 0)) ALManualPremium,
	   IsNull(max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.Premium end), '') GLPremium,
       IsNull(max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.[1M LayerPremium] end), 0) [GL 1M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.[2M LayerPremium] end), 0) [GL 2M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.[3M LayerPremium] end), 0) [GL 3M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.[4M LayerPremium] end), 0) [GL 4M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'General Liability'
					   then aaa.[5M LayerPremium] end), 0) [GL 5M LayerPremium],
	   IsNull(max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.Premium end), '') AutoPremium,
       IsNull(max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.[1M LayerPremium] end), 0) [AL 1M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.[2M LayerPremium] end), 0) [AL 2M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.[3M LayerPremium] end), 0) [AL 3M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.[4M LayerPremium] end), 0) [AL 4M LayerPremium],
       IsNull(max(case when aaa.LOBCategory = 'Auto Liability'
					   then aaa.[5M LayerPremium] end), 0) [AL 5M LayerPremium],
	   max(aaa.GLJudgementFactor) GLJudgementFactor,
	   max(aaa.ALJudgementFactor) ALJudgementFactor
from (
select  a.QuoteID,
        a.NetRate_QuoteID,
	   (select ti.PolicyName
	    from [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
		     [GramercyRisk].[dbo].[tblInsureds] ti
	    where a.SubmissionGroupGuid = tsg.SubmissionGroupGUID
		  and tsg.InsuredGuid = ti.InsuredGUID) InsuredName,
	   a.RiskDescription ContractorType,
	   a.ProducerName Broker,
	   users.FirstName + ' ' +  users.LastName Underwriter,
	   a.PolicyNumber,
	   'Umbrella' LineOfBusiness,
	   'General Liability' LOBCategory,
	   convert(date, a.EffectiveDate) EffectiveDate,
       'MAN PREMIUM' ExposureBasis,
	   convert(varchar, f.LimitPremium1)  Exposure,
	   f.LimitPremium2 [1M LayerPremium],
	   f.LimitPremium3 [2M LayerPremium],
	   f.LimitPremium4 [3M LayerPremium],
	   f.LimitPremium5 [4M LayerPremium],
	    round(f.LimitPremium1 * f.LayerFactor5, 0) [5M LayerPremium],
	   f.LimitPremium2 + f.LimitPremium3 + f.LimitPremium4 + f.LimitPremium5 +
         round(f.LimitPremium1 * f.LayerFactor5, 0) Premium,
	   a.DisplayStatus RenewalStatus,
	  (select max(tq.PolicyNumber)
	   from [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
			[GramercyRisk].[dbo].[tblQuotes] tq,
			[GramercyRisk].[dbo].[tblCompanyLines] cl,
			[GramercyRisk].[dbo].[lstLines] l
	   where sg.InsuredGuid = tsg.InsuredGUID
	     and tsg.SubmissionGroupGuid = tq.SubmissionGroupGuid
	     and convert(date, tq.ExpirationDate) = convert(date, a.ExpirationDate)
		 and tq.CompanyLineGuid = cl.CompanyLineGUID
		 and cl.LineGUID = l.LineGUID
		 and l.LineName in ('Commercial General Liability', 'Commercial Package')) UnderlyingPolicyNumber,
		e.PremOpsFactor GLJudgementFactor,
		e.PPTLtTruckFactor ALJudgementFactor
from [GramercyRisk].[dbo].[tblQuotes] a,
	 [GramercyRisk].[dbo].[lstPolicyTypes] b,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Liabi] e,
     [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Liabi_Umbre] f,
	 [GramercyRisk].[dbo].[lstLines] i,
	 [GramercyRisk].[dbo].[tblUsers] users,
	 [GramercyRisk].[dbo].[tblSubmissionGroup] sg,
	 [GramercyRisk].[dbo].[tblCompanyLocations] cloc
where a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.PolicyNumber = a.PolicyNumber
/*  MH  2/2/2022 - As per Matt only check if DateBound is present, not prior to ReportAsOfDate
					and convert(date, aa.DateBound) &lt; dateadd(d, 1, @ReportAsOfDate) */
					and convert(date, aa.DateBound) is not NULL
					and aa.EffectiveDate between @PolEffBeginDate and @PolEffEndDate
					and aa.DateCreated &lt;= @ReportAsOfDate
					and aa.EffectiveDate &lt; @ReportAsOfDate)
  and a.ExpirationDate &gt;= case when @InForceOnly = 1
							   then @ReportAsOfDate
							   else a.ExpirationDate end
  and a.PolicyTypeID = b.PolicyTypeID
  and a.NetRate_QuoteID = d.QuoteID
  and d.QuoteID = e.QuoteID
  and e.NodeName = 'LiabilityUmbrella'
  and f.LiabilityUmbrellaID = e.LiabilityLimitsID
  and a.LineGUID = i.LineGUID
  and i.LineName = 'Umbrella'
  and a.UnderwriterUserGuid = users.UserGUID
  and f.LimitPremium2 + f.LimitPremium3 + f.LimitPremium4 + f.LimitPremium5 +
         round(f.LimitPremium1 * f.LayerFactor5, 0) &lt;&gt; 0
  and a.SubmissionGroupGuid = sg.SubmissionGroupGuid
  and a.CompanyLocationGuid = cloc.CompanyLocationGUID
  and cloc.CompanyGUID in (@CompanyGUID)
--  and a.InsuredPolicyName like 'Matc%'


union all



/* Umbrella Auto Liability */

select a.QuoteID,
        a.NetRate_QuoteID,
	   (select ti.PolicyName
	    from [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
		     [GramercyRisk].[dbo].[tblInsureds] ti
	    where a.SubmissionGroupGuid = tsg.SubmissionGroupGUID
		  and tsg.InsuredGuid = ti.InsuredGUID) InsuredName,
	   a.RiskDescription ContractorType,
	   a.ProducerName Broker,
	   users.FirstName + ' ' +  users.LastName Underwriter,
	   a.PolicyNumber,
	   'Umbrella' LineOfBusiness,
	   'Auto Liability' LOBCategory,
	   convert(date, a.EffectiveDate) EffectiveDate,
       'MAN PREMIUM' ExposureBasis,
	   convert(varchar, f.TotalAutoPremium)  Exposure,
	   f.NOAPPTLimitPremium1,
	   f.NOAPPTLimitPremium2,
	   f.NOAPPTLimitPremium3,
	   f.NOAPPTLimitPremium4,
	   f.NOAPPTLimitPremium5,
	   f.NOAPPTLimitPremium1 + f.NOAPPTLimitPremium2 + f.NOAPPTLimitPremium3 + f.NOAPPTLimitPremium4 + f.NOAPPTLimitPremium5 Premium,
	   a.DisplayStatus RenewalStatus,
	  (select max(tq.PolicyNumber)
	   from [GramercyRisk].[dbo].[tblSubmissionGroup] tsg,
			[GramercyRisk].[dbo].[tblQuotes] tq,
			[GramercyRisk].[dbo].[tblCompanyLines] cl,
			[GramercyRisk].[dbo].[lstLines] l
	   where sg.InsuredGuid = tsg.InsuredGUID
	     and tsg.SubmissionGroupGuid = tq.SubmissionGroupGuid
	     and convert(date, tq.ExpirationDate) = convert(date, a.ExpirationDate)
		 and tq.CompanyLineGuid = cl.CompanyLineGUID
		 and cl.LineGUID = l.LineGUID
		 and l.LineName = 'Commercial Auto') UnderlyingPolicyNumber,
		e.PremOpsFactor GLJudgementFactor,
		e.PPTLtTruckFactor ALJudgementFactor
from [GramercyRisk].[dbo].[tblQuotes] a,
	 [GramercyRisk].[dbo].[lstPolicyTypes] b,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Liabi] e,
     [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Liabi_Umbre] f,
	 [GramercyRisk].[dbo].[lstLines] i,
	 [GramercyRisk].[dbo].[tblUsers] users,
	 [GramercyRisk].[dbo].[tblSubmissionGroup] sg,
	 [GramercyRisk].[dbo].[tblCompanyLocations] cloc
where a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.PolicyNumber = a.PolicyNumber
/*  MH  2/2/2022 - As per Matt only check if DateBound is present, not prior to ReportAsOfDate
					and convert(date, aa.DateBound) &lt; dateadd(d, 1, @ReportAsOfDate) */
					and convert(date, aa.DateBound) is not NULL
					and aa.EffectiveDate between @PolEffBeginDate and @PolEffEndDate
					and aa.DateCreated &lt;= @ReportAsOfDate
					and aa.EffectiveDate &lt; @ReportAsOfDate)
  and a.ExpirationDate &gt;= case when @InForceOnly = 1
							   then @ReportAsOfDate
							   else a.ExpirationDate end
  and a.PolicyTypeID = b.PolicyTypeID
  and a.NetRate_QuoteID = d.QuoteID
  and d.QuoteID = e.QuoteID
  and e.NodeName = 'LiabilityUmbrella'
  and f.LiabilityUmbrellaID = e.LiabilityLimitsID
  and a.LineGUID = i.LineGUID
  and i.LineName = 'Umbrella'
  and a.UnderwriterUserGuid = users.UserGUID
  and f.NOAPPTLimitPremium1 + f.NOAPPTLimitPremium2 + f.NOAPPTLimitPremium3 + f.NOAPPTLimitPremium4 + f.NOAPPTLimitPremium5 &lt;&gt; 0
  and a.SubmissionGroupGuid = sg.SubmissionGroupGuid
  and a.CompanyLocationGuid = cloc.CompanyLocationGUID
  and cloc.CompanyGUID in (@CompanyGUID)
--  and a.InsuredPolicyName like 'Matc%'





) aaa
group by aaa.NetRate_QuoteID, 
        aaa.InsuredName,
       aaa.PolicyNumber,
	   aaa.EffectiveDate
) aaaa
--where aaaa.PolicyNumber = 'UMGR1044-02'
order by 1


end</CommandText>
