        </Field>
        <Field Name="TaskType1">
          <DataField>TaskType1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select b.Name_FirstLast TaskUserName,
       b.UserGUID
from GramercyRisk.dbo.tblNoteRecipients a,
     GramercyRisk.dbo.tblUsers b
where a.UserGUID = b.UserGUID
group by b.Name_FirstLast,
       b.UserGUID
order by 1
</CommandText>
      </Query>
      <Fields>
        <Field Name="TaskUserName">
          <DataField>TaskUserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserGUID">
          <DataField>UserGUID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select case when Subject like '%Endorsement%'
			then 'Endorsement'
			else Subject end TaskType
from dbo.tblNoteStore
where Subject not in ('blah blah blah',
					  'TEST 1')
group by case when Subject like '%Endorsement%'
			then 'Endorsement'
			else Subject end
order by count(*) desc</CommandText>
      </Query>
      <Fields>
        <Field Name="TaskType">
          <DataField>TaskType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
