    <DataSource Name="SharedDS">
      <DataSourceReference>/SharedDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>f7bc8e52-773d-48e7-ab0c-f8547e3fff58</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UserGUID">
            <Value>=Parameters!UserGUID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TaskStatus">
            <Value>=Parameters!TaskStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@MinDueDate">
            <Value>=Parameters!MinDueDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TaskType">
            <Value>=Parameters!TaskType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*
 declare @UserGuid				varchar(100)
 declare @TaskStatus			int -- 1 = Open, 2 = Closed and 3 = Both
 declare @MinDueDate			date
 declare @TaskType				varchar(50)

 
 
 set @UserGuid			= '7EBDF713-A473-4CDE-A64E-218C3CA5C15F'
 set @UserGuid			= '52A5EE17-404C-44A4-A6B1-2C3C42C46811'
 set @TaskStatus		= 1
 set @MinDueDate		= '1/1/2022'
 set @TaskType			= 'Endorsement'

*/
 
 declare @ShowStatusReason	int
 set @ShowStatusReason	= 1
 

 select aaa.CreatedDate AssignedDate,
        aaa.DueDate DueDate,
		aaa.CompletedDate DateCompleted,
		aaa.TaskUserName,
		aaa.InsuredPolicyName,
		aaa.ControlNo,
		aaa.Subject TaskType,
		case when aaa.Subject like '%Endorsement%'
			then 'Endorsement'
			else aaa.Subject end TaskType1
from (
 SELECT      DISTINCT TOP 100 PERCENT
                dbo.tblNoteRecipients.EntryGUID,
                dbo.tblNoteEntries.NoteGUID,
                dbo.tblNoteEntries.CreatedDate,
				dbo.tblNoteRecipients.CompletedDate,
                dbo.tblNoteDiaries.DueDate,
                SUBSTRING(dbo.tblNoteEntries.Body, 0, 100)            AS Body,
                dbo.tblNoteStore.Type,
                dbo.tblNoteStore.Subject,
                tq.ControlNo,
                tq.PolicyNumber,
                tq.InsuredPolicyName,
                dbo.tblUsers.LastName + ', ' + dbo.tblUsers.FirstName AS UnderwriterName,
                tq.ProducerLocationName                               AS Producer,
                lnt.Description                                       AS NoteType,
                tq.EffectiveDate,
                ISNULL(tq.StateID, '')                                AS StateID,
                ISNULL(
                    (
                        SELECT  TOP 1
                                lstQuoteStatus.Description
                        FROM
                                GramercyRisk.dbo.tblQuotes q
                            INNER JOIN
                                GramercyRisk.dbo.tblMaxQuoteIDs
                                    ON q.QuoteID = tblMaxQuoteIDs.MaxQuoteID
                            INNER JOIN
                                GramercyRisk.dbo.lstQuoteStatus
                                    ON q.QuoteStatusID = lstQuoteStatus.QuoteStatusID
                        WHERE
                                (q.ControlGuid = tq.ControlGuid)
                    ), ''
                      )                                               
                    +
                    CASE WHEN @ShowStatusReason = 1
                    THEN
                        CASE WHEN ISNULL(tq.QuoteStatusReasonID, 0) = 0 
                        THEN 
                        '' 
                        ELSE 
                        ' - ' + (SELECT 
                                 qsr.Reason 
                                 FROM 
                                 lstQuoteStatusReasons  qsr
                                 WHERE qsr.ID = tq.QuoteStatusReasonID) END 
                    ELSE '' END AS DisplayStatus,
                tblClaims_Claim.ClaimNumber                           AS ClaimNumber,
                lpt.Description                                       AS PolicyType,
                ln.LineName                                           AS LOB,
                qryPrm.Premium,
                tblQuotes2.NeededByDate,
				tq.ExpirationDate,
				 (   SELECT      TOP 1 CompanyName
                                                 FROM      GramercyRisk.dbo.tblQuotes subQ
                                                INNER JOIN GramercyRisk.dbo.tblCompanyLocations cl
                                                   ON subQ.CompanyLocationGuid   = cl.CompanyLocationGUID
                                                INNER JOIN GramercyRisk.dbo.tblCompanies co
                                                   ON cl.CompanyGUID = co.CompanyGUID WHERE subQ.QuoteGUID=tq.QuoteGUID) AS Carrier,
                TaskUsers.Name_FirstLast TaskUserName

    FROM
                GramercyRisk.dbo.tblNoteEntities (NOLOCK)
        RIGHT OUTER JOIN
                GramercyRisk.dbo.tblNoteRecipients (NOLOCK)
            INNER JOIN
                GramercyRisk.dbo.tblNoteEntries (NOLOCK)
                    ON dbo.tblNoteRecipients.EntryGUID = dbo.tblNoteEntries.ID
            INNER JOIN
                GramercyRisk.dbo.tblNoteDiaries (NOLOCK)
                    ON dbo.tblNoteEntries.ID = dbo.tblNoteDiaries.EntryGUID
            INNER JOIN
                GramercyRisk.dbo.tblNoteStore (NOLOCK)
                    ON dbo.tblNoteEntries.NoteGUID = dbo.tblNoteStore.ID
                    ON dbo.tblNoteEntities.NoteGUID = dbo.tblNoteStore.ID
        LEFT OUTER JOIN
                GramercyRisk.dbo.tblQuotes            tq (NOLOCK)
                    ON dbo.tblNoteEntities.ControlGuid = tq.ControlGuid
        LEFT OUTER JOIN
                GramercyRisk.dbo.tblClaims_Claim
                    ON tblClaims_Claim.ClaimGuid = tblNoteEntities.AssociatedEntityGUID
        LEFT OUTER JOIN
                GramercyRisk.dbo.tblUsers
                    ON tq.UnderwriterUserGuid = dbo.tblUsers.UserGUID
        LEFT OUTER JOIN
                GramercyRisk.dbo.tblUsers TaskUsers
                    ON tblNoteRecipients.UserGUID = TaskUsers.UserGUID
        LEFT OUTER JOIN
                GramercyRisk.dbo.lstNoteTypes             lnt
                    ON dbo.tblNoteStore.Type = lnt.NoteTypeID
        LEFT OUTER JOIN
                GramercyRisk.dbo.lstPolicyTypes           lpt
                    ON lpt.PolicyTypeID = tq.PolicyTypeID
        LEFT OUTER JOIN
                GramercyRisk.dbo.lstLines                 ln
                    ON tq.LineGUID = ln.LineGUID
        LEFT OUTER JOIN
                GramercyRisk.dbo.qrySumIMSInvoicedPremium qryPrm
                    ON qryPrm.ControlNo = tq.ControlNo
        LEFT OUTER JOIN
                GramercyRisk.dbo.tblQuotes2
                    ON tblQuotes2.QuoteID = tq.QuoteID
    WHERE
/*                (tblNoteRecipients.CompletedDate IS NULL)
                AND */(tblNoteRecipients.IsDiary = 1)
                AND (tblNoteRecipients.UserGUID in (@UserGUID))
--                AND (tblNoteStore.Type = COALESCE(@NoteStoreType, tblNoteStore.Type))
                AND tblNoteEntities.EntityType &lt;&gt; 'NotARealNamespace.NotARecreatableType.Document'
                AND
                    (
                        tq.QuoteID =
                        (
                            SELECT
                                MAX(tq1.QuoteID)
                            FROM
                                GramercyRisk.dbo.tblQuotes tq1
                            WHERE
                                tq.ControlGuid = tq1.ControlGuid
                        )
                        OR tq.QuoteID IS NULL
                    )
) aaa
where IsNull(aaa.CompletedDate, '1/1/2000') = case when @TaskStatus = 1
											  then '1/1/2000'
							                  when @TaskStatus = 2
											  then aaa.CompletedDate
											  else IsNull(aaa.CompletedDate, '1/1/2000') end
  and convert(date, aaa.DueDate) &gt;= @MinDueDate
  and case when aaa.Subject like '%Endorsement%'
			then 'Endorsement'
			else aaa.Subject end in (@TaskType)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
