    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select ProducerGUID,
       ProducerName
from tblProducers
order by 2</CommandText>
      </Query>
      <Fields>
        <Field Name="ProducerGUID">
          <DataField>ProducerGUID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="ProducerName">
          <DataField>ProducerName</DataField>
