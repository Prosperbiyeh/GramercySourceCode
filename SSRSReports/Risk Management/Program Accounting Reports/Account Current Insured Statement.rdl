    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ProducerGUID">
            <Value>=Parameters!ProducerGUID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select d.InsuredID,
       d.Name InsuredName
from tblProducerLocations a,
     tblSubmissionGroup b,
	 tblQuotes c,
	 tblInsureds d
where a.ProducerGUID in (@ProducerGUID)
  and a.ProducerLocationGUID = b.ProducerLocationGuid
  and b.SubmissionGroupGUID = c.SubmissionGroupGuid
  and c.PolicyNumber is not NULL
  and b.InsuredGuid = d.InsuredGUID
group by d.InsuredID,
       d.Name
order by 2</CommandText>
      </Query>
      <Fields>
        <Field Name="InsuredID">
          <DataField>InsuredID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="InsuredName">
          <DataField>InsuredName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet4">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InsuredID">
            <Value>=Parameters!InsuredID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select c.PolicyNumber
from tblInsureds a,
     tblSubmissionGroup b,
     tblQuotes c
where a.InsuredID in (@InsuredID)
  and a.InsuredGUID = b.InsuredGuid
  and b.SubmissionGroupGUID = c.SubmissionGroupGuid
  and c.PolicyNumber is not NULL
group by c.PolicyNumber
order by right(c.PolicyNumber, 2) desc, c.PolicyNumber</CommandText>
      </Query>
      <Fields>
        <Field Name="PolicyNumber">
          <DataField>PolicyNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
