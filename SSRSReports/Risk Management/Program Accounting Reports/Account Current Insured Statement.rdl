    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PolicyNumber">
            <Value>=Parameters!PolicyNumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @PolicyNumber	varchar(max)

set @PolicyNumber = 'BAGR3994-00'

*/

--  select datepart(dd, @TransactionDateTo)



/*

declare @TreatyName	varchar(200);

select @TreatyName = a.TreatyName
from ReportReference.CompanyReinsTreaties a
where a.CompanyReinsTreatyID = @CompanyReinsTreatyID
*/

   DECLARE @ValidInvoicesTables TABLE(InvoiceNum INT)

	INSERT INTO @ValidInvoicesTables
	SELECT DISTINCT tblFin_Invoices.InvoiceNum
	FROM		    tblQuotes
	INNER JOIN	    tblCompanyLocations ON tblQuotes.CompanyLocationGuid = tblCompanyLocations.CompanyLocationGuid
	INNER JOIN      tblCompanies ON tblCompanyLocations.CompanyGuid = tblCompanies.CompanyGuid
	INNER JOIN	    tblFin_Invoices ON tblQuotes.QuoteID = tblFin_Invoices.QuoteID
	INNER JOIN	    tblSubmissionGroup ON tblQuotes.SubmissionGroupGuid = tblSubmissionGroup.SubmissionGroupGuid
	INNER JOIN	    tblInsureds ON tblSubmissionGroup.InsuredGuid = tblInsureds.InsuredGuid
	INNER JOIN      ReportReference.CompanyReinsTreaties ON tblCompanies.CompanyID = ReportReference.CompanyReinsTreaties.CompanyID
	WHERE           tblFin_Invoices.Failed = 0
	AND             tblFin_Invoices.InvoiceTypeID &lt;&gt; 'PB'
	AND			    tblInsureds.TestInsured = 0
	AND             tblQuotes.PolicyNumber in (@PolicyNumber)
 /*   AND             ReportReference.CompanyReinsTreaties.CompanyReinsTreatyID = @CompanyReinsTreatyID
	AND             convert(date, tblQuotes.EffectiveDate) between ReportReference.CompanyReinsTreaties.TreatyBeginDate and ReportReference.CompanyReinsTreaties.TreatyEndDate
AND             CASE WHEN (dbo.IsDownPaymentInvoice(tblFin_Invoices.InvoiceNum) = 1) OR (dbo.IsAnnualPayPolicy(tblFin_Invoices.InvoiceNum) = 1)
                     THEN CASE WHEN (DATEDIFF(d, @TransactionDateFrom, tblFin_Invoices.PostDate) &gt;= 0) AND (DATEDIFF(d, @TransactionDateTo, tblFin_Invoices.PostDate) &lt;= 0)
					           THEN 1
							   ELSE 0
						  END
					 ELSE CASE WHEN (DATEDIFF(d, @TransactionDateFrom, tblFin_Invoices.DueDate) &gt;= 0) AND (DATEDIFF(d, @TransactionDateTo, tblFin_Invoices.DueDate) &lt;= 0)
					           THEN 1
							   ELSE 0
						  END
				END = 1
    AND				tblFin_Invoices.OfficeInvoiceNum = 13912 */




select aaa.Policy_Number,
       aaa.Insured_Name,
       max(aaa.[Due Date]) [Due Date],
	   case when aaa.TransactionType = 'Premium' and sum(aaa.[Gross Premium]) &gt;= 0
	        then 'Additional Premium'
			when aaa.TransactionType = 'Premium'
	        then 'Return Premium'
			else aaa.TransactionType end TransactionType,
--	   aaa.OfficeInvoiceNum,
	   sum(aaa.[Gross Premium]) [Gross Premium],
	   sum(aaa.[Tax/Fees]) [Tax/Fees],
	   abs(sum(aaa.[Amt Received])) [Amt Received],
	   sum(aaa.[Gross Premium]) +
	   sum(aaa.[Tax/Fees]) -
	   sum(aaa.[Amt Received]) [Amount Due]
from (
SELECT          tblCompanyLocations.Name AS CarrierName,
                tblCompanyLocations.Address1 as CarrierAddress1, 
				tblCompanyLocations.Address2 as CarrierAddress2, 
				tblCompanyLocations.City + ', ' + tblCompanyLocations.State + ' ' + tblCompanyLocations.ZipCode AS CarrierCSZ,
				tblCompanyLocations.LocationCode AS AccountNumber,
                tblInsureds.Name AS Insured_Name,
				tblQuotes.PolicyNumber as Policy_Number,
				tblFin_Invoices.OfficeInvoiceNum,
				CASE WHEN tblQuotes.EndorsementEffective IS NULL
				     THEN tblQuotes.EffectiveDate
					 ELSE tblQuotes.EndorsementEffective
			    END AS TransEffDate,
				--Case When tblQuotes.QuoteStatusID = 12 then 'Cancellation'
		  --           when  tblQuotes.originalQuoteGUID is Null then lstPolicyTypes.Description
		  --           when  tblQuotes.originalQuoteGUID is Not Null and tblQuotes.QuoteStatusID &lt;&gt; 12 and lstTransactionTypes.ID = 'E' Then 'Endorsement'
		  --           when  tblQuotes.originalQuoteGUID is Not Null and tblQuotes.QuoteStatusID &lt;&gt; 12 and lstTransactionTypes.ID = 'R' Then 'Reinstatement'
		  --           when  tblQuotes.originalQuoteGUID is Not Null and tblQuotes.QuoteStatusID &lt;&gt; 12 and lstTransactionTypes.ID = 'A' Then 'Audit'
		  --           when  tblQuotes.originalQuoteGUID is Not Null and tblQuotes.QuoteStatusID &lt;&gt; 12 and lstTransactionTypes.ID = 'N' Then 'Correction'
	   --              Else  'Endorsement'
	   --         End as TransactionType,
	            CASE dbo.Gramercy_GetInstallmentNum(tblFin_Invoices.InvoiceNum)
                     WHEN -1 THEN 'Premium'
                     WHEN  0 THEN 'Downpayment'
                     ELSE 'Installment # ' + CONVERT(VARCHAR, dbo.Gramercy_GetInstallmentNum(tblFin_Invoices.InvoiceNum))
                END AS TransactionType,
				tblQuotes.EndorsementNum AS [Endorsement Number],
				tblQuotes.InsuredState AS [Insured State],
				tblQuotes.EffectiveDate AS [Effective Date],
				tblQuotes.ExpirationDate AS [Expiration Date],
				tblFin_Invoices.PostDate AS [Trans Date],
				tblFin_Invoices.DueDate AS [Due Date],
				ISNULL((SELECT     SUM(INVD.AmtBilled)
				        FROM       tblFin_InvoiceDetails INVD
				        INNER JOIN tblFin_PolicyCharges PC ON INVD.ChargeCode = PC.ChargeCode
				        WHERE      INVD.InvoiceNum = tblFin_Invoices.InvoiceNum
			            AND        PC.ChargeType = 'P'), 0) AS [Gross Premium],
				(SELECT TOP 1 QD.CompanyCommission
                 FROM         tblQuoteDetails QD
                 WHERE        QD.QuoteGuid = tblQuotes.QuoteGUID) AS [Comm Rate],
				CASE WHEN tblFin_Invoices.Remitter = 'I'
				     THEN ISNULL((SELECT     SUM(isnull(invd.MGAAmt, 0))
						          FROM       tblFin_InvoiceDetails INVD
						          WHERE      INVD.InvoiceNum = tblFin_Invoices.InvoiceNum
						          AND        INVD.ChargeType = 'P'), 0) +
						  ISNULL((SELECT     SUM(isnull(IIP.PayeeAmt, 0))
						          FROM       tblFin_InvoicedItemsPayees IIP
								  INNER JOIN viewEntityTypes VP ON IIP.PayeeGuid = VP.EntityGuid
						          WHERE      IIP.InvoiceNum = tblFin_Invoices.InvoiceNum
						          AND        VP.EntityType IN ('B', 'P')), 0) 
					 ELSE ISNULL((SELECT     SUM(isnull(invd.MGAAmt, 0) + isnull(invd.RemitterAmt, 0))
						          FROM       tblFin_InvoiceDetails INVD
						          WHERE      INVD.InvoiceNum = tblFin_Invoices.InvoiceNum
						          AND        INVD.ChargeType = 'P'), 0)
				END AS [Commission Amount],
				ISNULL((SELECT     SUM(IP.PayeeAmt)
				        FROM       tblFin_InvoicedItemsPayees IP
						INNER JOIN tblFin_InvoiceDetails ID ON IP.InvoiceNum = ID.InvoiceNum AND IP.ChargeCode = ID.ChargeCode AND IP.CompanyLineGuid = ID.CompanyLineGuid
						INNER JOIN tblFin_PolicyCharges PC on ID.ChargeCode = PC.ChargeCode
						INNER JOIN tblCompanyLines CL ON IP.CompanyLineGuid = CL.CompanyLineGuid
						INNER JOIN viewEntityTypes VE ON IP.PayeeGUID = VE.EntityGuid
						WHERE      ID.InvoiceNum = tblFin_Invoices.InvoiceNum
						AND        PC.ChargeType = 'F'
						AND        VE.EntityType IN ('C', 'CL', 'CO', 'CG')), 0) AS [Tax/Fees],
				--ISNULL((SELECT     SUM(IP.PayeeAmt)
				--        FROM       tblFin_InvoicedItemsPayees IP
				--		INNER JOIN tblFin_InvoiceDetails ID ON IP.InvoiceNum = ID.InvoiceNum AND IP.ChargeCode = ID.ChargeCode AND IP.CompanyLineGuid = ID.CompanyLineGuid
				--		INNER JOIN tblFin_PolicyCharges PC on ID.ChargeCode = PC.ChargeCode
				--		INNER JOIN tblCompanyLines CL ON IP.CompanyLineGuid = CL.CompanyLineGuid
				--		INNER JOIN viewEntityTypes VE ON IP.PayeeGUID = VE.EntityGuid
				--		WHERE      ID.InvoiceNum = tblFin_Invoices.InvoiceNum
				--		AND        ID.ChargeName &lt;&gt; 'Terrorism'
				--		AND        VE.EntityType IN ('C', 'CL', 'CO', 'CG')), 0) +
				--ISNULL((SELECT     SUM(INVD.AmtBilled)
				--        FROM       tblFin_InvoiceDetails INVD
				--        INNER JOIN tblFin_PolicyCharges PC ON INVD.ChargeCode = PC.ChargeCode
				--        WHERE      INVD.InvoiceNum = tblFin_Invoices.InvoiceNum
			 --           AND        PC.ChargeType = 'P'
				--		AND        INVD.ChargeName = 'Terrorism'), 0) AS [Net Premium],
				ISNULL((SELECT     SUM(JP.Amount * -1)
				        FROM       tblFin_JournalPostings JP
				        INNER JOIN tblFin_Journal J ON JP.TransactNum = J.TransactNum
				        INNER JOIN tblFin_GlAccountTypes GLT ON JP.GLAcctID = GLT.GLAcctID
						INNER JOIN tblFin_PolicyCharges PC ON JP.ChargeCode = PC.ChargeCode
						INNER JOIN viewEntityTypes VE ON JP.EntityGuid = VE.EntityGuid
				        WHERE      JP.InvoiceNum = tblFin_Invoices.InvoiceNum
						AND        J.TransDescID = 'I'
				        AND        GLT.AccountType = 'P'
						AND        VE.EntityType IN ('C', 'CL', 'CO', 'CG')), 0) AS [Net Premium],
				ISNULL((SELECT     SUM(JP.Amount * -1)
				        FROM       tblFin_JournalPostings JP
				        INNER JOIN tblFin_Journal J ON JP.TransactNum = J.TransactNum
				        INNER JOIN tblFin_GlAccountTypes GLT ON JP.GLAcctID = GLT.GLAcctID
						INNER JOIN tblFin_PolicyCharges PC ON JP.ChargeCode = PC.ChargeCode
						INNER JOIN viewEntityTypes VE ON JP.EntityGuid = VE.EntityGuid
				        WHERE      JP.InvoiceNum = tblFin_Invoices.InvoiceNum
						AND        J.TransDescID &lt;&gt; 'I'
				        AND        GLT.AccountType = 'R'), 0) AS [Amt Received],
				lstLines.LineName AS [LOB]
FROM            tblFin_Invoices
INNER JOIN      tblQuotes ON tblFin_Invoices.QuoteID = tblQuotes.QuoteID
INNER JOIN	    tblCompanyLocations ON tblQuotes.CompanyLocationGuid = tblCompanyLocations.CompanyLocationGuid
INNER JOIN      tblCompanies ON tblCompanyLocations.CompanyGUID = tblCompanies.CompanyGUID
INNER JOIN	    tblSubmissionGroup ON tblQuotes.SubmissionGroupGuid = tblSubmissionGroup.SubmissionGroupGuid
INNER JOIN	    tblInsureds ON tblSubmissionGroup.InsuredGuid = tblInsureds.InsuredGuid
LEFT OUTER JOIN lstLines ON tblQuotes.LineGUID = lstLines.LineGUID
LEFT OUTER JOIN lstTransactionTypes on lstTransactionTypes.ID = tblQuotes.TransactionTypeID
LEFT OUTER JOIN lstPolicyTypes ON lstPolicyTypes.PolicyTypeID = tblQuotes.PolicyTypeID
LEFT OUTER JOIN lstBillingTypes ON tblQuotes.BillingTypeID = lstBillingTypes.BillingTypeID
LEFT OUTER JOIN tblQuotes2 ON tblQuotes.QuoteID = tblQuotes2.QuoteID
WHERE           tblFin_Invoices.InvoiceNum IN (SELECT * FROM @ValidInvoicesTables)
/*
ORDER BY        tblInsureds.Name,
                tblCompanyLocations.Name,
				tblQuotes.PolicyNumber,
				tblFin_Invoices.InvoiceNum
*/

) aaa /*
where aaa.TransactionType like 'Installment%'
   or aaa.TransactionType = 'Downpayment' */
group by aaa.Policy_Number,
       aaa.Insured_Name,
	   aaa.TransactionType
--order by [Due Date]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
