    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select CompanyGUID,
       CompanyName
from [dbo].[tblCompanies]
where CompanyName &lt;&gt; 'Placeholder Company'
group by CompanyGUID,
       CompanyName
order by 2</CommandText>
      </Query>
