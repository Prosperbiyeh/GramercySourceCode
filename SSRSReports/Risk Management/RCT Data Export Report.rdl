<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SharedDS">
      <DataSourceReference>/SharedDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>a306c32f-b357-44c6-b86e-a1a3c2be3139</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportAsOfDate">
            <Value>=Parameters!ReportAsOfDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @PolEffBeginDate		date;
declare @PolEffEndDate			date;
/*
declare @ReportAsOfDate			date;
*/
declare @InForceOnly			int;


set @PolEffBeginDate	= '1/1/2018'
set @PolEffEndDate		= GetDate()

/*
set @ReportAsOfDate     = GetDate()
*/
set @InForceOnly		= 1


select aaaa.* /*,
      (select pc.FName + ' ' + pc.LName
	   from [GramercyRisk].[dbo].[tblProducerContacts] pc
	   where pc.ProducerContactGUID = aaaa.ProducerContactGuid) ProducerContactName,
      (select pc.Email
	   from [GramercyRisk].[dbo].[tblProducerContacts] pc
	   where pc.ProducerContactGUID = aaaa.ProducerContactGuid) ProducerContactEmail,
      (select pc.Phone
	   from [GramercyRisk].[dbo].[tblProducerContacts] pc
	   where pc.ProducerContactGUID = aaaa.ProducerContactGuid) ProducerContactPhone */
from (
select --aaa.QuoteID,
       --aaa.NetRate_QuoteID,
     --max(aaa.InsuredPolicyName) InsuredPolicyName,
	   aaa.InsuredId,
       aaa.InsuredPolicyName,
	   il.Address1,
	   il.Address2,
	   il.City,
	   il.State,
	   il.ZipCode,
	   max(aaa.PolicyNumber) PolicyNumber,
	   min(aaa.EffectiveDate) EffectiveDate,
	   aaa.ExpirationDate,
--  To get RN when a NB policy is written for a RN account
	   max(aaa.Type) PolicyType,
--	   max(aaa.RiskDescription) ContractorType,
/*	  (select max(tq.RiskDescription)
	   from [dbo].[tblQuotes] tq
	   where aaa.InsuredPolicyName = tq.InsuredPolicyName
	     and tq.QuoteID = (select max(tq2.QuoteID)
						   from [dbo].[tblQuotes] tq2
						   where tq2.InsuredPolicyName = tq.InsuredPolicyName
             and tq2.RiskDescription is not NULL)) ContractorType, */
	   aaa.ContractorType,
	   case when aaa.ProducerName = 'AssuredPartners Northeast, LLC'
			then 'AssuredPartners Northeast, LLC (Melville)'
			when aaa.ProducerName in ('Cook Maran', 'Cook Maran &amp; Associates, Inc.')
			then 'EPIC'
			when aaa.ProducerName in ('Cool Insuring Agency, Inc.', 'Cool - a Gallager Company')
			then 'Cool – a Gallagher Company'
			else aaa.ProducerName end Broker,
	   max(aaa.Underwriter) Underwriter,
	   max(aaa.UnderwriterEmail) UnderwriterEmail,
	   sum(TotalPremium) TotalPremium,
/* GL */
	   sum(GLPremium) GLPremium,
       sum(GLPerOccLimit) GLPerOccLimit,
       sum(TotalGLPayroll) TotalGLPayroll,
       sum(TotalSubCosts) TotalSubCosts,
/* Property */
	   sum(PropertyPremium)PropertyPremium,
	   sum(PropertyLimit)PropertyLimit,
/* IM */
	   sum(IMPremium) IMPremium,
	   sum(IMLimit + IMEDPLimit) TotalIMLimit,
/* Crime */
	   sum(CrimePremium) CrimePremium,
	   sum(CrimeLimit) CrimeLimit,
/* Auto */
	   sum(AutoPremium) AutoPremium,
	   sum(VehicleCount) VehicleCount,
/* Umbrella */
	   sum(UmbrellaPremium) UmbrellaPremium,       
	   sum(UmbrellaLimit) UmbrellaLimit,   
/* EBL */		    
	   case when sum(EBLIndicator) &lt;&gt; 0 then 'X' end EBLIndicator,       
	   max(EBLRetroDate) EBLRetroDate,
/* Terrorism */
       case when sum(TerorismIndicator) &lt;&gt; 0 then 'X' end TerorismIndicator,
	   sum(TRIAPremium) TRIAPremium,
/* Composite Rate */
       NULL CompositeRateIndicator,
	   NULL CompositeRate,
/* Fees */
	  sum(NYFireFee) NYFireFee,
	  sum(MVRFee) MVRFee,
	  min(aaa.InsuredCounty) InsuredCounty,
	  max(aaa.InsuredCounty) InsuredCounty2, 
	  (select max(e.FName + ' ' + e.LName)
	   from [GramercyRisk].[dbo].[tblInsuredLocations] il
	   left join [GramercyRisk].[dbo].[tblInsuredContacts] e
	     on il.InsuredLocationGUID = e.InsuredLocationGUID
		and StatusID = 1
	   left join [GramercyRisk].[dbo].[tblInsuredSpecialContacts] sc
		   on e.InsuredContactGUID = sc.InsuredContactGUID
		 left join [lstInsuredSpecialContactTypes] ct
		   on sc.SpecialContactTypeID = ct.SpecialContactTypeID
	   where aaa.InsuredGUID = il.InsuredGUID
	     and ct.SpecialContactType in ('Inspection Contact')) InspectionContactName, 
	  (select max(e.Email)
	   from [GramercyRisk].[dbo].[tblInsuredLocations] il
	   left join [GramercyRisk].[dbo].[tblInsuredContacts] e
	     on il.InsuredLocationGUID = e.InsuredLocationGUID
		and StatusID = 1
	   left join [GramercyRisk].[dbo].[tblInsuredSpecialContacts] sc
		   on e.InsuredContactGUID = sc.InsuredContactGUID
		 left join [lstInsuredSpecialContactTypes] ct
		   on sc.SpecialContactTypeID = ct.SpecialContactTypeID
	   where aaa.InsuredGUID = il.InsuredGUID
	     and ct.SpecialContactType in ('Inspection Contact')) InspectionContactEmail, 
	  (select max(e.Phone)
	   from [GramercyRisk].[dbo].[tblInsuredLocations] il
	   left join [GramercyRisk].[dbo].[tblInsuredContacts] e
	     on il.InsuredLocationGUID = e.InsuredLocationGUID
		and StatusID = 1
	   left join [GramercyRisk].[dbo].[tblInsuredSpecialContacts] sc
		   on e.InsuredContactGUID = sc.InsuredContactGUID
		 left join [lstInsuredSpecialContactTypes] ct
		   on sc.SpecialContactTypeID = ct.SpecialContactTypeID
	   where aaa.InsuredGUID = il.InsuredGUID
	     and ct.SpecialContactType in ('Inspection Contact')) InspectionContactPhone,
	  max(aaa.ProducerContactGuid) ProducerContactGuid,
	  max(case when aaa.LineName in ('Commercial Package', 'Commercial General Liability') then aaa.ProducerContactName end) ProducerContactName,
	  max(case when aaa.LineName in ('Commercial Package', 'Commercial General Liability') then aaa.ProducerContactEmail end) ProducerContactEmail,
	  max(case when aaa.LineName in ('Commercial Package', 'Commercial General Liability') then aaa.ProducerContactPhone end) ProducerContactPhone
from (
select -- a.QuoteID,
        a.ProducerContactGUID,
       (select iq.InsuredGUID
	    from [GramercyRisk].[dbo].ivQuotes iq
	    where a.QuoteGUID = iq.QuoteGUID) InsuredGUID,
       (select ti.InsuredID
	    from [GramercyRisk].[dbo].ivQuotes iq,
		     [GramercyRisk].[dbo].tblInsureds ti
	    where a.QuoteGUID = iq.QuoteGUID
		  and iq.InsuredGUID = ti.InsuredGUID) InsuredId,   
       max(case when c.LineName in ('Commercial Package', 'Commercial General Liability')
	        then a.PolicyNumber end) PolicyNumber,
       (select ti.PolicyName
	    from [GramercyRisk].[dbo].ivQuotes iq,
		     [GramercyRisk].[dbo].tblInsureds ti
	    where a.QuoteGUID = iq.QuoteGUID
		  and iq.InsuredGUID = ti.InsuredGUID) InsuredPolicyName,
       (select max(tq2.RiskDescription)
	    from [GramercyRisk].[dbo].tblSubmissionGroup sg1,
			 [GramercyRisk].[dbo].tblSubmissionGroup sg2,
			 [GramercyRisk].[dbo].tblQuotes tq2
		where a.SubmissionGroupGuid = sg1.SubmissionGroupGuid
		  and sg1.InsuredGuid = sg2.InsuredGuid
		  and sg2.SubmissionGroupGUID = tq2.SubmissionGroupGuid
		  and tq2.QuoteID = (select max(tq3.QuoteID)
							 from [GramercyRisk].[dbo].tblQuotes tq3
							 where tq3.SubmissionGroupGUID = tq2.SubmissionGroupGuid
							   and tq3.RiskDescription is not NULL)) ContractorType,
	   convert(date, a.EffectiveDate) EffectiveDate,
	   convert(date, a.ExpirationDate) ExpirationDate,
	   b.Type,
	   c.LineName,
	   a.RiskDescription,
	   a.ProducerName,
	  (select users.Name_FirstLast
	   from dbo.tblUsers users
	   where a.UnderwriterUserGuid = users.UserGUID
	     and c.LineName in ('Commercial Package', 'Commercial General Liability')) Underwriter,
	  (select users.EmailAddress
	   from dbo.tblUsers users
	   where a.UnderwriterUserGuid = users.UserGUID
	     and c.LineName in ('Commercial Package', 'Commercial General Liability')) UnderwriterEmail,
	   a.QuoteID,
	   d.QuoteID NetRate_QuoteID,
	   d.PolicyTotalsID,
	   sum(d.TotalPremium - d.SurchargePremium) TotalPremium,
/* GL */
	   (select sum(qpp.LiabilityTotal) + sum(qpp.Terrorism)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID
		  and qpp.NodeName = 'PolicyLiability') + IsNull(sum(d.AdditionalInsureds), 0) GLPremium,
       (select convert(int, replace(qgl.EachOccurrence, ',', ''))
		from [dbo].[NetRate_Quote_Insur_Quote_Liabi] qgl
		where d.QuoteID = qgl.QuoteID
		  and qgl.NodeName = 'LiabilityLimits')  GLPerOccLimit,
       (select sum(convert(int, replace(qll.Exposure, ',', '')))
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
			 [dbo].NetRate_Quote_Insur_Quote_Locat_Liabi qll
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = qll.LocationID
		  and qll.ExposureBasis = 'payroll'
		  and qll.Exposure &lt;&gt; 'if any'
		  and qll.Exposure &lt;&gt; 'if an')  TotalGLPayroll,
       (select sum(convert(int, replace(qll.Exposure, ',', '')))
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
			 [dbo].NetRate_Quote_Insur_Quote_Locat_Liabi qll
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = qll.LocationID
		  and qll.ExposureBasis = 'cost'
		  and qll.Exposure &lt;&gt; 'if any'
--		  and qll.Exposure &lt;&gt; 'if an'
			) TotalSubCosts,
/* Property */
	   (select sum(qpp.PropertyTotal) + sum(qpp.Terrorism)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID
		  and qpp.NodeName = 'PolicyProperty') PropertyPremium,
	   (select IsNull(sum(convert(int, replace(locp.BuildingLimit, ',', ''))), 0) +
	           IsNull(sum(convert(int, replace(locp.PropertyExposure, ',', ''))), 0) +
	           IsNull(sum(convert(int, replace(locp.BusinessIncomeExposure, ',', ''))), 0)
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Premi] locp
		where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locp.LocationID) PropertyLimit,
/* IM */
	   (select sum(locl.InlandMarine)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat] locl
		where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locl.LocationID) IMPremium,
	   (select sum(IsNull(convert(int, replace(replace(locig.MiscItemsLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.InstallBasicLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.OwnedLeasedEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.RentalReimLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.RentedEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.ToolsLimit, '$', ''), ',', '')), 0) )
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan] loci,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan_Grame] locig
	    where d.QuoteID = loc.QuoteID
		  and loc.LocationID = loci.LocationID
		  and loci.InlandMarineID = locig.InlandMarineID) IMLimit,
	   (select sum(IsNull(convert(int, replace(replace(locie.EDPDataMediaLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locie.EDPElecEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locie.EDPExtraExpenseLimit, '$', ''), ',', '')), 0) )
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan] loci,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan_EDP] locie
	    where d.QuoteID = loc.QuoteID
		  and loc.LocationID = loci.LocationID
		  and loci.InlandMarineID = locie.InlandMarineID) IMEDPLimit,
/* Crime */
	   (select sum(locl.CrimeTotal)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat] locl
		where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locl.LocationID) CrimePremium,
	   (select sum(Limit)
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Crime] locc,
			 [dbo].[gramercyCrimeLimit_vw] gcl
	    where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locc.LocationID
		  and locc.CrimeID = gcl.CrimeID) CrimeLimit,
/* Auto */
	   (select sum(qpb.Total)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Busin] qpb
		where d.PolicyTotalsID = qpb.PolicyTotalsID) AutoPremium,
	   (select count(iqlbv.UnitNumber)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Busin] iqlb,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Busin_Vehic] iqlbv
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = iqlb.LocationID
		  and iqlb.BusinessAutoID = iqlbv.BusinessAutoID
		  and IsNull(iqlbv.[Transaction], '') &lt;&gt; 'Deleted') VehicleCount,
/* Umbrella */
	   (select sum(iqlltu.TotalPremium)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat] iqll,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat_TaxBa] iqllt,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat_TaxBa_Umbre] iqlltu
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = iqll.LocationID
		  and iqll.LocationTotalsID = iqllt.LocationTotalsID
		  and iqllt.TaxBasesID = iqlltu.TaxBasesID) UmbrellaPremium,       
	   (select sum(convert(int, replace(iql.ExcessLimit, ',', '')))
		from [dbo].[NetRate_Quote_Insur_Quote_Liabi] iql
		where d.QuoteID = iql.QuoteID) UmbrellaLimit,   
/* EBL */		    
	   (select count(nrad.RetroDate)
		from [dbo].[tblNetRateAdditionalData] nrad
		where a.QuoteGUID = nrad.QuoteGUID) EBLIndicator,       
	   (select min(nrad.RetroDate)
		from [dbo].[tblNetRateAdditionalData] nrad
		where a.QuoteGUID = nrad.QuoteGUID) EBLRetroDate,
/* Terrorism */
       (select count(case when IsNull(qpp.Terrorism, 0) &lt;&gt; 0 then qpp.Terrorism end)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID) TerorismIndicator,
	   (select sum(qpp.Terrorism)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID) TRIAPremium,
/* Composite Rate */
       NULL CompositeRateIndicator,
	   NULL CompositeRate,
/* Fees */
	  (select sum(qoc.amount)
	   from [dbo].[tblQuoteOptions] qo,
	        [dbo].[tblQuoteOptionCharges] qoc,
			[dbo].[tblFin_PolicyCharges] fpc
       where a.QuoteGUID = qo.QuoteGUID
	     and qo.QuoteOptionGuid = qoc.QuoteOptionGuid
		 and qoc.ChargeCode = fpc.ChargeCode
		 and fpc.ChargeName = 'NYFF') NYFireFee,
	  (select sum(qoc.amount)
	   from [dbo].[tblQuoteOptions] qo,
	        [dbo].[tblQuoteOptionCharges] qoc,
			[dbo].[tblFin_PolicyCharges] fpc
       where a.QuoteGUID = qo.QuoteGUID
	     and qo.QuoteOptionGuid = qoc.QuoteOptionGuid
		 and qoc.ChargeCode = fpc.ChargeCode
		 and fpc.ChargeName = 'NY Motor Vehicle Fee') MVRFee,
	   a.InsuredCounty,
	   g.FName + ' ' + g.LName ProducerContactName,
	   g.Email ProducerContactEmail,
	   g.Phone ProducerContactPhone
from [dbo].[tblQuotes] a,
	 [dbo].[lstPolicyTypes] b,
	 [dbo].[lstLines] c,
	 [dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [dbo].[tblCompanyLines] cl,
	 [dbo].[tblCompanyLocations] cloc,
	 [dbo].[tblProducerContacts] g
where a.PolicyNumber is not NULL
  and a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.PolicyNumber = a.PolicyNumber
/*  MH  2/2/2022 - As per Matt only check if DateBound is present, not prior to ReportAsOfDate
					and convert(date, aa.DateBound) &lt; dateadd(d, 1, @ReportAsOfDate) */
					and convert(date, aa.DateBound) is not NULL
					and convert(date, aa.EffectiveDate) between @PolEffBeginDate and @PolEffEndDate
					and convert(date, aa.DateCreated) &lt;= @ReportAsOfDate
					and convert(date, aa.EffectiveDate) &lt;= @ReportAsOfDate)
  and convert(date, a.ExpirationDate) &gt; case when @InForceOnly = 1
										     then @ReportAsOfDate
											 else dateadd(d, -1, convert(date, a.ExpirationDate)) end
  and a.PolicyTypeID = b.PolicyTypeID
  and a.LineGUID = c.LineGUID
  and a.NetRate_QuoteID = d.QuoteID 
  and a.CompanyLineGuid = cl.CompanyLineGUID
  and cl.CompanyLocationGuid = cloc.CompanyLocationGUID
  and a.ProducerContactGuid = g.ProducerContactGuid
--  and g.StatusID = 1
--  and cloc.CompanyGUID in (@CompanyGuid)
--  and a.PolicyNumber = 'BAGR1545-00'
--  and a.InsuredPolicyName like 'Pro Metal of NY Corp'
group by -- a.QuoteID,
       a.SubmissionGroupGuid,
       a.InsuredPolicyName,
	   convert(date, a.EffectiveDate),
	   convert(date, a.ExpirationDate),
	   b.Type,
	   case when c.LineName in ('Commercial Package', 'Commercial General Liability')
	        then a.PolicyNumber end,
	   c.LineName,
	   a.RiskDescription,
	   a.ProducerName,
	   a.QuoteID,
	   a.QuoteGUID,
	   a.UnderwriterUserGuid,
	   a.InsuredCounty,
	   d.QuoteID,
	   d.PolicyTotalsID,
	   a.ProducerContactGuid,
	   g.FName + ' ' + g.LName ,
	   g.Email,
	   g.Phone
) aaa,
     [dbo].[tblInsuredLocations] il
where il.InsuredGUID = aaa.InsuredGUID
  and il.Hidden = 0
  and il.LocationTypeID = (select min(il2.LocationTypeID)
						   from [dbo].[tblInsuredLocations] il2
						   where il.InsuredGUID = il2.InsuredGUID
							 and il2.Hidden = 0)
--where aaa.InsuredId in (121, 126)
group by --aaa.QuoteID,
       --aaa.NetRate_QuoteID, 
	   aaa.InsuredId,
	   aaa.InsuredPolicyName,
--	   aaa.EffectiveDate,
	   aaa.ExpirationDate,
--	   aaa.Type,
--	   aaa.RiskDescription,
	   case when aaa.ProducerName = 'AssuredPartners Northeast, LLC'
			then 'AssuredPartners Northeast, LLC (Melville)'
			when aaa.ProducerName in ('Cook Maran', 'Cook Maran &amp; Associates, Inc.')
			then 'EPIC'
			when aaa.ProducerName in ('Cool Insuring Agency, Inc.', 'Cool - a Gallager Company')
			then 'Cool – a Gallagher Company'
			else aaa.ProducerName end,
	   aaa.ContractorType,
	   aaa.InsuredGUID,
	   il.Address1,
	   il.Address2,
	   il.City,
	   il.State,
	   il.ZipCode
--	   aaa.Underwriter
--	   aaa.InsuredCounty
) aaaa


/*  Old 7/20/2022

declare @PolEffBeginDate		date;
declare @PolEffEndDate			date;
/*
declare @ReportAsOfDate			date;
*/
declare @InForceOnly			int;


set @PolEffBeginDate	= '1/1/2018'
set @PolEffEndDate		= GetDate()

/*
set @ReportAsOfDate     = GetDate()
*/
set @InForceOnly		= 1



select aaaa.*,
      (select pc.FName + ' ' + pc.LName
	   from [GramercyRisk].[dbo].[tblProducerContacts] pc
	   where pc.ProducerContactGUID = aaaa.ProducerContactGuid) ProducerContactName,
      (select pc.Email
	   from [GramercyRisk].[dbo].[tblProducerContacts] pc
	   where pc.ProducerContactGUID = aaaa.ProducerContactGuid) ProducerContactEmail,
      (select pc.Phone
	   from [GramercyRisk].[dbo].[tblProducerContacts] pc
	   where pc.ProducerContactGUID = aaaa.ProducerContactGuid) ProducerContactPhone
from (
select --aaa.QuoteID,
       --aaa.NetRate_QuoteID,
     --max(aaa.InsuredPolicyName) InsuredPolicyName,
	   aaa.InsuredId,
       aaa.InsuredPolicyName,	
	   max(aaa.PolicyNumber) PolicyNumber,
	   min(aaa.EffectiveDate) EffectiveDate,
	   aaa.ExpirationDate,
--  To get RN when a NB policy is written for a RN account
	   max(aaa.Type) PolicyType,
--	   max(aaa.RiskDescription) ContractorType,
/*	  (select max(tq.RiskDescription)
	   from [dbo].[tblQuotes] tq
	   where aaa.InsuredPolicyName = tq.InsuredPolicyName
	     and tq.QuoteID = (select max(tq2.QuoteID)
						   from [dbo].[tblQuotes] tq2
						   where tq2.InsuredPolicyName = tq.InsuredPolicyName
             and tq2.RiskDescription is not NULL)) ContractorType, */
	   aaa.ContractorType,
	   case when aaa.ProducerName = 'AssuredPartners Northeast, LLC'
			then 'AssuredPartners Northeast, LLC (Melville)'
			when aaa.ProducerName in ('Cook Maran', 'Cook Maran &amp; Associates, Inc.')
			then 'EPIC'
			when aaa.ProducerName in ('Cool Insuring Agency, Inc.', 'Cool - a Gallager Company')
			then 'Cool – a Gallagher Company'
			else aaa.ProducerName end Broker,
	   max(aaa.Underwriter) Underwriter,
	   max(aaa.UnderwriterEmail) UnderwriterEmail,
	   sum(TotalPremium) TotalPremium,
/* GL */
	   sum(GLPremium) GLPremium,
       sum(GLPerOccLimit) GLPerOccLimit,
       sum(TotalGLPayroll) TotalGLPayroll,
       sum(TotalSubCosts) TotalSubCosts,
/* Property */
	   sum(PropertyPremium)PropertyPremium,
	   sum(PropertyLimit)PropertyLimit,
/* IM */
	   sum(IMPremium) IMPremium,
	   sum(IMLimit + IMEDPLimit) TotalIMLimit,
/* Crime */
	   sum(CrimePremium) CrimePremium,
	   sum(CrimeLimit) CrimeLimit,
/* Auto */
	   sum(AutoPremium) AutoPremium,
	   sum(VehicleCount) VehicleCount,
/* Umbrella */
	   sum(UmbrellaPremium) UmbrellaPremium,       
	   sum(UmbrellaLimit) UmbrellaLimit,   
/* EBL */		    
	   case when sum(EBLIndicator) &lt;&gt; 0 then 'X' end EBLIndicator,       
	   max(EBLRetroDate) EBLRetroDate,
/* Terrorism */
       case when sum(TerorismIndicator) &lt;&gt; 0 then 'X' end TerorismIndicator,
	   sum(TRIAPremium) TRIAPremium,
/* Composite Rate */
       NULL CompositeRateIndicator,
	   NULL CompositeRate,
/* Fees */
	  sum(NYFireFee) NYFireFee,
	  sum(MVRFee) MVRFee,
	  min(aaa.InsuredCounty) InsuredCounty,
	  max(aaa.InsuredCounty) InsuredCounty2, 
	  (select max(e.FName + ' ' + e.LName)
	   from [GramercyRisk].[dbo].[tblInsuredLocations] il
	   left join [GramercyRisk].[dbo].[tblInsuredContacts] e
	     on il.InsuredLocationGUID = e.InsuredLocationGUID
		and StatusID = 1
	   left join [GramercyRisk].[dbo].[tblInsuredSpecialContacts] sc
		   on e.InsuredContactGUID = sc.InsuredContactGUID
		 left join [lstInsuredSpecialContactTypes] ct
		   on sc.SpecialContactTypeID = ct.SpecialContactTypeID
	   where aaa.InsuredGUID = il.InsuredGUID
	     and ct.SpecialContactType in ('Inspection Contact')) InspectionContactName, 
	  (select max(e.Email)
	   from [GramercyRisk].[dbo].[tblInsuredLocations] il
	   left join [GramercyRisk].[dbo].[tblInsuredContacts] e
	     on il.InsuredLocationGUID = e.InsuredLocationGUID
		and StatusID = 1
	   left join [GramercyRisk].[dbo].[tblInsuredSpecialContacts] sc
		   on e.InsuredContactGUID = sc.InsuredContactGUID
		 left join [lstInsuredSpecialContactTypes] ct
		   on sc.SpecialContactTypeID = ct.SpecialContactTypeID
	   where aaa.InsuredGUID = il.InsuredGUID
	     and ct.SpecialContactType in ('Inspection Contact')) InspectionContactEmail, 
	  (select max(e.Phone)
	   from [GramercyRisk].[dbo].[tblInsuredLocations] il
	   left join [GramercyRisk].[dbo].[tblInsuredContacts] e
	     on il.InsuredLocationGUID = e.InsuredLocationGUID
		and StatusID = 1
	   left join [GramercyRisk].[dbo].[tblInsuredSpecialContacts] sc
		   on e.InsuredContactGUID = sc.InsuredContactGUID
		 left join [lstInsuredSpecialContactTypes] ct
		   on sc.SpecialContactTypeID = ct.SpecialContactTypeID
	   where aaa.InsuredGUID = il.InsuredGUID
	     and ct.SpecialContactType in ('Inspection Contact')) InspectionContactPhone,
	  max(aaa.ProducerContactGuid) ProducerContactGuid
from (
select -- a.QuoteID,
        a.ProducerContactGUID,
       (select iq.InsuredGUID
	    from [GramercyRisk].[dbo].ivQuotes iq
	    where a.QuoteGUID = iq.QuoteGUID) InsuredGUID,
       (select ti.InsuredID
	    from [GramercyRisk].[dbo].ivQuotes iq,
		     [GramercyRisk].[dbo].tblInsureds ti
	    where a.QuoteGUID = iq.QuoteGUID
		  and iq.InsuredGUID = ti.InsuredGUID) InsuredId,   
       max(case when c.LineName in ('Commercial Package', 'Commercial General Liability')
	        then a.PolicyNumber end) PolicyNumber,
       (select ti.PolicyName
	    from [GramercyRisk].[dbo].ivQuotes iq,
		     [GramercyRisk].[dbo].tblInsureds ti
	    where a.QuoteGUID = iq.QuoteGUID
		  and iq.InsuredGUID = ti.InsuredGUID) InsuredPolicyName,
       (select max(tq2.RiskDescription)
	    from [GramercyRisk].[dbo].tblSubmissionGroup sg1,
			 [GramercyRisk].[dbo].tblSubmissionGroup sg2,
			 [GramercyRisk].[dbo].tblQuotes tq2
		where a.SubmissionGroupGuid = sg1.SubmissionGroupGuid
		  and sg1.InsuredGuid = sg2.InsuredGuid
		  and sg2.SubmissionGroupGUID = tq2.SubmissionGroupGuid
		  and tq2.QuoteID = (select max(tq3.QuoteID)
							 from [GramercyRisk].[dbo].tblQuotes tq3
							 where tq3.SubmissionGroupGUID = tq2.SubmissionGroupGuid
							   and tq3.RiskDescription is not NULL)) ContractorType,
	   convert(date, a.EffectiveDate) EffectiveDate,
	   convert(date, a.ExpirationDate) ExpirationDate,
	   b.Type,
	   c.LineName,
	   a.RiskDescription,
	   a.ProducerName,
	  (select users.Name_FirstLast
	   from dbo.tblUsers users
	   where a.UnderwriterUserGuid = users.UserGUID
	     and c.LineName in ('Commercial Package', 'Commercial General Liability')) Underwriter,
	  (select users.EmailAddress
	   from dbo.tblUsers users
	   where a.UnderwriterUserGuid = users.UserGUID
	     and c.LineName in ('Commercial Package', 'Commercial General Liability')) UnderwriterEmail,
	   a.QuoteID,
	   d.QuoteID NetRate_QuoteID,
	   d.PolicyTotalsID,
	   sum(d.TotalPremium - d.SurchargePremium) TotalPremium,
/* GL */
	   (select sum(qpp.LiabilityTotal) + sum(qpp.Terrorism)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID
		  and qpp.NodeName = 'PolicyLiability') + IsNull(sum(d.AdditionalInsureds), 0) GLPremium,
       (select convert(int, replace(qgl.EachOccurrence, ',', ''))
		from [dbo].[NetRate_Quote_Insur_Quote_Liabi] qgl
		where d.QuoteID = qgl.QuoteID
		  and qgl.NodeName = 'LiabilityLimits')  GLPerOccLimit,
       (select sum(convert(int, replace(qll.Exposure, ',', '')))
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
			 [dbo].NetRate_Quote_Insur_Quote_Locat_Liabi qll
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = qll.LocationID
		  and qll.ExposureBasis = 'payroll'
		  and qll.Exposure &lt;&gt; 'if any'
		  and qll.Exposure &lt;&gt; 'if an')  TotalGLPayroll,
       (select sum(convert(int, replace(qll.Exposure, ',', '')))
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
			 [dbo].NetRate_Quote_Insur_Quote_Locat_Liabi qll
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = qll.LocationID
		  and qll.ExposureBasis = 'cost'
		  and qll.Exposure &lt;&gt; 'if any'
--		  and qll.Exposure &lt;&gt; 'if an'
			) TotalSubCosts,
/* Property */
	   (select sum(qpp.PropertyTotal) + sum(qpp.Terrorism)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID
		  and qpp.NodeName = 'PolicyProperty') PropertyPremium,
	   (select IsNull(sum(convert(int, replace(locp.BuildingLimit, ',', ''))), 0) +
	           IsNull(sum(convert(int, replace(locp.PropertyExposure, ',', ''))), 0) +
	           IsNull(sum(convert(int, replace(locp.BusinessIncomeExposure, ',', ''))), 0)
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Premi] locp
		where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locp.LocationID) PropertyLimit,
/* IM */
	   (select sum(locl.InlandMarine)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat] locl
		where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locl.LocationID) IMPremium,
	   (select sum(IsNull(convert(int, replace(replace(locig.MiscItemsLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.InstallBasicLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.OwnedLeasedEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.RentalReimLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.RentedEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locig.ToolsLimit, '$', ''), ',', '')), 0) )
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan] loci,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan_Grame] locig
	    where d.QuoteID = loc.QuoteID
		  and loc.LocationID = loci.LocationID
		  and loci.InlandMarineID = locig.InlandMarineID) IMLimit,
	   (select sum(IsNull(convert(int, replace(replace(locie.EDPDataMediaLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locie.EDPElecEquipLimit, '$', ''), ',', '')), 0)
	             + IsNull(convert(int, replace(replace(locie.EDPExtraExpenseLimit, '$', ''), ',', '')), 0) )
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan] loci,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Inlan_EDP] locie
	    where d.QuoteID = loc.QuoteID
		  and loc.LocationID = loci.LocationID
		  and loci.InlandMarineID = locie.InlandMarineID) IMEDPLimit,
/* Crime */
	   (select sum(locl.CrimeTotal)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat] locl
		where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locl.LocationID) CrimePremium,
	   (select sum(Limit)
	    from [dbo].[NetRate_Quote_Insur_Quote_Locat] loc,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Crime] locc,
			 [dbo].[gramercyCrimeLimit_vw] gcl
	    where d.QuoteID = loc.QuoteID
		  and loc.LocationID = locc.LocationID
		  and locc.CrimeID = gcl.CrimeID) CrimeLimit,
/* Auto */
	   (select sum(qpb.Total)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Busin] qpb
		where d.PolicyTotalsID = qpb.PolicyTotalsID) AutoPremium,
	   (select count(iqlbv.UnitNumber)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Busin] iqlb,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Busin_Vehic] iqlbv
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = iqlb.LocationID
		  and iqlb.BusinessAutoID = iqlbv.BusinessAutoID
		  and IsNull(iqlbv.[Transaction], '') &lt;&gt; 'Deleted') VehicleCount,
/* Umbrella */
	   (select sum(iqlltu.TotalPremium)
		from [dbo].[NetRate_Quote_Insur_Quote_Locat] iql,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat] iqll,
		     [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat_TaxBa] iqllt,
			 [dbo].[NetRate_Quote_Insur_Quote_Locat_Locat_TaxBa_Umbre] iqlltu
		where d.QuoteID = iql.QuoteID
		  and iql.LocationID = iqll.LocationID
		  and iqll.LocationTotalsID = iqllt.LocationTotalsID
		  and iqllt.TaxBasesID = iqlltu.TaxBasesID) UmbrellaPremium,       
	   (select sum(convert(int, replace(iql.ExcessLimit, ',', '')))
		from [dbo].[NetRate_Quote_Insur_Quote_Liabi] iql
		where d.QuoteID = iql.QuoteID) UmbrellaLimit,   
/* EBL */		    
	   (select count(nrad.RetroDate)
		from [dbo].[tblNetRateAdditionalData] nrad
		where a.QuoteGUID = nrad.QuoteGUID) EBLIndicator,       
	   (select min(nrad.RetroDate)
		from [dbo].[tblNetRateAdditionalData] nrad
		where a.QuoteGUID = nrad.QuoteGUID) EBLRetroDate,
/* Terrorism */
       (select count(case when IsNull(qpp.Terrorism, 0) &lt;&gt; 0 then qpp.Terrorism end)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID) TerorismIndicator,
	   (select sum(qpp.Terrorism)
		from [dbo].[NetRate_Quote_Insur_Quote_Polic_Polic] qpp
		where d.PolicyTotalsID = qpp.PolicyTotalsID) TRIAPremium,
/* Composite Rate */
       NULL CompositeRateIndicator,
	   NULL CompositeRate,
/* Fees */
	  (select sum(qoc.amount)
	   from [dbo].[tblQuoteOptions] qo,
	        [dbo].[tblQuoteOptionCharges] qoc,
			[dbo].[tblFin_PolicyCharges] fpc
       where a.QuoteGUID = qo.QuoteGUID
	     and qo.QuoteOptionGuid = qoc.QuoteOptionGuid
		 and qoc.ChargeCode = fpc.ChargeCode
		 and fpc.ChargeName = 'NYFF') NYFireFee,
	  (select sum(qoc.amount)
	   from [dbo].[tblQuoteOptions] qo,
	        [dbo].[tblQuoteOptionCharges] qoc,
			[dbo].[tblFin_PolicyCharges] fpc
       where a.QuoteGUID = qo.QuoteGUID
	     and qo.QuoteOptionGuid = qoc.QuoteOptionGuid
		 and qoc.ChargeCode = fpc.ChargeCode
		 and fpc.ChargeName = 'NY Motor Vehicle Fee') MVRFee,
	   a.InsuredCounty
from [dbo].[tblQuotes] a,
	 [dbo].[lstPolicyTypes] b,
	 [dbo].[lstLines] c,
	 [dbo].[NetRate_Quote_Insur_Quote_Polic] d,
	 [dbo].[tblCompanyLines] cl,
	 [dbo].[tblCompanyLocations] cloc
where a.PolicyNumber is not NULL
  and a.QuoteID = (select max(aa.QuoteID)
					from [GramercyRisk].[dbo].[tblQuotes] aa
					where aa.PolicyNumber = a.PolicyNumber
/*  MH  2/2/2022 - As per Matt only check if DateBound is present, not prior to ReportAsOfDate
					and convert(date, aa.DateBound) &lt; dateadd(d, 1, @ReportAsOfDate) */
					and convert(date, aa.DateBound) is not NULL
					and convert(date, aa.EffectiveDate) between @PolEffBeginDate and @PolEffEndDate
					and convert(date, aa.DateCreated) &lt;= @ReportAsOfDate
					and convert(date, aa.EffectiveDate) &lt;= @ReportAsOfDate)
  and convert(date, a.ExpirationDate) &gt; case when @InForceOnly = 1
										     then @ReportAsOfDate
											 else dateadd(d, -1, convert(date, a.ExpirationDate)) end
  and a.PolicyTypeID = b.PolicyTypeID
  and a.LineGUID = c.LineGUID
  and a.NetRate_QuoteID = d.QuoteID 
  and a.CompanyLineGuid = cl.CompanyLineGUID
  and cl.CompanyLocationGuid = cloc.CompanyLocationGUID
--  and cloc.CompanyGUID in (@CompanyGuid)
--  and a.PolicyNumber = 'BAGR1545-00'
--  and a.InsuredPolicyName like 'Pro Metal of NY Corp'
group by -- a.QuoteID,
       a.SubmissionGroupGuid,
       a.InsuredPolicyName,
	   convert(date, a.EffectiveDate),
	   convert(date, a.ExpirationDate),
	   b.Type,
	   case when c.LineName in ('Commercial Package', 'Commercial General Liability')
	        then a.PolicyNumber end,
	   c.LineName,
	   a.RiskDescription,
	   a.ProducerName,
	   a.QuoteID,
	   a.QuoteGUID,
	   a.UnderwriterUserGuid,
	   a.InsuredCounty,
	   d.QuoteID,
	   d.PolicyTotalsID,
	   a.ProducerContactGuid
) aaa
group by --aaa.QuoteID,
       --aaa.NetRate_QuoteID, 
	   aaa.InsuredId,
	   aaa.InsuredPolicyName,
--	   aaa.EffectiveDate,
	   aaa.ExpirationDate,
--	   aaa.Type,
--	   aaa.RiskDescription,
	   case when aaa.ProducerName = 'AssuredPartners Northeast, LLC'
			then 'AssuredPartners Northeast, LLC (Melville)'
			when aaa.ProducerName in ('Cook Maran', 'Cook Maran &amp; Associates, Inc.')
			then 'EPIC'
			when aaa.ProducerName in ('Cool Insuring Agency, Inc.', 'Cool - a Gallager Company')
			then 'Cool – a Gallagher Company'
			else aaa.ProducerName end,
	   aaa.ContractorType,
	   aaa.InsuredGUID
--	   aaa.Underwriter
--	   aaa.InsuredCounty
) aaaa

*/</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
