  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select a.InsuredPolicyName,
       a.PolicyNumber,
	   a.EffectiveDate,
	   a.ExpirationDate/*,
	  (select sum(d.TotalPremium - d.SurchargePremium)
	   from [dbo].[NetRate_Quote_Insur_Quote_Polic] d
	   where a.NetRate_QuoteID = d.QuoteID) GWP */
from [dbo].[tblQuotes] a,
     [dbo].[tblCompanyLines] b,
     [dbo].[lstLines] c
where a.PolicyNumber is not NULL
  and a.ExpirationDate &lt; GetDate()
  and a.QuoteID = (select max(aa.QuoteID)
				   from [dbo].[tblQuotes] aa
				   where a.PolicyNumber = aa.PolicyNumber
				     and a.TransactionTypeID &lt;&gt; 'A'
				     and IsNull(aa.DateIssued, aa.DateBound) is not NULL)
  and a.CompanyLineGuid = b.CompanyLineGUID
  and b.LineGUID = c.LineGUID
  and c.LineName in ('Commercial Package', 'Commercial General Liability')
  and not exists (select 1
				  from [dbo].[tblQuotes] aa
				  where a.PolicyNumber = aa.PolicyNumber
				     and a.TransactionTypeID = 'A'
				     and IsNull(aa.DateIssued, aa.DateBound) is not NULL)
--  and a.InsuredPolicyName = 'A T S Mechanical Contractors Inc.'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
