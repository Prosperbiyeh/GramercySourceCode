    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BudgetPlanID">
            <Value>=Parameters!BudgetPlanID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CompanyGuid">
            <Value>=Parameters!CompanyGuid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BudgetYear">
            <Value>=Parameters!BudgetYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PolicyType">
            <Value>=Parameters!PolicyType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @BudgetPlanID		int
declare @PolicyType			varchar(30)
declare @CompanyGuid		varchar(100)
declare @BudgetYear			int

set @BudgetPlanID = 60
set @PolicyType = 'New Business'
set @CompanyGuid = (select CompanyGUID
					from tblCompanies
					where CompanyName like 'Tri%')

set @BudgetYear = 2020

*/

declare @BudgetYYYYMM		char(6)

set @BudgetYYYYMM = (select YYYYMM
					 from [GramercyRisk].[ReportReference].[BudgetPlan]
					 where @BudgetPlanID = BudgetPlanID)

select max(aaaa.YYYYMM) YYYYMM,
       aaaa.BudgetYear,
	   aaaa.BudgetMonth,
	   aaaa.BudgetMonthName,
--	   aaaa.PolicyType,
	   aaaa.TransactionType,
	   sum(aaaa.GWP) GWP,
	   sum(aaaa.PYGWP) PYGWP,
	   IsNull((select sum(case when 'New Business' in (@PolicyType) and 'Renewal' in (@PolicyType)
                then NBPrem + RenPrem
                when 'New Business' in (@PolicyType)
                then NBPrem
				when 'Renewal' in (@PolicyType)
						then RenPrem end) ReportBudgetTotalGWP
		from [ReportReference].[BudgetPlan] a
		where a.BudgetYear = @BudgetYear
		  and a.YYYYMM = max(aaaa.YYYYMM)), 0) MonthPlanGWP
from (
select aaa.YYYYMM,
	   b.BudgetYear,
	   b.BudgetMonth,
       b.BudgetMonthName,
       aaa.PolicyType,
       case when aaa.TransactionType = 'Correction'
	        then 'Endorsement'
			else aaa.TransactionType end TransactionType,
       sum(aaa.GWP) GWP,
	   0 PYGWP
from (
select a.QuoteID,
       c.CompanyName,
/*  MH  3/4/2022 - Fixing code below to use the existance of an Expiring Policy Number
	   case when substring(a.PolicyNumber, len(a.PolicyNumber), 1) = 0
	        then 'New Business'
			else 'Renewal'
			end PolicyType, */
	   case when IsNull(a.ExpiringPolicyNumber, '') = ''
	        then 'New Business'
			else 'Renewal' end PolicyType,
/* MH  8/24/2021 - If a Correction has a preceding Audit, then it is an Audit Transaction)
      IsNull((select case when a.DisplayStatus = 'Cancelled'
						  then 'Cancellation'
						  when a.EndorsementComment like '%Commission%'
						  then 'Endorsement'
						  else  e.TransactionType end 
			  from [GramercyRisk].[dbo].[lstTransactionTypes] e
			  where a.TransactionTypeID = e.ID), 'Bind') TransactionType, */
      IsNull((select case when a.TransactionTypeID = 'N'
	                       and exists (select 1
									   from [GramercyRisk].[dbo].[tblQuotes] aa
									   where a.PolicyNumber = aa.PolicyNumber
									     and aa.QuoteID &lt; a.QuoteID
										 and aa.TransactionTypeID = 'A')
						  then 'Audit'
						  when a.DisplayStatus = 'Cancelled'
						  then 'Cancellation'
						  when a.EndorsementComment like '%Commission%'
						  then 'Endorsement'
						  else  e.TransactionType end 
			  from [GramercyRisk].[dbo].[lstTransactionTypes] e
			  where a.TransactionTypeID = e.ID), 'Bind') TransactionType,
       convert(varchar(6), 
       CASE WHEN a.DateBound &gt;= IsNull(a.EndorsementEffective, a.EffectiveDate) AND a.DateBound &gt;= a.EffectiveDate 
	        THEN a.DateBound
            WHEN IsNull(a.EndorsementEffective, a.EffectiveDate) &gt;= a.DateBound AND IsNull(a.EndorsementEffective, a.EffectiveDate) &gt;= a.EffectiveDate 
			THEN IsNull(a.EndorsementEffective, a.EffectiveDate)
            WHEN a.EffectiveDate &gt;= a.DateBound AND a.EffectiveDate &gt;= IsNull(a.EndorsementEffective, a.EffectiveDate) 
			THEN a.EffectiveDate
            ELSE a.DateBound end, 112) YYYYMM,
	   a.DateBound,
	   a.EndorsementEffective,
	   a.EffectiveDate,
       a.DisplayStatus,
      (select max(f.AnnualPremium)
	   from [GramercyRisk].[dbo].[tblFin_Invoices] f
	   where a.QuoteID = f.QuoteID
             and f.Failed = 0) GWP
from [GramercyRisk].[dbo].[tblQuotes] a,
     [GramercyRisk].[dbo].[tblCompanyLocations] b,
	 [GramercyRisk].[dbo].[tblCompanies] c,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d
	 
where a.PolicyNumber is not NULL /*
  and a.DateIssued &lt; dateadd(d, 1, @ReportAsOfDate)
  and a.EffectiveDate &lt; dateadd(d, 1, @ReportAsOfDate)
  and a.EndorsementEffective &lt; dateadd(d, 1, @ReportAsOfDate)
  and a.ExpirationDate &gt;= @ReportAsOfDate */
  and a.DateBound is not NULL
  and a.CompanyLocationGuid = b.CompanyLocationGUID
  and b.CompanyGUID = c.CompanyGUID
  and a.NetRate_QuoteID = d.QuoteID 
  and exists (select 1
			  from [GramercyRisk].[dbo].[tblFin_Invoices] f
			  where a.QuoteID = f.QuoteID)
  and c.CompanyGUID in (@CompanyGuid)
--  and a.TransactionTypeID = 'N'

) aaa
  left join [GramercyRisk].[ReportReference].[BudgetPlan] b
    on aaa.YYYYMM = b.YYYYMM
where aaa.YYYYMM is not null
--  and aaa.YYYYMM = b.YYYYMM
  and b.BudgetYear = @BudgetYear
  and aaa.YYYYMM &lt;= @BudgetYYYYMM
group by aaa.YYYYMM,
	   b.BudgetYear,
	   b.BudgetMonth,
       b.BudgetMonthName,
       aaa.PolicyType,
       case when aaa.TransactionType = 'Correction'
	        then 'Endorsement'
			else aaa.TransactionType end,
       case when aaa.PolicyType = 'New Business'
	        then b.NBPrem
			else b.RenPrem end
/*
) aaaa */



union all

select aaa.YYYYMM,
	   b.BudgetYear,
	   b.BudgetMonth,
       b.BudgetMonthName,
       aaa.PolicyType,
       case when aaa.TransactionType = 'Correction'
	        then 'Endorsement'
			else aaa.TransactionType end TransactionType,
       0 GWP,
	   sum(aaa.GWP) PYGWP
from (
select a.QuoteID,
       c.CompanyName,
/*  MH  3/4/2022 - Fixing code below to use the existance of an Expiring Policy Number
	   case when substring(a.PolicyNumber, len(a.PolicyNumber), 1) = 0
	        then 'New Business'
			else 'Renewal'
			end PolicyType, */
	   case when IsNull(a.ExpiringPolicyNumber, '') = ''
	        then 'New Business'
			else 'Renewal' end PolicyType,
/* MH  8/24/2021 - If a Correction has a preceding Audit, then it is an Audit Transaction)
      IsNull((select case when a.DisplayStatus = 'Cancelled'
						  then 'Cancellation'
						  when a.EndorsementComment like '%Commission%'
						  then 'Endorsement'
						  else  e.TransactionType end 
			  from [GramercyRisk].[dbo].[lstTransactionTypes] e
			  where a.TransactionTypeID = e.ID), 'Bind') TransactionType, */
      IsNull((select case when a.TransactionTypeID = 'N'
	                       and exists (select 1
									   from [GramercyRisk].[dbo].[tblQuotes] aa
									   where a.PolicyNumber = aa.PolicyNumber
									     and aa.QuoteID &lt; a.QuoteID
										 and aa.TransactionTypeID = 'A')
						  then 'Audit'
						  when a.DisplayStatus = 'Cancelled'
						  then 'Cancellation'
						  when a.EndorsementComment like '%Commission%'
						  then 'Endorsement'
						  else  e.TransactionType end 
			  from [GramercyRisk].[dbo].[lstTransactionTypes] e
			  where a.TransactionTypeID = e.ID), 'Bind') TransactionType,
       convert(varchar(6), 
       CASE WHEN a.DateBound &gt;= IsNull(a.EndorsementEffective, a.EffectiveDate) AND a.DateBound &gt;= a.EffectiveDate 
	        THEN a.DateBound
            WHEN IsNull(a.EndorsementEffective, a.EffectiveDate) &gt;= a.DateBound AND IsNull(a.EndorsementEffective, a.EffectiveDate) &gt;= a.EffectiveDate 
			THEN IsNull(a.EndorsementEffective, a.EffectiveDate)
            WHEN a.EffectiveDate &gt;= a.DateBound AND a.EffectiveDate &gt;= IsNull(a.EndorsementEffective, a.EffectiveDate) 
			THEN a.EffectiveDate
            ELSE a.DateBound end, 112) YYYYMM,
	   a.DateBound,
	   a.EndorsementEffective,
	   a.EffectiveDate,
       a.DisplayStatus,
      (select max(f.AnnualPremium)
	   from [GramercyRisk].[dbo].[tblFin_Invoices] f
	   where a.QuoteID = f.QuoteID
             and f.Failed = 0) GWP
from [GramercyRisk].[dbo].[tblQuotes] a,
     [GramercyRisk].[dbo].[tblCompanyLocations] b,
	 [GramercyRisk].[dbo].[tblCompanies] c,
	 [GramercyRisk].[dbo].[NetRate_Quote_Insur_Quote_Polic] d
	 
where a.PolicyNumber is not NULL /*
  and a.DateIssued &lt; dateadd(d, 1, @ReportAsOfDate)
  and a.EffectiveDate &lt; dateadd(d, 1, @ReportAsOfDate)
  and a.EndorsementEffective &lt; dateadd(d, 1, @ReportAsOfDate)
  and a.ExpirationDate &gt;= @ReportAsOfDate */
  and a.DateBound is not NULL
  and a.CompanyLocationGuid = b.CompanyLocationGUID
  and b.CompanyGUID = c.CompanyGUID
  and a.NetRate_QuoteID = d.QuoteID 
  and exists (select 1
			  from [GramercyRisk].[dbo].[tblFin_Invoices] f
			  where a.QuoteID = f.QuoteID)
  and c.CompanyGUID in (@CompanyGuid)
--  and a.TransactionTypeID = 'N'

) aaa
  left join [GramercyRisk].[ReportReference].[BudgetPlan] b
    on aaa.YYYYMM = b.YYYYMM - 100
where aaa.YYYYMM is not null
--  and aaa.YYYYMM = b.YYYYMM - 100
  and b.BudgetYear = @BudgetYear
  and aaa.YYYYMM &lt;= @BudgetYYYYMM - 100
group by aaa.YYYYMM,
	   b.BudgetYear,
	   b.BudgetMonth,
       b.BudgetMonthName,
       aaa.PolicyType,
       case when aaa.TransactionType = 'Correction'
	        then 'Endorsement'
			else aaa.TransactionType end,
       case when aaa.PolicyType = 'New Business'
	        then b.NBPrem
			else b.RenPrem end

) aaaa
where aaaa.PolicyType in (@PolicyType)
group by aaaa.BudgetYear,
       aaaa.BudgetMonth,
	   aaaa.BudgetMonthName,
--	   aaaa.PolicyType,
	   aaaa.TransactionType</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
