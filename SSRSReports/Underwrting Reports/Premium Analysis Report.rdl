    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select CompanyGUID,
       CompanyName
from [GramercyRisk].[dbo].[tblCompanies]
where CompanyName &lt;&gt; 'Placeholder Company'
order by 2</CommandText>
      </Query>
      <Fields>
        <Field Name="CompanyGUID">
          <DataField>CompanyGUID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BudgetYear">
            <Value>=Parameters!BudgetYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select BudgetPlanID,
       BudgetMonthName,
       LastDay 
from [GramercyRisk].[ReportReference].[BudgetPlan]
where BudgetYear = @BudgetYear
  and LastDay &lt; dateadd(m, 1, GetDate())
order by LastDay</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BudgetPlanID">
          <DataField>BudgetPlanID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BudgetMonthName">
          <DataField>BudgetMonthName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastDay">
          <DataField>LastDay</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet4">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>select BudgetYear
from [GramercyRisk].[ReportReference].[BudgetPlan]
group by BudgetYear
union
select 2020
union
select 2019
union
select 2018
order by 1 desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="BudgetYear">
          <DataField>BudgetYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet5">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PolicyType">
            <Value>=Parameters!PolicyType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BudgetYear">
            <Value>=Parameters!BudgetYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BudgetPlanID">
            <Value>=Parameters!BudgetPlanID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select sum(case when 'New Business' in (@PolicyType) and 'Renewal' in (@PolicyType)
                then NBPrem + RenPrem
                when 'New Business' in (@PolicyType)
                then NBPrem
		when 'Renewal' in (@PolicyType)
                then RenPrem end) ReportBudgetTotalGWP
from [ReportReference].[BudgetPlan] a
where BudgetYear = @BudgetYear
  and BudgetPlanID &lt; = @BudgetPlanID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ReportBudgetTotalGWP">
          <DataField>ReportBudgetTotalGWP</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet6">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@BudgetYear">
            <Value>=Parameters!BudgetYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select BudgetPlanID
from [GramercyRisk].[ReportReference].[BudgetPlan]
where BudgetYear = @BudgetYear
  and BudgetMonthName = DATENAME(m, GetDate())
order by LastDay</CommandText>
      </Query>
      <Fields>
        <Field Name="BudgetPlanID">
          <DataField>BudgetPlanID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
