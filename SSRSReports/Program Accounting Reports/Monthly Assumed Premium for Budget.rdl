  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportYear">
            <Value>=Parameters!ReportYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AsOfDate">
            <Value>=Parameters!AsOfDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @ReportYear		char(4)
declare @AsOfDate		date

set @ReportYear		= '2022'
set @AsOfDate		= GetDate()

*/

select left(YYYYMM, 4) Year,
       datename(month, convert(date, YYYYMM + '01')) Month,
	   convert(char(2), right(YYYYMM, 2)) MonthNum,
	   LineName LOB,
	   sum([PeriodAssumedEarnedPremium]) [PeriodAssumedEarnedPremium]
from [Sandbox].[dbo].[PremiumByYYYYMMFuture]
where left(YYYYMM, 4) in (@ReportYear)
  and [TransEffDate] &lt; dateadd(d, 1, @AsOfDate)
group by left(YYYYMM, 4),
     datename(month, convert(date, YYYYMM + '01')),
	   convert(char(2), right(YYYYMM, 2)),
	   LineName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthNum">
          <DataField>MonthNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LOB">
          <DataField>LOB</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PeriodAssumedEarnedPremium">
          <DataField>PeriodAssumedEarnedPremium</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <CommandText>select left(YYYYMM, 4) Year
from [Sandbox].[dbo].[PremiumByYYYYMMFuture]
where YYYYMM &gt; '201812'
group by left(YYYYMM, 4)
order by 1 desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
