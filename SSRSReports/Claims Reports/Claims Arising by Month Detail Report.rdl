<?xml version="1.0" encoding="utf-8"?>
<Report MustUnderstand="df" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:df="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition/defaultfontfamily">
  <df:DefaultFontFamily>Segoe UI</df:DefaultFontFamily>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SharedDS">
      <DataSourceReference>/SharedDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>cfed413d-1c57-4177-a5c6-7a5f63972a4a</rd:DataSourceID>
    </DataSource>
    <DataSource Name="SharedLocalDS">
      <DataSourceReference>/SharedLocalDS</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>1eed025a-0692-48c6-a9a0-0939247e24e6</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RptMonth">
            <Value>=Parameters!RptMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RptYear">
            <Value>=Parameters!RptYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AsOfDate">
            <Value>=Parameters!AsOfDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CategoryType">
            <Value>=Parameters!CategoryType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LOB">
            <Value>=Parameters!LOB.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>/*

declare @RptYear			char(4);
declare @RptMonth			int;
declare	@LOB				varchar(2000);
declare @CategoryType		varchar(50);
declare @AsOfDate			date;

*/

declare @RptBeginDate		date;
declare @RptEndDate			date;



begin

/*

set @RptYear		= '2021'
set @RptMonth		= 8
set @CategoryType	= 'Beginning Inventory'
set @CategoryType	= 'Opened'
set @CategoryType	= 'Reopened'
set @CategoryType	= 'Closed'
set @CategoryType	= 'Closing Inventory'
set @LOB			= 'AP'
set @AsOfDate		= '7/31/2021'

*/

set @RptBeginDate	= convert(date, convert(varchar(2), @RptMonth) + '/01/' + @RptYear)
set @RptEndDate		= dateadd(d, -1, dateadd(m, 1,  convert(date, convert(varchar(2), @RptMonth) + '/01/' + @RptYear)))



select *
from (
select 1 CategoryOrder, @RptYear RptYear, 
       'Beginning Inventory' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month,
       aaaaa.*
from ( 
select c.claim_id,
       e.party_id,
       b.policy_num,
	   g.company_name,
       ltrim(case when e.first_name is NULL 
			then e.last_name
			else e.first_name + ' ' + e.last_name end) ClaimantName,
	   c.fh_claim_num,
	   c.loss_date,
	   c.reported_date,
       f.history_action_type,
	   f.history_datetime,
	   c.handler_id,
	   c.handler_id2,
	   d.first_name,
	   d.last_name, a.lob_id,	   
	   case when f.open_close_status = 'R' then IsNull(c.claim_status_id, 'ST0CNP') else c.claim_status_id end claim_status_id,
	   case when f.open_close_status = 'O' then 'Open'
			when f.open_close_status = 'C' then 'Closed'
			when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
	   c.closure_method_id,
	   f.closed_date
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
     [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] e,
	 [dbo].[fh_party_history] f,
     [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = c.claim_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and a.claim_id = e.claim_id
  and e.party_id = f.party_id
  and c.history_datetime = (select max(cc.history_datetime)	
							from [dbo].[fh_claim_history] cc
							where c.claim_id = cc.claim_id
							  and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
							  and cc.history_action_type = case when cc.open_close_status = 'O'
															    then 'I'
															    else cc.history_action_type end
							  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
  and f.history_action_type = case when f.open_close_status = 'O'
								   then 'I'
								   else f.history_action_type end
  and b.tier_company_id = g.company_id
--  and a.claim_id = '001020000224447'
--  and a.fh_claim_num = '209114'

) aaaaa
where aaaaa.history_datetime &lt; @RptBeginDate
  and aaaaa.history_datetime &lt; @AsOfDate
  and aaaaa.open_close_status_name in ('Open', 'Reopened')
  and not exists (select 1
				  from [dbo].[fh_party_history] ccccc
				  where aaaaa.claim_id = ccccc.claim_id
				    and aaaaa.party_id = ccccc.party_id
				    and ccccc.open_close_status = 'C'
					and ccccc.history_datetime &gt; aaaaa.history_datetime
					and ccccc.history_datetime &lt; @RptBeginDate
					and ccccc.history_datetime &lt; @AsOfDate
					and ccccc.closed_date &lt; @RptBeginDate
					and ccccc.closed_date &lt; @AsOfDate)
  and aaaaa.history_datetime = (select max(ccccc.history_datetime)
                                from [dbo].[fh_party_history] ccccc
							    where aaaaa.claim_id = ccccc.claim_id
								  and aaaaa.party_id = ccccc.party_id
								  and ccccc.history_action_type = case when ccccc.open_close_status = 'O'
																	   then 'I'
																	   else ccccc.history_action_type end
								  and ccccc.history_datetime &lt; @RptBeginDate
								  and ccccc.history_datetime &lt; @AsOfDate)

union all

select 2 CategoryOrder, @RptYear RptYear, 
       'Opened' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month,
       aaaaa.*
from (
select c.claim_id,
       e.party_id,
       b.policy_num,
	   g.company_name,
       ltrim(case when e.first_name is NULL 
			then e.last_name
			else e.first_name + ' ' + e.last_name end) ClaimantName,
	   c.fh_claim_num,
	   c.loss_date,
	   c.reported_date,
       f.history_action_type,
	   f.history_datetime,
	   c.handler_id,
	   c.handler_id2,
	   d.first_name,
	   d.last_name, a.lob_id,	 
	   c.claim_status_id,
	   case when f.open_close_status = 'O' then 'Open'
			when f.open_close_status = 'C' then 'Closed'
			when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,
       
	   c.closure_method_id,
	   f.closed_date
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
     [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] e,
	 [dbo].[fh_party_history] f,
     [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = c.claim_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and a.claim_id = e.claim_id
  and e.party_id = f.party_id
  and f.history_action_type = 'I'
  and c.history_datetime = (select max(cc.history_datetime)	
							from [dbo].[fh_claim_history] cc
							where c.claim_id = cc.claim_id
							  and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
							  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
  and b.tier_company_id = g.company_id
--  and c.claim_status_id is not NULL
  and IsNull(c.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
--  and a.claim_id = '000250000220591' 
--  and a.fh_claim_num = '209114'

) aaaaa
where aaaaa.history_datetime &gt;= @RptBeginDate
  and aaaaa.history_datetime &lt; dateadd(d, 1, @RptEndDate)
  and aaaaa.history_datetime &lt; dateadd(d, 1, @AsOfDate)
  and aaaaa.open_close_status_name in ('Open')

union all

select 3 CategoryOrder, @RptYear RptYear, 
       'Reopened' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month,
       aaaaa.*
from (
select c.claim_id,
       e.party_id,
       b.policy_num,
	   g.company_name,
       ltrim(case when e.first_name is NULL 
			then e.last_name
			else e.first_name + ' ' + e.last_name end) ClaimantName,
	   c.fh_claim_num,
	   c.loss_date,
	   c.reported_date,
       f.history_action_type,
	   f.history_datetime,
	   c.handler_id,
	   c.handler_id2,
	   d.first_name,
	   d.last_name, a.lob_id,	 
	   IsNull(c.claim_status_id, 'ST0CNP') claim_status_id,
	   case when f.open_close_status = 'O' then 'Open'
			when f.open_close_status = 'C' then 'Closed'
			when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
	   c.closure_method_id,
	   f.closed_date
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
     [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] e,
	 [dbo].[fh_party_history] f,
     [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = c.claim_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and a.claim_id = e.claim_id
  and e.party_id = f.party_id
  and f.history_action_type = case when f.open_close_status = 'O'
								   then 'I'
								   else f.history_action_type end
  and f.history_datetime = (select min(ff.history_datetime)
									from [dbo].[fh_party_history] ff
									where ff.claim_id = f.claim_id
									  and ff.party_id = f.party_id
									  and ff.open_close_status = f.open_close_status
									  and ff.closed_date = f.closed_date)
  and c.history_datetime = (select max(cc.history_datetime)	
							from [dbo].[fh_claim_history] cc
							where c.claim_id = cc.claim_id
							  and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
							  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
  and b.tier_company_id = g.company_id
--  and c.claim_status_id is not NULL
  and IsNull(c.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
--  MH  1/20/2022  - Don't count reopens right after another reopen
		  and not exists (select 1
					  from [dbo].[fh_party_history] ff
					  where ff.claim_id = f.claim_id
					    and ff.party_id = f.party_id
					    and ff.history_datetime = (select max(fff.history_datetime)
													from [dbo].[fh_party_history] fff
													where fff.claim_id = ff.claim_id
													and fff.party_id = ff.party_id
													and fff.history_datetime &lt; f.history_datetime)
					    and ff.open_close_status = 'R')
--  and a.claim_id = '001020000199427' 
--  and a.fh_claim_num = '209114'

) aaaaa
where aaaaa.history_datetime &gt;= @RptBeginDate 
  and aaaaa.history_datetime &lt; dateadd(d, 1, @RptEndDate)
  and aaaaa.history_datetime &lt; dateadd(d, 1, @AsOfDate)
  and aaaaa.open_close_status_name in ('Reopened')

union all

select 4 CategoryOrder, @RptYear RptYear, 
       'Closed' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month,
       aaaaa.*
from (
select c.claim_id,
       e.party_id,
       b.policy_num,
	   g.company_name,
       ltrim(case when e.first_name is NULL 
			then e.last_name
			else e.first_name + ' ' + e.last_name end) ClaimantName,
	   c.fh_claim_num,
	   c.loss_date,
	   c.reported_date,
       f.history_action_type,
	   f.history_datetime,
	   c.handler_id,
	   c.handler_id2,
	   d.first_name,
	   d.last_name, a.lob_id,	 
	   case when IsNull((select sum(rp.loss_amt * -1)
						  from [dbo].[fh_reserve_paid] rp,
							   [dbo].[fh_reserve_type] h,
							   [dbo].[fh_code_general] i
						  where rp.claim_id = e.claim_id
							and rp.party_id = e.party_id
							and rp.reserve_date &lt;= f.closed_date
							and rp.reserve_or_paid_code = 'P'
							and rp.reserve_type_id = h.reserve_type_id
							and i.item_id = h.reserve_group_id
							and i.item_name = 'Indemnity'), 0) &gt;= 0
            then 'CWOP'
			else 'CWP' end claim_status_id,
--	   IsNull(c.claim_status_id, 'ST0CNP') claim_status_id,
	   case when f.open_close_status = 'O' then 'Open'
			when f.open_close_status = 'C' then 'Closed'
			when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
	   c.closure_method_id,
	   f.closed_date
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
     [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] e,
	 [dbo].[fh_party_history] f,
     [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = c.claim_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and a.claim_id = e.claim_id
  and e.party_id = f.party_id
  and f.history_action_type = case when f.open_close_status = 'O'
								   then 'I'
								   else f.history_action_type end
 and c.history_datetime = (select max(cc.history_datetime)	
							from [dbo].[fh_claim_history] cc
							where c.claim_id = cc.claim_id
							  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
  and b.tier_company_id = g.company_id
--  and IsNull(c.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
  and f.history_datetime = IsNull((select max(ff.history_datetime)
									from [dbo].[fh_party_history] ff
									where ff.claim_id = f.claim_id
									  and ff.party_id = f.party_id
									  and ff.open_close_status = 'C'
									  and ff.open_close_status = f.open_close_status
									  and ff.closed_date = f.closed_date
									  and ff.history_datetime &gt;= @RptBeginDate 
									  and ff.history_datetime &lt; dateadd(d, 1, @RptEndDate)), f.history_datetime)
--  MH  1/20/2022  - Don't count closes right after another close
		  and not exists (select 1
					  from [dbo].[fh_party_history] ff
					  where ff.claim_id = f.claim_id
					    and ff.party_id = f.party_id
					    and ff.history_datetime = (select max(fff.history_datetime)
													from [dbo].[fh_party_history] fff
													where fff.claim_id = ff.claim_id
													and fff.party_id = ff.party_id
													and fff.history_datetime &lt; f.history_datetime)
					    and ff.open_close_status = 'C')
--  and e.party_id = '001020000186411' 
--  and a.fh_claim_num = '209613'

) aaaaa
where aaaaa.history_datetime &gt;= @RptBeginDate 
  and aaaaa.history_datetime &lt; dateadd(d, 1, @RptEndDate)
  and aaaaa.history_datetime &lt; dateadd(d, 1, @AsOfDate)
  and aaaaa.closed_date &gt;= @RptBeginDate 
  and aaaaa.closed_date &lt; dateadd(d, 1, @RptEndDate)
  and aaaaa.closed_date &lt; dateadd(d, 1, @AsOfDate)
  and aaaaa.open_close_status_name in ('Closed')

union all

select 5 CategoryOrder, @RptYear RptYear, 
       'Closing Inventory' CategoryType,
	   datepart(mm, @RptBeginDate) MonthNum,
	   datename(Month, @RptBeginDate) Month,
       aaaaa.*
from (
select c.claim_id,
       e.party_id,
       b.policy_num,
	   g.company_name,
       ltrim(case when e.first_name is NULL 
			then e.last_name
			else e.first_name + ' ' + e.last_name end) ClaimantName,
	   c.fh_claim_num,
	   c.loss_date,
	   c.reported_date,
       f.history_action_type,
	   f.history_datetime,
	   c.handler_id,
	   c.handler_id2,
	   d.first_name,
	   d.last_name, a.lob_id,   
	   case when f.open_close_status = 'R' then IsNull(c.claim_status_id, 'ST0CNP') else c.claim_status_id end claim_status_id,
	   case when f.open_close_status = 'O' then 'Open'
			when f.open_close_status = 'C' then 'Closed'
			when f.open_close_status = 'R' then 'Reopened' end open_close_status_name,       
	   c.closure_method_id,
	   f.closed_date
from [dbo].[fh_claim] a,
     [dbo].[fh_policy] b,
     [dbo].[fh_claim_history] c,
	 [dbo].[fh_user] d,
	 [dbo].[fh_party] e,
	 [dbo].[fh_party_history] f,
     [dbo].[fh_company] g
where a.policy_id = b.policy_id
  and b.tier1_company_id = 'C13339'
  and a.claim_id = c.claim_id
  and IsNull(c.handler_id, a.handler_id) = d.user_id
  and c.history_action_type = case when c.open_close_status = 'O'
								   then 'I'
								   else c.history_action_type end
  and a.claim_id = e.claim_id
  and e.party_id = f.party_id
  and f.history_action_type = case when f.open_close_status = 'O'
								   then 'I'
								   else f.history_action_type end
  and c.history_datetime = (select max(cc.history_datetime)	
							from [dbo].[fh_claim_history] cc
							where c.claim_id = cc.claim_id
							  and IsNull(cc.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
							  and cc.history_action_type = case when cc.open_close_status = 'O'
															    then 'I'
															    else cc.history_action_type end
							  and cc.history_datetime &lt;= dateadd(ss, 3, f.history_datetime))
  and b.tier_company_id = g.company_id
--  and c.claim_status_id is not NULL
  and IsNull(c.claim_status_id, 'XXXXX') not in ('ST000O', 'ST00RO')
--  and e.party_id = '000250000207220' 
--  and a.fh_claim_num = '209114'

) aaaaa
where aaaaa.history_datetime &lt; dateadd(d, 1, @RptEndDate)
  and aaaaa.history_datetime &lt; dateadd(d, 1, @AsOfDate)
  and aaaaa.open_close_status_name in ('Open', 'Reopened')
  and not exists (select 1
				  from [dbo].[fh_party_history] ccccc
				  where aaaaa.claim_id = ccccc.claim_id
				    and aaaaa.party_id = ccccc.party_id
				    and ccccc.open_close_status = 'C'
					and ccccc.history_datetime &gt; aaaaa.history_datetime
					and ccccc.history_datetime &lt; dateadd(d, 1, @RptEndDate)
					and ccccc.history_datetime &lt; dateadd(d, 1, @AsOfDate)
					and ccccc.closed_date &lt; dateadd(d, 1, @RptEndDate)
					and ccccc.closed_date &lt; dateadd(d, 1, @AsOfDate))
  and aaaaa.history_datetime = (select max(ccccc.history_datetime)
                                from [dbo].[fh_party_history] ccccc
							    where aaaaa.claim_id = ccccc.claim_id
								  and aaaaa.party_id = ccccc.party_id
								  and ccccc.history_action_type = case when ccccc.open_close_status = 'O'
																	   then 'I'
																	   else ccccc.history_action_type end
								  and ccccc.history_datetime &lt; dateadd(d, 1, @RptEndDate)
								  and ccccc.history_datetime &lt; dateadd(d, 1, @AsOfDate))

) aaaaaa 
where aaaaaa.CategoryType = @CategoryType
  and aaaaaa.lob_id in (@LOB)
order by company_name,
		 policy_num,
		 ClaimantName


end</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CategoryOrder">
          <DataField>CategoryOrder</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RptYear">
          <DataField>RptYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategoryType">
          <DataField>CategoryType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthNum">
          <DataField>MonthNum</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="claim_id">
          <DataField>claim_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="party_id">
          <DataField>party_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="history_action_type">
          <DataField>history_action_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="policy_num">
          <DataField>policy_num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fh_claim_num">
          <DataField>fh_claim_num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="loss_date">
          <DataField>loss_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="company_name">
          <DataField>company_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="history_datetime">
          <DataField>history_datetime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ClaimantName">
          <DataField>ClaimantName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="reported_date">
          <DataField>reported_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="handler_id">
          <DataField>handler_id</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="handler_id2">
          <DataField>handler_id2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
