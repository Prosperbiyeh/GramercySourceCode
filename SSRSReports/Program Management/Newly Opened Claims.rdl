    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>SharedLocalDS</DataSourceName>
        <CommandText>select b.lob_id,
       b.lob_name
from [dbo].[fh_claim] a,
     [dbo].[fh_lob] b,
     [dbo].[fh_policy] c
where a.lob_id = b.lob_id
  and a.policy_id = c.policy_id
  and c.tier1_company_id = 'C13339'
group by b.lob_id,
         b.lob_name
order by 2</CommandText>
      </Query>
      <Fields>
        <Field Name="lob_id">
