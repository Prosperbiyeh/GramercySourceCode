          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lob_name">
          <DataField>lob_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
